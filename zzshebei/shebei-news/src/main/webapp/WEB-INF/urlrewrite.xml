<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!--资讯中心-->
	<rule>
       <from>^/baojia/list(\d*)--t(\d+).htm$</from>
       <to>/baojia/list.htm?category=$1&amp;type=$2</to>
    </rule>
    
    <rule>
       <from>^/zixun/list(\d*)--t(\d+).htm$</from>
       <to>/zixun/list.htm?category=$1&amp;type=$2</to>
    </rule>
    
   	<rule>
		<from>^/baojia/list(\d*)--t(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/baojia/list.htm?category=$1&amp;type=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	
		<rule>
		<from>^/zixun/list(\d*)--t(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/zixun/list.htm?category=$1&amp;type=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	
    <rule>
       <from>^/baojia/bjdetails(\d+).htm$</from>
       <to>/baojia/bjdetails.htm?id=$1</to>
    </rule>
    
    <rule>
       <from>^/zixun/zxdetails(\d+).htm$</from>
       <to>/zixun/zxdetails.htm?id=$1</to>
    </rule>
</urlrewrite>