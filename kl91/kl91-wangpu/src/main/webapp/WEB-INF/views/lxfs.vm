#parse("/common/topBar.vm")
#set($pageName="lxfs")
#parse("/common/menu.vm")

<div class="wrapper site-main clearfix">
	
	## left vm
	#parse("/common/left.vm")
	##

	<div class="site-right">
		<div class="wrap mt20 ct">
			<div class="wt">
				<div class="t">联系方式</div>
			</div>
			<div class="wc">
				<p>公司名：$!{company.companyName}</p>
				<p>联系人：$!{company.contact} #if($!{company.sex}==0)&nbsp;&nbsp;先生#elseif($!{company.sex}==1)&nbsp;&nbsp;女士#end</p>
				<p>
					手机：
					#if(${sessionUser})
						<span>$!{company.mobile}</span>
					#else
						<span>登录可见</span>
					#end
				</p>
				<p>
					邮箱：
					#if(${sessionUser})
						<span>$!{company.email}</span>
					#else
						<span>登录可见</span>
					#end
				</p>
				<p>
					QQ：
					#if(${sessionUser})
						<span>$!{company.qq}</span>
					#else
						<span>登录可见</span>
					#end
				</p>
				<p>公司地址：$!{company.address}</p>
				<p>公司主页：$!{company.website}</p>
			</div>
		</div>
	</div>

</div>