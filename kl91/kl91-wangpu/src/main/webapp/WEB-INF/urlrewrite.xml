<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN" "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/lxfs.htm$</from>
		<to>/lxfs.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/dangan.htm$</from>
		<to>/dangan.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/index.htm$</from>
		<to>/index.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/jieshao.htm$</from>
		<to>/jieshao.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/products.htm$</from>
		<to>/products.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/productsDetail(\d+).htm$</from>
		<to>/productsDetail.htm?account=$1&amp;id=$2</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/liuyan.htm$</from>
		<to>/liuyan.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/liuyan--pid(\d+).htm$</from>
		<to>/liuyan.htm?account=$1&amp;pid=$2</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/doLiuyan.htm$</from>
		<to>/doLiuyan.htm?account=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%]*)/success.htm$</from>
		<to>/success.htm?account=$1</to>
	</rule>
	
</urlrewrite>