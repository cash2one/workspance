<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
        <from>^/index(\d+).htm$</from>
        <to>/index.htm?cid=$1</to>    	
     </rule>
     <rule>
        <from>^/detail(\d+).htm$</from>
        <to>/detail.htm?id=$1</to>    	
     </rule>
     <rule>
        <from>^/products(\d+).htm$</from>
        <to>/products.htm?cid=$1</to>    	
     </rule>
     <rule>
        <from>^/products(\d+)--(\d*).htm$</from>
        <to>/products.htm?cid=$1&amp;group=$2</to>
     </rule>
     <rule>
		<from>^/products(\d+)--(\d*)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/products.htm?cid=$1&amp;group=$2&amp;keywords=$3&amp;start=$4&amp;limit=$5</to>
	</rule>
	 <rule>
		<from>^/products--(\d*)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/products.htm?group=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
        <from>^/credit(\d+).htm$</from>
        <to>/credit.htm?cid=$1</to>    	
     </rule>
   <rule>
        <from>^/companyContact(\d+).htm$</from>
        <to>/companyContact.htm?cid=$1</to>    	
   </rule>
    <rule>
        <from>^/companyInfo(\d+).htm$</from>
        <to>/companyInfo.htm?cid=$1</to>    	
   </rule>
   <rule>
        <from>^/message(\d+).htm$</from>
        <to>/message.htm?cid=$1</to>    	
   </rule>
   <rule>
        <from>^/companyNews(\d+).htm$</from>
        <to>/companyNews.htm?cid=$1</to>    	
   </rule>
   <rule>
        <from>^/newsDetails(\d+).htm$</from>
        <to>/newsDetails.htm?id=$1</to>    	
   </rule>
   <rule>
		<from>^/companyNews(\d+)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/companyNews.htm?cid=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/companyNews--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/companyNews.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
   <rule>
        <from>^/technicalArticles(\d+).htm$</from>
        <to>/technicalArticles.htm?cid=$1</to>    	
   </rule>
   <rule>
		<from>^/technicalArticles(\d+)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/technicalArticles.htm?cid=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/technicalArticles--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/technicalArticles.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
   <rule>
        <from>^/successStories(\d+).htm$</from>
        <to>/successStories.htm?cid=$1</to>    	
   </rule>
   <rule>
		<from>^/successStories(\d+)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/successStories.htm?cid=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/successStories--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/successStories.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
</urlrewrite>