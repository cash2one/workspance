<div class="mk-sidebar">
	#cacheFragment("$!{address.fragment}/fragment/esite/contact.htm?cid=$!{cid}&ertdfa","","","600")
	<div class="panel">
		<h3>
			<table>
				<tr>
					<td>公司名片</td>
				</tr>
			</table>
		</h3>
		<div class="panel-body ptmsb_h4">
			<h4>$!{compProfileDto.compProfile.name}</h4>
			<div class="pstmsb_p">
				<p>
					<span>联系人：$!{compProfileDto.contacts}</span>
				</p>
				<p>
					<span>
						手 机：
						#if($!{epAuthUser}) 
							$!{compProfileDto.mobile} 
						#else
						<a href="javascript:void(0);" class="loginView">登陆</a>
						免费查看手机号码! 
						#end
					</span>
				</p>
				<p>座  机：$!{compProfileDto.phoneArea} $!{compProfileDto.phone}</p>
				<p>
					地  址：#printTitle($!{compProfileDto.compProfile.address}  "")
				</p>
				<p><font color="red">联系时请说明来自中国环保网 www.huanbao.com</font></p>
			</div>
			<div class="button-a">
				##<a href="#">关注该企业</a>
				<a href="$!{address.esite}/message$!{cid}.htm">给他留言</a>
				<div class="clear"></div>
			</div>
		</div>
	</div>
	<script type="text/javascript" >
		jQuery(document).ready(function(){
		
			jQuery(".loginView").click(function(){
				if(jQuery("#loginstatus").val()!="1" ){
        			var loginbox=new Asto.util.PopupBox({
            			url:Asto.MINI_LOGIN_URL,
            	  		destUrl:Asto.DEST_URL,
            	  		callbackFn:function(success, data){
            	  			loginbox.close();
							Asto.util.Message.show({
                				msg:"登录成功,正在重新载入页面,请稍候...",
                				autoClose:10000
                			});
							window.location.reload();
            	 		},
						modal:true,
        				title:"请先登录后再查看联系方式",
            	  		width:510,
            	  		height:400
              		});
            	  	loginbox.show();
        		}
			});
			
		});
	</script>
	#end

	#cacheFragment("$!{address.fragment}/fragment/esite/tradeGroup.htm?cid=$!{cid}&ddd2","","","600")
	<div class="panel mt8px" #if($!{list.size()}==0) style="display : none" #end>
		<h3>
			<table>
				<tr>
					<td class="list-on">产品分类</td>
				</tr>
			</table>
		</h3>
		<div class="panel-body">
			<ul class="sb-list-ul">
				#foreach($category in $!{list})
				<li>
					<h4>$!{category.name}</h4>
					<ul>
						#foreach($child in $!{category.childs})
						<li>
							<a href="$!{address.esite}/products$!{cid}--$!{child.id}.htm" target="_blank">$!{child.name}</a>
						</li>
						#end
					</ul>
				</li>
				#end
			</ul>
		</div>
	</div>
	#end

	#cacheFragment("$!{address.fragment}/fragment/trade/newestSupply.htm?cid=$!{cid}&uid=$!{uid}&size=8","","","600")
		<div class="panel mt8px">
			<h3>
				<table>
					<tr>
						<td>最新产品信息</td>
					</tr>
				</table>
			</h3>
			<div class="panel-body">
				<ul class="ul-28px">
					#foreach($supply in $!{list})
					<li>
						<span style="color:#F27800;">[供应]</span>
						<span>
							<a href="#springUrl('/detail')$!{supply.id}.htm">#printTitle("$!{supply.title}" 15 "")</a>
						</span>
					</li>
					#end
				</ul>
			</div>
		</div>
	#end
	
	#if($broCode)
	<div class="panel mt8px">
		<h3>
			<table>
				<tr>
					<td>相关类别</td>
				</tr>
			</table>
		</h3>
		<div class="panel-body">
			<ul class="ul-28px">
				#foreach($tradeCategory in $!{broCode})
				<li>
					<span>
						<a href="$!{address.trade2}/list-$!{tradeCategory.id}.htm" target="_blank">#printTitle("$!{tradeCategory.name}" 15 "")</a>
					</span>
				</li>
				#end
			</ul>
		</div>
	</div>
	#end
</div>
#end