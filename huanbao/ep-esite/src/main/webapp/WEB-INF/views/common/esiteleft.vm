#if("$!{profile.memberCode}" == "10011001")
<div class="mk-sidebar">
	<div class="sb-search">
		<form action="#springUrl('/products.htm')" method="post">
			<input name="cid" type="hidden" value="$!{cid}"/>
			<input name="keywords" type="text" value="$!{keywords}"/>
			<button type="submit">搜 索</button>
		</form>
	</div>
	#cacheFragment("$!{address.fragment}/fragment/esite/tradeGroup.htm?cid=$!{cid}&ddd1","","","")
	<div class="panel mt8px">
		<h3>
			<table>
				<tr>
					<td class="list-on">产品分类</td>
				</tr>
			</table>
		</h3>
		<div class="panel-body">
			<ul class="sb-list-ul">
				#foreach($category in $!{list})
				<li>
					<h4>$!{category.name}</h4>
					<ul>
						#foreach($child in $!{category.childs})
						<li>
							<a href="$!{address.esite}/products$!{cid}--$!{child.id}.htm" target="_blank">$!{child.name}</a>
						</li>
						#end
					</ul>
				</li>
				#end
			</ul>
		</div>
	</div>
	#end
	#cacheFragment("$!{address.fragment}/fragment/esite/contact.htm?cid=$!{cid}&vip","","","")
	<div class="panel mt8px">
		<h3>
			<table>
				<tr>
					<td>联系方式</td>
				</tr>
			</table>
		</h3>
		<div class="panel-body">
			<p>联系人：$!{compProfileDto.contacts}</p>
			<p>手  机：$!{compProfileDto.mobile}</p>
			<p>电  话：$!{compProfileDto.phoneArea} $!{compProfileDto.phone}</p>
			<p>传  真：$!{compProfileDto.faxArea} $!{compProfileDto.fax}</p>
			<p>
				地  址：
					#if(${compProfileDto.compProfile.address})
						#printTitle($!{compProfileDto.compProfile.address} "")
					#end
			</p>
			<p>
				网  址：
				<a href="$!{compProfileDto.compProfile.domain}" target="_blank" ref="nofollow" title="$!{compProfileDto.compProfile.domain}" >#printTitle($!{compProfileDto.compProfile.domain} 28 "")</a>
			</p>
		</div>
	</div>
	#end
	
