						#if($!compProfile.compProfile.mainProductSupply!="")
    						</br>
    						该公司主营产品：${compProfile.compProfile.mainProductSupply}
						#end
					#end
				</div>
				<div id="ltbc_2" class="panelundis">
					<p>
					<table border="1" class="bordergrey">
						#foreach($item in $!{tradeProperty})
                          <tr>
                             <td class="compy_r_otherprods_l backgroundgrey txtcenter">$!{item.name}</td>
                             <td>$!propertyValueMap.get($!{item.id})</td>
                          </tr>
						#end
						<tr>
                             <td class="compy_r_otherprods_l backgroundgrey txtcenter">产品说明:</td>
                             <td>$!{tradeSupply.supply.useTo}</td>
                        </tr>
					 </table>
					</p>
				</div>
                <div style="clear:both"></div>
				</div>
                <div style="clear:both"></div>
			</div>
