<script language="JavaScript">

 if($!{show}==1){
   Asto.util.Message.show({
		msg:"询价成功！",
		autoClose:5000
	});
 }


jQuery(document).ready(function(){

	jQuery("#messageForm").submit(function(){
	if (jQuery("#title").val()=="") {
    		Asto.util.Message.show({
				msg:"请输入标题！",
				autoClose:10000
			});
			jQuery("#title").focus();
			return false;
    	}
    	if (jQuery("#details").val()==""){
    	    Asto.util.Message.show({
				msg:"请输入内容！",
				autoClose:10000
			});
			jQuery("#details").focus();
			return false;
    	}
	
		//提交
		if(jQuery("#loginstatus").val()!="1" ){
			
			var loginbox=new Asto.util.PopupBox({
    			url:Asto.MINI_LOGIN_URL,
    	  		destUrl:Asto.DEST_URL,
    	  		callbackFn:function(success, data){
    	  			loginbox.close();
    	  			jQuery("#loginstatus").val("1");
    				jQuery("#messageForm").submit();
    	 		},
				modal:true,
				title:"请先登录后再留言",
    	  		width:510,
    	  		height:400
      		});
			
    	  	loginbox.show();
			return false;
		}

	});
	
});
</script>