<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!--展会 -->
	<rule>
		<from>^/list(\d+).htm$</from>
		<to>/list.htm?categoryCode=$1</to>
	</rule>
	
	<rule>
		<from>^/list(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/list.htm?categoryCode=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	
	<rule>
		<from>^/details(\d+).htm$</from>
		<to>/details.htm?id=$1</to>	
	</rule>
	
	<rule>
		<from>^/search--([a-z,A-Z,0-9,%,+]+).htm$</from>
		<to>/search.htm?name=$1</to>	
	</rule>
	<rule>
		<from>^/search--([a-z,A-Z,0-9,%,+]*)--(\d*)--([a-z,A-Z,0-9,%]*)--(\d*)--(\d*)--([0-9,-]*)--([0-9,-]*)--s(\d*)--l(\d*).htm$</from>
		<to>/search.htm?name=$1&amp;industryCode=$2&amp;organizers=$3&amp;provinceCode=$4&amp;areaCode=$5&amp;startTime=$6&amp;endTime=$7&amp;start=$8&amp;limit=$9</to>	
	</rule>
</urlrewrite>