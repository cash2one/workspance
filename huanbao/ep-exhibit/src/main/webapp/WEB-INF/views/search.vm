<!--main leader start-->
<section class="hb_positionleader">
     <span class="hb_leader_tit">
			<a href="$!{address.www}">首页</a> 
			&gt;
			<a href="$!{address.exhibit}">展会中心</a>
			&gt;
			搜索“ <font class="fontred fontbold">$!{exhibitCondition.name}</font> ”
	 </span>
     <span class="hb_leader_tip">本周新增客户：<font class="fontorange">$!{weekAddAccount}</font>人</span>
     <div class="clear"></div>
</section>
<!-- main header end-->
<section class="hb_mainblock">
        <div class="etlists_section2_l borderlightgreen html5radius">
            <article class="et_section3_l_mid">
                  <header class="etlists_section3_l_header1">
                        <div>
                           <h3 class="et_bg fontlightgreen paddingleft14 html5radius">信息列表</h3>
                        </div>
                        <div class="clear"></div>
                  </header>
                  <section class="et_list">
                         <div class="et_list_tit backgroundgreen fontlightgreen fontbold">
                                    <span class="ls_wid112">所处行业</span>
                                    <span class="ls_wid200">展会名称</span>
                                    <span class="ls_wid112">展会地区</span>
                                    <span class="ls_wid112">开始时间</span>
                                    <span class="ls_wid112">结束时间</span>
                                    <div class="clear"></div>
                         </div>
                          
						
						<ul class="et_list_ls">	 
							 #foreach($list in ${page.records})
                             <li>
                                     <span class="ls_wid112"><a href="" >$!{list.industryName}</a></span>
                                     <span class="ls_wid200"><a href="#springUrl('/details')$!{list.exhibit.id}.htm" >#printTitle($!{list.exhibit.name} 14 "")</a></span>
                                     <span class="ls_wid112">$!{list.area.provice} $!{list.area.city}</span>
                                     <span class="ls_wid112">#printFragmentDate('yyyy-MM-dd',$!{list.exhibit.gmtStart})</span>
                                     <span class="ls_wid112">#printFragmentDate('yyyy-MM-dd',$!{list.exhibit.gmtEnd})</span>
                             </li>
                             #end                      
                             <div class="clear"></div>
                         </ul>
						 
						 #staticPageNav(${page} "#springUrl('/search')--$!{encodeKeywords}--$!{exhibitCondition.industryCode}--$!{exhibitCondition.organizers}--$!{exhibitCondition.provinceCode}--$!{exhibitCondition.areaCode}--#printFragmentDate('yyyy-MM-dd',$!{exhibitCondition.gmtStart})--#printFragmentDate('yyyy-MM-dd',$!{exhibitCondition.gmtEnd})" "" "")
						
                  </section>
           </article>
            <div class="clear"></div>
        </div>
        <div class="etlists_section2_r et_wd266 floatright">
           <!--展会搜索-->
           #parse("/common/searchExhibit.vm")
		   <!--展会资讯-->
		   #parse("/common/newsExhibit.vm")
           <!--服务框-->
		   #parse("/common/toolsBox.vm") 
           <!--展会指南--> 
           #parse("/common/leaderExhibit.vm") 
        </div>
        <div class="clear"></div>
</section>
