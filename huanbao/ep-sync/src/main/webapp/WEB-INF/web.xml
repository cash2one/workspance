<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
        http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    	<servlet-name>seedServlet</servlet-name>
    	<servlet-class>com.zz91.ep.sync.servlet.SeedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>seedServlet</servlet-name>
        <url-pattern>/seed</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>imptTradeSupplyServlet</servlet-name>
    	<servlet-class>com.zz91.ep.sync.servlet.ImptTradeSupplyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>imptTradeSupplyServlet</servlet-name>
        <url-pattern>/imptTradeSupply</url-pattern>
    </servlet-mapping>
    
	 <servlet>
    	<servlet-name>imptCompAccountServlet</servlet-name>
    	<servlet-class>com.zz91.ep.sync.servlet.ImptCompAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>imptCompAccountServlet</servlet-name>
        <url-pattern>/imptCompAccount</url-pattern>
    </servlet-mapping> 
 	
 	
 	 <servlet>
    	<servlet-name>imptTradeBuyServlet</servlet-name>
    	<servlet-class>com.zz91.ep.sync.servlet.ImptTradeBuyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>imptTradeBuyServlet</servlet-name>
        <url-pattern>/imptTradeBuy</url-pattern>
    </servlet-mapping>
 	
 	 <servlet>
    	<servlet-name>imptCompProfileServlet</servlet-name>
    	<servlet-class>com.zz91.ep.sync.servlet.ImptCompProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>imptCompProfileServlet</servlet-name>
        <url-pattern>/imptCompProfile</url-pattern>
    </servlet-mapping> 
 	
	<servlet>
		<servlet-name>initSystem</servlet-name>
		<servlet-class>com.zz91.ep.sync.servlet.InitServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
</web-app>
