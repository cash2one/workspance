<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
       <from>^/supply$</from>
       <to>/supply/index.htm</to>
    </rule>
    <rule>
       <from>^/supply/index--(\d*).htm$</from>
       <to>/supply/index.htm?category=$1</to>
    </rule>
    <rule>
       <from>^/supply/index--(\d*)--s(\d*)--l(\d*).htm$</from>
       <to>/supply/index.htm?category=$1&amp;start=$2&amp;limit=$3</to>
    </rule>
    
    <rule>
       <from>^/supply/detail(\d+).htm$</from>
       <to>/supply/detail.htm?id=$1</to>
    </rule>    
    
    <rule>
       <from>^/buy/list--([0-9,a-z,A-Z,%,+]*).htm$</from>
       <to>/buy/list.htm?keywords=$1</to>
    </rule>
    <rule>
        <from>^/buy/list--([0-9,a-z,A-Z,%,+]*)--(\d*)--(\d*)--(\d*)--([a-z]*)--(\d*)--([a-z]*)--s(\d*)--l(\d*).htm$</from>
       <to>/buy/list.htm?keywords=$1&amp;group=$2&amp;region=$3&amp;time=$4&amp;showStyle=$5&amp;sort=$6&amp;dir=$7&amp;start=$8&amp;limit=$9</to>
    </rule>
    <rule>
    	<from>^/buy/detail(\d+).htm$</from>
    	<to>/buy/detail.htm?id=$1</to>
    </rule>
    <!-- 标签子页 -->
    <rule>
    	<from>^/bq(\d+).htm$</from>
    	<to>/bq.htm?id=$1</to>
    </rule>
    <!-- 标签子页 分页 -->
    <rule>
    	<from>^/bq(\d+)-p(\d+).htm$</from>
    	<to>/bq.htm?id=$1&amp;pageNum=$2</to>
   	</rule>
    <rule>
    	<from>^/zdbuy$</from>
    	<to>/zdbuy/list.htm</to>
    </rule>
    <rule>
    	<from>^/zdbuy/list(\d+).htm$</from>
    	<to>/zdbuy/list.htm?categoryCode=$1</to>
    </rule>
    <rule>
        <from>^/zdbuy/list(\d*)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
       <to>/zdbuy/list.htm?categoryCode=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
    </rule>
    <rule>
    	<from>^/zdbuy/detail(\d+).htm$</from>
    	<to>/zdbuy/detail.htm?id=$1</to>
    </rule>
    <!-- 子网列表页 -->
    <rule>
    	<from>^/cp-([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/cp.htm?code=$1</to>
    </rule>
    <rule>
       <from>^/cp-([0-9,a-z,A-Z,%,+]*)-(\d*)-(\d*)-(\d*)-(\d*)-(\d*)-p(\d*).htm$</from>
       <to>/cp.htm?code=$1&amp;region=$2&amp;time=$3&amp;priceFrom=$4&amp;priceTo=$5&amp;sort=$6&amp;pageNum=$7</to>
    </rule>
    <!-- 最新产品 -->
 	<rule>
    	<from>^/newg-c([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/newg.htm?category=$1</to>
    </rule>
    <rule>
    	<from>^/newg-p(\d*).htm$</from>
    	<to>/newg.htm?pageNum=$1</to>
    </rule>
    <rule>
    	<from>^/newg-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/newg.htm?category=$1&amp;pageNum=$2</to>
    </rule>
 	<rule>
    	<from>^/newb-c([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/newb.htm?category=$1</to>
    </rule>
    <rule>
    	<from>^/newb-p(\d*).htm$</from>
    	<to>/newb.htm?pageNum=$1</to>
    </rule>
    <rule>
    	<from>^/newb-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/newb.htm?category=$1&amp;pageNum=$2</to>
    </rule>
    <!-- 最新子网 -->
    <rule>
    	<from>^/zw-c([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/zw.htm?parentId=$1</to>
    </rule>
    <rule>
    	<from>^/zw-p(\d*).htm$</from>
    	<to>/zw.htm?pageNum=$1</to>
    </rule>
    <rule>
    	<from>^/zw-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/zw.htm?parentId=$1&amp;pageNum=$2</to>
    </rule>
    <!-- 最新标签 -->
    <rule>
    	<from>^/newbq-c([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/newbq.htm?code=$1</to>
    </rule>
    <rule>
    	<from>^/newbq-p(\d*).htm$</from>
    	<to>/newbq.htm?pageNum=$1</to>
    </rule>
    <rule>
    	<from>^/newbq-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/newbq.htm?code=$1&amp;pageNum=$2</to>
    </rule>
    
    
    <!-- 供应类别 -->
    <rule>
       <from>^/category--([0-9,a-z,A-Z,%,+]*).htm$</from>
       <to>/category.htm?category=$1</to>
    </rule>
    <!-- 供应列表 -->
    <rule>
       <from>^/list-(\d*).htm$</from>
       <to>/list.htm?id=$1</to>
    </rule>
    <rule>
       <from>^/list-(\d*)--s(\d*)--l(\d*).htm$</from>
       <to>/list.htm?id=$1&amp;start=$2&amp;limit=$3</to>
    </rule>
    
    <rule>
    	<from>^/(\w+)/?$</from>
    	<to>/category.htm?cn=$1</to>
    </rule>
</urlrewrite>
