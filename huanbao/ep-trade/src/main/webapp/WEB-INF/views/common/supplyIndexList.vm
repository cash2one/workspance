<div class="clear"></div>
<div class="tdl_ls">
    <ul>
        <!-- 标王列表开始-->
        #foreach($l in ${bwList})
            <li>
                <article class="ts_ls_box_top">
                    <section class="ls_selector">
                        <input type="checkbox" value="$!{l.targetId}" name="supplyIds" />
                        <div class="ls_selector_tipbox">
                            <p class="lss_tipbox_t">您可以:</p>
                            <p class="lss_tipbox_b">
                                <a class="lss_tipbox_p" href="javascript:batchInquiry()">批量询价</a>
                                <span class="lss_tipbox_o">或</span>
                                <a class="lss_tipbox_c" href="javascript:infoContrast()">对比产品</a>
                            </p>
                        </div>
                    </section>
                    <section class="ls_ifo">
                        <header class="ls_ifo_img">
                            #if("$!{l.photoCover}"=="")
                                <a href="#springUrl('/supply/detail')$!{l.targetId}.htm" target="_blank" title="$!{l.title}">
                                    <img src="$!{address.img}/huanbao/images/trade/tradelist/noprod.gif" alt="$!{l.title}"/>
                                </a>
                                #else
                                    <a href="#springUrl('/supply/detail')$!{l.targetId}.htm" target="_blank" title="$!{l.title}">
                                        <img src="$!{address.thumb}$!{address.resource}$!{l.photoCover}&width=100&height=100" alt="$!{l.title}"/>
                                    </a>
                            #end
                        </header>
                        <section class="ls_ifo_details">
                            <h3>
                                <a href="#springUrl('/supply/detail')$!{l.targetId}.htm" target="_blank" title="$!{l.title}">
                                    <span class="fontorange comm_em_red p_ellipsis">#printTitle($!{l.title} 20 '')</span>
                                </a>
                            </h3>
                            <p class="ls_ifo_content comm_em_red p_ellipsis">#printTitle($!{l.detailsQuery} 50 '...')</p>
                            <p class="ls_ifo_com">
                                <a href="$!{address.esite}/index$!{l.cid}.htm" target="_blank">$!{l.cname}</a>
                                #if($velocityCount==1)
                                    <img src="$!{address.img}/huanbao/images/trade/bw/jpbw.jpg" />
                                #elseif($velocityCount > 1 && 4 > $velocityCount )
                                    <img src="$!{address.img}/huanbao/images/trade/bw/ypbw.jpg" />
                                #elseif($velocityCount >= 4)
                                    <img src="$!{address.img}/huanbao/images/trade/bw/tpbw.jpg" />
                                #end
                            </p>
##                            #if("$!{list.mainProductBuy}" !="")
##                                <div class="clear"></div>
##                                <p class="ls_ifo_com">主营产品:#printTitle($!{list.mainProductBuy} 15 '')</p>
##                            #end
                        </section>
                    </section>
                    <section class="ls_detal">
                        <div class="ls_detal_area">
                            <span>$!{l.provinceName}</span>
                            <span>$!{l.areaName}</span>
                        </div>
                        <div class="ls_detal_price">
                            #if ("$!{l.priceNum}" != "" && "$!{l.priceNum}" != "0" && "$!{l.priceNum}" != "0.0")
                                <span class="fontbold fontorange">$!{l.priceNum}</span>
                                #if("$!{l.priceUnits}" == "") 
                                    元 
                                #else 
                                    $!{l.priceUnits} 
                                #end 
                            #else 
								面议 
                            #end
                        </div>
                        <div class="ls_detal_mem">
                            #if("$!{l.memberCode}"=="10011001")
                                <img src="${address.img}/huanbao/images/company/zht.png" width="51" hight="23"/>
                                <a href="#" class="1001001">高级会员</a>
                            #else
                                <a href="#" class="1001003">普通会员</a>
                            #end
                        </div>
                        <div class="ls_detal_link">
                            <a target="_blank" class="ls_detal_link02" href="http://www.huanbao.com/esite/companyContact50001450.htm">联系方式</a>
                        </div>
                    </section>
                </article>
            </li>
        #end
        <!-- 标王列表结束-->
		
        #foreach($list in $!{page.records})
            <li>
                <article class="ts_ls_box_top">
                    <section class="ls_selector">
                        <input type="checkbox" name="supplyIds" value="$!{list.supply.id}"/>
                        <div class="ls_selector_tipbox">
                            <p class="lss_tipbox_t">您可以:</p>
                            <p class="lss_tipbox_b">
                                <a href="javascript:batchInquiry()" class="lss_tipbox_p">批量询价</a>
                                <span class="lss_tipbox_o">或</span>
                                <a href="javascript:infoContrast()" class="lss_tipbox_c">对比产品</a>
                            </p>
                        </div>
                    </section>
                    <section class="ls_ifo">
                        <header class="ls_ifo_img">
                            #if("$!{list.supply.photoCover}"=="")
                                <a href="#springUrl('/supply/detail')$!{list.supply.id}.htm" target="_blank" title="$!{list.supply.title}">
                                    <img src="$!{address.img}/huanbao/images/trade/tradelist/noprod.gif" alt="$!{list.supply.title}"/>
                                </a>
                            #else
                                <a href="#springUrl('/supply/detail')$!{list.supply.id}.htm" target="_blank" title="$!{list.supply.title}">
                                    <img src="$!{address.thumb}$!{address.resource}$!{list.supply.photoCover}&width=100&height=100" alt="$!{list.supply.title}"/>
                                </a>
                            #end
                        </header>
                        <section class="ls_ifo_details">
                            <h3>
                                <a href="#springUrl('/supply/detail')$!{list.supply.id}.htm" target="_blank" title="$!{list.supply.title}">
                                    <span class="fontorange comm_em_red p_ellipsis">#printTitle($!{list.supply.title} 20 '')</span>
                                </a>
                            </h3>
                            <p class="ls_ifo_content comm_em_red p_ellipsis">#printTitle($!{list.supply.detailsQuery} 85 '...')</p>
                            <p class="ls_ifo_com">
								#if("$!{list.comp.memberCode}"=="10011001")
									<a href="http://$!{list.comp.domainTwo}.huanbao.com/" target="_blank">#printTitle($!{list.comp.name} 16 '')</a>
								#else
									<a href="$!{address.esite}/index$!{list.comp.id}.htm" target="_blank">#printTitle($!{list.comp.name} 16 '')</a>
								#end
                                #if("$!{list.account.qq}" != "")
                                    <a rel="nofollow" target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=$!{list.account.qq}&amp;site=www.huanbao.com.com&amp;menu=yes" title="QQ:$!{list.account.qq}">
                                        <img border="0" src="http://wpa.qq.com/pa?p=2:$!{list.account.qq}:4" alt="QQ:$!{list.account.qq}" onerror="this.src='${address.img}/zz91/china/images/qq.gif'" />
                                    </a>
                                #end
                            </p>
                            #if("$!{list.supply.mainProductBuy}" !="")
                                <div class="clear"></div>
                                <p class="ls_ifo_com">主营产品:#printTitle($!{list.supply.mainProductBuy} 15 '')</p>
                            #end
                        </section>
                    </section>
                    <section class="ls_detal">
                        <div class="ls_detal_area">
                            <span>$!{list.area.province}</span>
                            <span>$!{list.area.city}</span>
                        </div>
                        <div class="ls_detal_price">
                            #if ("$!{list.supply.priceNum}" != "" && "$!{list.supply.priceNum}" != "0" && "$!{list.supply.priceNum}" != "0.0")
                                <span class="fontbold fontorange">$!{list.supply.priceNum}</span>
                                #if("$!{list.supply.priceUnits}" == "") 元 #else $!{list.supply.priceUnits} #end 
                            #else 
                                面议
                            #end
                        </div>
                        <div class="ls_detal_mem">
                            #if("$!{list.comp.memberCode}"=="10011001")
                                <a href="http://www.huanbao.com/fudus/vpindex.htm" target="_blank" style="padding-top: 0px;">
									<img src="${address.img}/huanbao/images/company/zht.png" width="51" hight="23"/>
								</a>
                                <a class="1001001">高级会员</a>
                            #else
                                <a class="1001003">普通会员</a>
                            #end
                        </div>
                        <div class="ls_detal_link">
                            <a href="$!{address.esite}/companyContact$!{list.comp.id}.htm" class="ls_detal_link02" target="_blank">联系方式</a>
                        </div>
                    </section>
                </article>
            </li>
        #end
        <div class="clear"></div>
    </ul>
</div>
<div class="pagesbar margintop8">
    #if($search.encodeKeywords)
        #set($kws="?keywords=$!{search.encodeKeywords}")
    #end 
    #staticPageNav($!{page} "#springUrl('/list')" "-$!{id}" "" "$!{kws}")
    <div class="clear">&nbsp;</div>
    #staticMorePageNav($!{page} "#springUrl('/list')" "-$!{id}" "" "$!{kws}")
</div>
