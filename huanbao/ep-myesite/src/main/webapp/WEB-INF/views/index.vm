## section Two: top supply, top buy, top news, advertisements
<section class="container mt clearfix" >
	<div class="grid_4 well">
		<div class="well-head">
			<h3 class="t">今日最新供应信息</h3>
		</div>
		<div class="well-content">
			#cacheFragment("$!{address.fragment}/fragment/trade/newestSupply.htm?size=16&gq","","","3600")
				<ul>
					#foreach($l in $!{list})
					<li>
						<span class="red">供应</span>
						<a href="$!{address.esite}/detail$!{l.id}.htm" target="_blank">#printTitle($!{l.title} 20 "")</a>
					</li>
					#end
				</ul>
			#end
		</div>
	</div>
	<div class="grid_4 well">
		<div class="well-head">
			<h3 class="t">今日最新求购信息</h3>
		</div>
		<div class="well-content">
			#cacheFragment("$!{address.fragment}/fragment/trade/newestBuy.htm?size=16&qgc","","","3600")
				<ul>
					#foreach($l in $!{list})
					<li>
						<span class="red">求购</span>
						<a href="$!{address.trade}/buy/detail$!{l.id}.htm" target="_blank">#printTitle($!{l.title} 20 "")</a>
					</li>
					#end
				</ul>
			#end
		</div>
	</div>
	<div class="grid_4 well">
		<div class="well-head">
			<h3 class="t">今日最新资讯</h3>
		</div>
		<div class="well-content">
		##	#cacheFragment("$!{address.fragment}/fragment/news/topNews.htm?size=10&rc", "", "", "3600")
		#cacheFragment("$!{address.fragment}/fragment/news/categoryNews.htm?size=10&zx", "", "", "3600")
				<ul>
					#foreach($l in $!{list})
					<li>
						<span>.</span>
						<a href="$!{address.news}/details$!{l.id}.htm" target="_blank">#printTitle($!{l.title} 20 "")</a>
					</li>
					#end
				</ul>
			#end
			<div class="center mt">
				<a href="$!{address.www}/fudus/leaveMsg.htm" target="_blank">
					<img src="$!{address.img}/huanbao/images/myesite/hbmem_46.jpg" />
				</a>
			</div>
		</div>
	</div>
</section>

