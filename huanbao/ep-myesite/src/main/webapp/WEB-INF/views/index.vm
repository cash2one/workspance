<script type="text/javascript">
	jQuery(document).ready(function(){
		myrc.select("Js_select");
		myrc.menu(1, 1);
		
		
	jQuery("#updateMblogPhoto").click(function(){
    	var showpicId= jQuery("#showPicId").val();
       	var photoBox=new Asto.util.Modal({
    	   title:"上传博文照片",
    	   url:"#springUrl('/photo/mblogPromptUpload.htm')?targetType=mblog&targetId=0&albumId=-1&max=1",
		   width:500,
    	   height:350,
    	   modal:true
        });
    	Asto.util.Callback.setSubmitCallback(function(success, data){
    		if(success=="true"){
				var photos=new Array();
			    if(jQuery("#photos").val()!=""){
					photos.push(jQuery("#photos").val());
				}
			   //重新加载新上传的图片
    			for(var key in data){
				var i=jQuery("#showPicId").val();
    				if(i==8){
					    jQuery("#updateMblogPhoto").parent().css("display","none"); 
    				}
    				##jQuery("#updateMblogPhoto"+i).attr('src',"$!{address.thumb}$!{address.resource}"+data[key]+"&width=88&height=84");
    				jQuery("#showImg").append("<li><img src='$!{address.thumb}$!{address.resource}"+data[key]+"&width=88&height=84' id='updateMblogPhoto"+i+"' width='88px' height='84px'/></li>");
    				i++;
    				jQuery("#showPicId").val(i);
    				
					photos.push(data[key]);
					$("#show-count").html(parseInt(i)-1);
					$("#show-count2").html(9-parseInt(i));
    		    }
				jQuery("#photos").val(photos.join(","));
    		}
    		photoBox.close();
        });  
     
    })

});
</script>
