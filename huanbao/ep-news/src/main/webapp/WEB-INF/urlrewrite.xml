<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!-- 资讯 -->
	<rule>
		<from>^/list(\d+).htm$</from>
		<to>/list.htm?categoryCode=$1</to>
	</rule>
	<rule>
		<from>^/list([0-9,a-z,A-Z,%]+).htm$</from>
		<to>/list.htm?categoryKey=$1</to>
	</rule>
	<rule>
		<from>^/list(\d*)--s(\d*)--l(\d*).htm$</from>
		<to>/list.htm?categoryCode=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	
	<rule>
		<from>^/compNewsList--s(\d*)--l(\d*).htm$</from>
		<to>/compNewsList.htm?start=$1&amp;limit=$2</to>
	</rule>
	
	<rule>
		<from>^/list([0-9,a-z,A-Z,%]*)--s(\d*)--l(\d*).htm$</from>
		<to>/list.htm?categoryKey=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/details(\d+).htm$</from>
		<to>/details.htm?id=$1</to>	
	</rule>
	<rule>
		<from>^/search--([0-9,a-z,A-Z,%,+,.]*)--s(\d*)--l(\d*).htm$</from>
		<to>/search.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/search--([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/search.htm?keywords=$1</to>
	</rule>
	
	<!-- 专题 -->
	<rule>
		<from>^/zhuanti/list--([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/zhuanti/list.htm?category=$1</to>
	</rule>
	<rule>
		<from>^/zhuanti/list--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/zhuanti/list.htm?category=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
</urlrewrite>