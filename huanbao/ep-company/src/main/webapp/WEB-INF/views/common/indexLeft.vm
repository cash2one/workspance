<div class="cpleft floatleft">
	#cacheFragment("$!{address.fragment}/fragment/company/loadIndustryChain.htm?size=100&chain","","","600")
		<div class="cpleft_brobj margintop8">
			<ul>
				<li class="cpleft_title"><a href="#">热门产业圈</a></li>
				<li>
					<ul class="cpleft_infolist">
						#foreach($industryChain in $!{industryChains})
							#if($!{search.industryChainId}==$!{industryChain.id}&&$!{cityCode}==$!{industryChain.areaCode})
								<li><span>· </span><a href="$!{address.company}/list-$!{industryChain.id}-$!{industryChain.areaCode}.htm"><b class="red">$!{industryChain.areaName}$!{industryChain.categoryName}</b></a></li>
							#else
								<li><span>· </span><a href="$!{address.company}/list-$!{industryChain.id}-$!{industryChain.areaCode}.htm">$!{industryChain.areaName}$!{industryChain.categoryName}</a></li>
							#end
						#end
					</ul>
				</li>
			</ul>
		</div>
	#end
	<div class="cpleft_brobj margintop8">
		<ul>
			<li class="cpleft_title">按客户类型查找环保公司</li>
			<li>
				<ul class="cpleft_infolist">
					#foreach($list in $!{companyCategory})
						<li><span>· </span><a href="#springUrl('/index')----$!{list.value}.htm">$!{list.name}</a></li>
					#end
				</ul>
			</li>
		</ul>
	</div>
	<div class="cpleft_brobj margintop8">
		<ul>
			<li class="cpleft_title">按行业查找公司</li>
			<li>
				<ul class="cpleft_infolist">
					#foreach($company in $!{companyIndustry})
						<li><span>· </span><a href="#springUrl('/index')--$!{company.code}--.htm">$!{company.name}</a></li>
					#end
				</ul>
			</li>
		</ul>
	</div>
								
	#cacheFragment("$!{address.fragment}/fragment/company/recommendCompany.htm?categoryCode=10001010&size=30&sdfd","","","600")
		<div class="cpleft_brobj margintop8">
			<ul>
				<li class="cpleft_title">推荐公司</li>##<a href="$!{address.trade}/buy/list.htm"></a>
				<li>
					<ul class="cpleft_infolist">
						#foreach($company in $!{list})
							<li><span>· </span><a href="http://$!{company.domainTwo}.huanbao.com">#printTitle($!{company.name}  12  "")</a></li>
						#end
					</ul>
				</li>
			</ul>
		</div>
	#end
	#cacheFragment("$!{address.fragment}/fragment/company/newestCompany.htm?size=20","","","600")
		<div class="cpleft_brobj margintop8">
			<ul>
				<li class="cpleft_title">最新加入商家</li>
				<li>
					<ul class="cpleft_infolist">
						#foreach($newcompany in $!{list})
							<li><span>· </span><a href="$!{address.esite}/index$!{newcompany.id}.htm">#printTitle($!{newcompany.name}  12  "" )</a></li>
						#end
					</ul>
				</li>
			</ul>
		</div>
	#end
</div>