<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	
	 <rule>
       <from>^/index--(\d*)--.htm$</from>
       <to>/index.htm?industryCode=$1</to>
    </rule>
    
     <rule>
       <from>^/index----(\d*).htm$</from>
       <to>/index.htm?businessCode=$1</to>
    </rule>
	
	<rule>
       <from>^/index--([0-9,a-z,A-Z,%,+]*)--(\d*)--(\d*)--(\d*)--(\d*)--s(\d*)--l(\d*).htm$</from>
       <to>/index.htm?keywords=$1&amp;provinceCode=$2&amp;areaCode=$3&amp;industryCode=$4&amp;businessCode=$5&amp;start=$6&amp;limit=$7</to>
    </rule>
  
    <rule>
       <from>^/list--a(\d*).htm$</from>
       <to>/list.htm?industryCode=$1</to>
    </rule>
    
    <rule>
       <from>^/list--(\d*).htm$</from>
       <to>/list.htm?ctagsId=$1</to>
    </rule>
    
      <rule>
       <from>^/list--c(\d*).htm$</from>
       <to>/list.htm?businessCode=$1</to>
    </rule>
 
    
    <rule>
       <from>^/list--b(\d*).htm$</from>
       <to>/list.htm?cityCode=$1</to>
    </rule>
    
     <rule>
       <from>^/list-(\d*)-(\d*).htm$</from>
       <to>/list.htm?industryChainId=$1&amp;cityCode=$2</to>
    </rule>
    
    
    <rule>
       <from>^/list--([0-9,a-z,A-Z,%,+]*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--s(\d*)--l(\d*).htm$</from>
       <to>/list.htm?keywords=$1&amp;provinceCode=$2&amp;areaCode=$3&amp;industryCode=$4&amp;businessCode=$5&amp;ctagsId=$6&amp;cityCode=$7&amp;industryChainId=$8&amp;start=$9&amp;limit=$10</to>
    </rule>
    
    <!-- 为seo 404百度蜘蛛抓 定义的url规则 开始-->
    <rule>
       <from>^/list--([0-9,a-z,A-Z,%,+]*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)s(\d*)--l(\d*).htm$</from>
       <to>/list.htm?keywords=$1&amp;provinceCode=$2&amp;areaCode=$3&amp;industryCode=$4&amp;businessCode=$5&amp;ctagsId=$6&amp;cityCode=$7&amp;industryChainId=$8&amp;start=$9&amp;limit=$10</to>
    </rule>
    
    <rule>
       <from>^/list-(\d*).htm$</from>
       <to>/list.htm?ctagsId=$1</to>
    </rule>
    <!-- 为seo 404百度蜘蛛抓 定义的url规则 结束-->
     
    <!-- 最新公司 -->
 	<rule>
    	<from>^/newc-a([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/newc.htm?provinceCode=$1</to>
    </rule>
    
    <rule>
    	<from>^/newc-p(\d*).htm$</from>
    	<to>/newc.htm?pageNum=$1</to>
    </rule>
    
    <rule>
    	<from>^/newc-a([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/newc.htm?provinceCode=$1&amp;pageNum=$2</to>
    </rule>
    
     	<rule>
    	<from>^/newc-c([0-9,a-z,A-Z,%,+]*).htm$</from>
    	<to>/newc.htm?industryCode=$1</to>
    </rule>
    
    <rule>
    	<from>^/newc-p(\d*).htm$</from>
    	<to>/newc.htm?pageNum=$1</to>
    </rule>
    
    <rule>
    	<from>^/newc-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
    	<to>/newc.htm?industryCode=$1&amp;pageNum=$2</to>
    </rule>
</urlrewrite>