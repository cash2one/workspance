#set($layout='/layout/normLayout1.vm')

<link rel="stylesheet" type="text/css" href="http://img0.huanbao.com/huanbao/mblog/css/wei.css" />
<link rel="stylesheet" type="text/css" href="http://img0.huanbao.com/huanbao/mblog/css/jquery.alert.css"/>
<script type="text/javascript" src="http://img0.huanbao.com/huanbao/mblog/js/wei.js"></script>
<script type="text/javascript" src="http://img0.huanbao.com/huanbao/mblog/js/jquery.easydrag.js"></script>
<script type="text/javascript" src="http://img0.huanbao.com/huanbao/mblog/js/jquery.alert.js"></script>


<div class="wei-main">
	<div class="wei-nav">
    	<div class="wei-nav-title fl">
        	<img src="http://img0.huanbao.com/huanbao/mblog/images/title_img.jpg" />
        </div>
        
        <div class="wei-nav-1 fl">
        	<ul>
            	<li><a href="#springUrl('/index').htm">首页</a></li>
                <li><a href="#springUrl('/mblog').htm">个人主页</a></li>
                <li><a href="#springUrl('/info').htm">账户设置</a></li>
            </ul>
        </div>
        
		<form action="#springUrl('/searchMblog.htm')" method="post" id="searchMblog" name="searchMblog">
        <div class="wei-nav-search fl">
        	<input type="text" class="wei-nav-text" id="mBlogKeywords" name="mBlogKeywords"/>
            <a href="javascript:searchFindMblog();"></a>
        </div>
        </form>        
        <div class="wei-nav-2 fl">
        	<ul>
            	<li><a href="$!{address.myesite}/index.htm">生意管家</a></li>
                <li><a href="http://www.huanbao.com">环保网首页</a></li>
            </ul>
        </div>
        
        <div class="wei-nav-exit fr">
        	<a href="#springUrl('/logout.htm')"></a>
        </div>
        <div class="cb"></div>
    </div>
    <!--end wei-nav-->
    <div class="SearchFind">
    	<div class="sf-search">
        	<div class="sf-search-nav">
            	<ul id="sf-search-nav">
                	<li><a href="#springUrl('/searchMblog').htm">微交流</a></li>
                    <li><a href="#" style="color:#000;font-weight:bold">找人</a></li>
                    <div class="cb"></div>
                </ul>
            </div>
            <!--end sf-nav-->
            <div class="cb"></div>
			<form action="#springUrl('/searchFind.htm')" method="post" id="searchFind" name="searchFind">
            <div class="sf-search-search">
            	<div class="sf-search-img"><img src="http://img0.huanbao.com/huanbao/mblog/images/search_img.jpg" /></div>
                <div class="sf-search-box">
                	<input type="text" class="sf-search-text" id="keywords" name="keywords"/>
                    <a href="javascript:searchFindInfo();"></a>
                </div>
                <!--end sf-search-box-->
                <div class="cb"></div>
            </div>
			</form>
            <!--end sf-search-->
            <div class="sf-search-result">
            	找到 #if($!{page.records}) $!{page.records.size()} #else 0 #end 条结果
            </div>
            <!--end sf-result-->
        </div>
        <!--end sf-search-->
        <div class="sf-left">
            #foreach($!{l} in $!{page.records})
            <div class="sf-left-item">
            	<div class="sf-item-img">
                	<a href="#springUrl('/ublog')$!{l.mBlogInfo.id}.htm">
						#if($!{l.mBlogInfo.headPic} && $!{l.mBlogInfo.headPic}!="") 
							<img src="$!{address.thumb}$!{address.resource}$!{l.mBlogInfo.headPic}" width="80px;" height="80px"/>
						#else
							<img src="http://img0.huanbao.com/huanbao/mblog/images/heading_img5.jpg" />
						#end
					</a>
                </div>
                <!--end sf-item-img-->
                <div class="sf-item-box">
                	<div class="ff-text-box1">
                    	<div class="ff-box1-left">
                         <a href="#springUrl('/ublog')$!{l.mBlogInfo.id}.htm">$!{l.mBlogInfo.name}</a>
						</div>
                        <div class="cb"></div>
                    </div>
                    <!--end ff-text-box1-->
                    <div class="sf-item-box4">
                    	<div class="sf-box4-img fl">
							#if(${l.mBlogInfo.sex}==1)
							<img src="http://img0.huanbao.com/huanbao/mblog/images/icon_ms.jpg" />
							#else
							<img src="http://img0.huanbao.com/huanbao/mblog/images/img10.jpg" />
							#end
                        </div>
                        <div class="sf-box4-text fl">
                        	$!{l.provinceName}&nbsp;$!{l.areaName}<a href="#springUrl('/ublog')$!{l.mBlogInfo.id}.htm"><span class="greed ml8">$!{address.mblog}/ublog$!{l.mBlogInfo.id}.htm</span></a>
                        </div>
                        <div class="cb"></div>
                    </div>
                    <!--end sf-item-box4-->
                    <div class="ff-text-box2">
                    	<div class="ff-box2-left fl">关注<span class="greed">$!{l.followCount}</span></div>
                        <div class="ff-box2-fans fl">粉丝<span class="greed">$!{l.fansCount}</span></div>
                        <div class="ff-box2-text fl">微博<span class="greed">$!{l.mblogCount}</span></div>
                        <div class="cb"></div>
                    </div>
                    <!--end ff-text-box2-->
                    <div class="ff-text-box3">
                    	简介： 
						#if($!{l.compProfile.detailsQuery}) 
							#printTitle($!{l.compProfile.detailsQuery} 60 "")... 
						#end
                    </div>
                    <!--end ff-text-box3-->
                </div>
                <!--end sf-item-box-->
                <div class="sf-item-focus">
					#if($!{l.type}=='0')
                	<img src="http://img0.huanbao.com/huanbao/mblog/images/focus.jpg" onclick="AddFocus(this,$!{l.mBlogInfo.id});" />
					#end
					#if($!{l.type}=='1')
                	<img src="http://img0.huanbao.com/huanbao/mblog/images/focus2.jpg"/>
					#end
					#if($!{l.type}=='2')
                	<img src="http://img0.huanbao.com/huanbao/mblog/images/focus3.jpg"/>
					#end
                </div>
                <!--end sf-item-focus-->
            </div>
            <!--end sf-left-item-->
			#end
			
		    #if($!{page} && $!{keywords})
				#mBlogPageNav(${page} "#springUrl('/searchMblog')/$!{keywords}" "" "")
			#end
            <!--end ff-page-->
        </div>
        <!--end sf-left-->
        <div class="sf-right">
        	<div class="sf-right-title">
            	可能感兴趣的人
            </div>
            <!--end sf-right-title-->
			#foreach($!{l}  in $!{infoList})
            <div class="sf-right-item">
            	<div class="sf-item-img2">
                	<a href="#springUrl('/ublog')$!{l.id}.htm" onmouseover="heada(this,$!{l.id});" onmouseout="headb(this);">
						#if($!{l.headPic})
						<img src="$!{address.thumb}$!{address.resource} $!{l.headPic}" style="width:50px;height:50px;"/>	
						#else
						<img src="http://img0.huanbao.com/huanbao/mblog/images/heading_img3.jpg" />	
						#end
					</a>
                    <div class="heada" onmouseover="head1(this);" onmouseout="head2(this);"></div>
                    <div class="cb"></div>
                </div>
                <!--end sf-item-img2-->
                <div class="sf-item-box2">
                	<div class="sf-box2-title">
                    	<a href="#springUrl('/ublog')$!{l.id}.htm">$!{l.name}</a>
                    </div>
                    <div class="sf-box2-focus">
                    	<img src="http://img0.huanbao.com/huanbao/mblog/images/focus.jpg" onclick="AddFocus(this,$!{l.id});" />
                    </div>
                </div>
                <!--end sf-item-box2-->
                <div class="cb"></div>
            </div>
			<!--end sf-right-item-->
			#end
        </div>
        <!--end sf-right-->
		<div class="cb"></div>
    </div>
    <!--end SearchFind-->
</div>
<!--end main-->



#if(($!{newFansCount} && $!{newFansCount}>0) || ($!{commentCount} && $!{commentCount}>0) || $!{anteCount} && $!{anteCount}>0)
	
<div class="FloatBox">
	<div class="fb-close">
    	<img src="http://img0.huanbao.com/huanbao/mblog/images/close3.jpg" />
    </div>
    <!--end fb-close-->
    <div class="cb"></div>
	#if($!{newFansCount} && $!{newFansCount}>0)
	<div class="fb-box">
    	$!{newFansCount}位新粉丝，<a href="/mblog/newfans.htm">查看粉丝</a>
    </div>
	#end
    <!--end fb-box-->
	#if($!{commentCount} && $!{commentCount}>0)
    <div class="fb-box">
    	$!{commentCount}条新评论，<a href="/mblog/comments.htm">查看回复</a>
    </div>
	#end
    <!--end fb-box-->
	#if($!{anteCount} && $!{anteCount}>0)
    <div class="fb-box">
    	$!{anteCount}个新@我，<a href="/mblog/ante.htm">查看@我</a>
    </div>
	#end
    <!--end fb-box-->
</div>
<!--end FloatBox-->
#end


<script>
 function searchFindInfo(){
 	 jQuery("#searchFind").submit();
 } 
 
 function searchFindMblog(){
 	jQuery("#searchMblog").submit();
 }
</script>
