<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	 <rule>
        <from>^/index(\d+).htm$</from>
        <to>/index.htm?groupId=$1</to>    	
     </rule>
     <rule>
        <from>^/index--s(\d*)--l(\d*).htm$</from>
        <to>/index.htm?start=$1&amp;limit=$2</to>    	
     </rule>
     <rule>
        <from>^/index(\d+)--s(\d*)--l(\d*).htm$</from>
        <to>/index.htm?groupId=$1&amp;start=$2&amp;limit=$3</to>    	
     </rule>
     
     <rule>
		<from>^/index/([a-zA-Z0-9]+)--s(\d*)--l(\d*).htm$</from>
		<to>/index.htm?type=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
     <rule>
		<from>^/myfans--s(\d*)--l(\d*).htm$</from>
		<to>/myfans.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/newfans--s(\d*)--l(\d*).htm$</from>
		<to>/newfans.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/mblog--s(\d*)--l(\d*).htm$</from>
		<to>/mblog.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/mblog--([a-zA-Z0-9]+)--s(\d*)--l(\d*).htm$</from>
		<to>/mblog.htm?type=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/originalMblog--(\d*)--s(\d*)--l(\d*).htm$</from>
		<to>/originalMblog.htm?mblogId=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/originalMblog(\d+).htm$</from>
		<to>/originalMblog.htm?mblogId=$1</to>
	</rule>
	<rule>
		<from>^/ublog(\d+).htm$</from>
		<to>/ublog.htm?infoId=$1</to>
	</rule>
	<rule>
		<from>^/ublog(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/ublog.htm?infoId=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/ublog(\d+)/([a-zA-Z0-9]+)--s(\d*)--l(\d*).htm$</from>
		<to>/ublog.htm?infoId=$1&amp;type=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/ublog(\d+)/([a-zA-Z0-9]+).htm$</from>
		<to>/ublog.htm?infoId=$1&amp;type=$2</to>
	</rule>
	<rule>
		<from>^/ufans(\d+).htm$</from>
		<to>/ufans.htm?infoId=$1</to>
	</rule>
	<rule>
		<from>^/ufans(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/ufans.htm?infoId=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/originalMblog(\d+)--(\d*)--s(\d*)--l(\d*).htm$</from>
		<to>/originalMblog.htm?infoId=$1&amp;mblogId=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/follow--s(\d*)--l(\d*).htm$</from>
		<to>/follow.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/follow(\d+).htm$</from>
		<to>/follow.htm?groupId=$1</to>
	</rule>
	<rule>
		<from>^/follow(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/follow.htm?groupId=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/ufollow(\d+).htm$</from>
		<to>/ufollow.htm?infoId=$1</to>
	</rule>
	<rule>
		<from>^/ufollow(\d+)--s(\d*)--l(\d*).htm$</from>
		<to>/ufollow.htm?infoId=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/topic(\d+).htm$</from>
		<to>/topic.htm?mblogId=$1</to>
	</rule>
	<rule>
		<from>^/comments--s(\d*)--l(\d*).htm$</from>
		<to>/comments.htm?start=$1&amp;limit=$2</to>
	</rule>	
	<rule>
		<from>^/ante--s(\d*)--l(\d*).htm$</from>
		<to>/ante.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/message--s(\d*)--l(\d*).htm$</from>
		<to>/message.htm?start=$1&amp;limit=$2</to>
	</rule>	
	<rule>
		<from>^/searchFind/([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/searchFind.htm?keywords=$1&amp;start=$1&amp;limit=$2</to>
	</rule>	
	<rule>
		<from>^/searchMblog/([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/searchMblog.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
</urlrewrite>     