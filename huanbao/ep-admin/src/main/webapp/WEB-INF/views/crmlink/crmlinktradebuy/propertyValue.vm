<script type="text/javascript">
Ext.onReady(function(){
	var grid=new com.zz91.trade.editbuy.listTradePropertyValueGird({
		id:EDITBUY.BUY_GRID,
		title:"专业属性信息",
		layout:"fit",
		region:"center"
	});
	
	grid.getStore().baseParams["categoryCode"]=$!{categoryCode};
