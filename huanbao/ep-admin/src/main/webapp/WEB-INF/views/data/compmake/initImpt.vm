			<td><input type="text" value="4" name="details"/></td>
			<td>4</td>
			<td>4</td>
		</tr>
		<tr>
			<td class="required">供求商</td>
			<td colspan="3">
				<select name="mainUse">
        			<option value="1" selected>供应商</option>
        			<option value="2">求购商</option>
        		</select>
			</td>
		</tr>
		<tr>
