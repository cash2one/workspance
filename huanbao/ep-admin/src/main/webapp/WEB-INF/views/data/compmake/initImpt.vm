			<td><input type="text" value="" name="addressZip"/></td>
			<td></td>
			<td></td>
		</tr>
		<tr>
			<td>公司网址</td>
			<td><input type="text" value="26" name="domain"/></td>
			<td></td>
			<td>26</td>
		</tr>
		<tr>
			<td>加工方式</td>
			<td><input type="text" value="9" name="processMethod"/></td>
			<td></td>
			<td>9</td>
		</tr>
		<tr>
			<td>工艺</td>
			<td><input type="text" value="10" name="process"/></td>
			<td></td>
			<td>10</td>
		</tr>
