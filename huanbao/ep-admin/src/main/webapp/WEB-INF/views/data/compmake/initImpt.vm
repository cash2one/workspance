		<tr>
			<td>年营业额</td>
			<td><input type="text" value="18" name="yearTurnover"/></td>
			<td></td>
			<td>18</td>
		</tr>
		<tr>
			<td>年出口额</td>
			<td><input type="text" value="19" name="yearExports"/></td>
			<td></td>
			<td>19</td>
		</tr>
		<tr>
			<td>质量控制</td>
			<td><input type="text" value="20" name="qualityControl"/></td>
			<td></td>
			<td>20</td>
		</tr>
		<tr>
			<td>公司注册地</td>
			<td><input type="text" value="22" name="registerArea"/></td>
			<td></td>
			<td>22</td>
		</tr>
		<tr>
			<td>企业类型</td>
			<td><input type="text" value="23" name="enterpriseType"/></td>
			<td></td>
			<td>23</td>
		</tr>
		<tr>
			<td>信息有效判断列</td>
			<td><input type="text" value="1" name="isRight"/></td>
			<td>1</td>
			<td>1</td>
		</tr>
	</table>
	
	<h4>来源</h4>
	<div>
		#parse('/data/sourceFlag.vm')
	</div>
	<h4 >Excel文件</h4>
	<div><input type="file" name="excel" /></div>
	
	<div><input type="submit" name="importexcel" value="填写正确后，点这里导入" /></div>
</form>

