		<tr>
			<td>研发部门人数</td>
			<td><input type="text" value="13" name="developerNum"/></td>
			<td></td>
			<td>13</td>
		</tr>
		<tr>
			<td>厂房面积</td>
			<td><input type="text" value="14" name="plantArea"/></td>
