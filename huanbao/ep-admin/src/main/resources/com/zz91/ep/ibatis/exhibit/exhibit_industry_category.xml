<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="industry">
	<typeAlias alias="industry" type="com.zz91.ep.domain.exhibit.ExhibitIndustryCategory" />
	
	<resultMap id="industryResult" class="industry">
		<result property="id" column="id" />
		<result property="name" column="name"/>
		<result property="code" column="code"/>
		<result property="gmtCreated" column="gmt_created"/>
		<result property="gmtModified" column="gmt_modified"/>
	</resultMap>
	
	<sql id="fullColumn">
		i.id,
		i.name,
		i.code,
		i.gmt_created,
		i.gmt_modified
	</sql>
	
	<select id="queryExhibitIndustryCategoryAll" resultMap="industryResult">
		select 
			 <include refid="industry.fullColumn" />
		from exhibit_industry_category i
		<include refid="common.pageOrderBy" />
	</select>
	
</sqlMap>