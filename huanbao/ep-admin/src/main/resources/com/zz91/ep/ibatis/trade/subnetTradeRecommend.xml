<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="subnetTradeRecommend">

	<typeAlias alias="subnetTradeRecommend" type="com.zz91.ep.domain.trade.SubnetTradeRecommend"/>
	
	<insert id="insertSubnetTradeRecommend" parameterClass="subnetTradeRecommend">
		insert into `subnet_trade_recommend`
		(
		`subnet_category`,
		`supply_id`,
		`gmt_created`,
		`gmt_modified`)
		values
		(
		#subnetCategory#,
		#supplyId#,
		now(),
		now()
		)
		<selectKey keyProperty="id" resultClass="java.lang.Integer">
			select last_insert_id()
		</selectKey>
	</insert>
	
	<delete id="deleteSubnetTradeRecommend" parameterClass="java.lang.Integer">
		delete from subnet_trade_recommend where supply_id=#supplyId#
	</delete>
</sqlMap>