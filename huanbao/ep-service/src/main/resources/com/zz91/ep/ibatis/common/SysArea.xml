<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="sysarea">

	<typeAlias alias="sysArea" type="com.zz91.ep.domain.sys.SysArea" />

	<resultMap class="sysArea" id="simpleResultMap">
		<result property="code" column="code" />
		<result property="name" column="name"/>
	</resultMap>

	<select id="querySysAreasByCode" resultMap="simpleResultMap" parameterClass="java.lang.String">
		select
			code,name
		from sys_area
		where code like concat(#value#, '____')
	</select>
	
	<select id="getSysAreaByCode" resultMap="simpleResultMap" parameterClass="java.lang.String">
		select
			code,name
		from sys_area
		where code =#value#
	</select>
	
	<select id="queryNameByCode" resultClass="java.lang.String" parameterClass="java.lang.String">
		select name from sys_area where code = #value#
	</select>
	
</sqlMap>