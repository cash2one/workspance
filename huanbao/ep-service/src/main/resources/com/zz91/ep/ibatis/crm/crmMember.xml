<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="crmMember">
	
	<select id="queryName"  parameterClass="java.lang.String" resultClass="java.lang.String">
		select name from crm_member where code = #value#;
	</select>
	
</sqlMap>