<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="subnetLink">

	<typeAlias alias="subnetLink" type="com.zz91.ep.domain.trade.SubnetLink"/>
	
	<resultMap class="subnetLink" id="simplyColumnMap">
		<result property="name" column="name"/>
		<result property="link" column="link"/>
	</resultMap>
	
	<select id="querySubnetLinkByparentId" parameterClass="java.lang.Integer" resultMap="simplyColumnMap">
		select name,link from subnet_link 
		where parent_id=#parentId#
	</select>
	
</sqlMap>