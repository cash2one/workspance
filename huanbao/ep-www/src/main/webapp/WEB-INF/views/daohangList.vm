							#end
							#if($velocityCount%3 == 1)
							<a href="$!{address.www}/daohangList--$!{parentCode}--$!{value}.htm" ><em class="c077">$!{value}</em></a>
							#end
							#if($velocityCount%3 == 2)
							<a href="$!{address.www}/daohangList--$!{parentCode}--$!{value}.htm" ><em class="cF27">$!{value}</em></a>
							#end
						#end
					</p>
				</div>
				#end
				#cacheFragment("$!{address.fragment}/fragment/news/categoryNews.htm?categoryCode=1000&size=10", "", "", "600")
				<div class="pg_mksrf_p">
					<h3>环保资讯快递</h3>
					<ul>
						#foreach($l in $!{list})
						<li><a href="$!{address.news}/details${l.id}.htm" target="_blank" title="$!{l.title}">#printTitle($!{l.titleIndex} 21 "")</a></li>
						#end
					</ul>
				</div>
				#end
				#cacheFragment("$!{address.fragment}/fragment/company/keywordCompany.htm?keyword=$!{keywordEncode}&size=10", "", "", "600")
				<div class="pg_mksrf_p">
					<h3>供应$!{keywords}厂家</h3>
					<ul>
						#foreach($l in $!{list})
						<li><a href="$!{address.esite}/index$!{l.id}.htm" title="$!{l.name}">#printTitle($!{l.name} 23 "")</a></li>
						#end
					</ul>
				</div>
