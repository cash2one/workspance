#set($layout="/layout/trade/qymapdefault.vm")

<div class="sonspace"></div>
<div class="header sonmenu_bj">
        <div class="sonmenu_left"></div>
        <div class="sonmenu_cen">
                <ul>
                    <li class="sonmenu_centit"><a href="$!{address.www}">首页</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/ws">污水处理</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/ys">原水处理</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/kq">空气净化</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/yj">材料药剂</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/beng">泵</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/fa">阀</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/fj">风机</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/hw">环卫设备</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/yqyb">仪器仪表</a></li>
                    <li class="sonmenu_centit"><a href="$!{address.www}/zh">综合设备</a></li>
                </ul>
        </div>
        <div class="sonmenu_right"></div>
        <div class="clear"></div>
</div>
<div class="sonspace"></div>
<div class="header">
    <div class="qyleft">
      <div class="qyleft_oneclass">
     			<div class="qyleft_oneclass_tit">按行业搜索：</div>
                <div class="qyleft_oneclass_info">
					 <ul>
    				#cacheFragment("$!{address.fragment}/fragment/trade/zwCategory.htm?a", "", "", "300")
    					#foreach($ct in $!{list})
    					<li><a href="#springUrl('/trade')/zw-c$!{ct.id}.htm" >$!{ct.name}</a></li>
    					#end
    				#end
                     </ul>
					 <div class="clear"></div>
                </div>
	  </div>
      <div class="sonspaces"></div>
	  <div class="qyleft_oneclass">
			    <div class="qyleft_oneclass_tit">$!{categoryName}</div>
				<div class="qyleft_oneclass_info">
    				<ul>
    					 #foreach($list in $!{page.records})
    						<li><a href="#springUrl('/trade')/cp-$!{list.code}.htm" target="_blank">#printTitle($!{list.name} 18 "")</a></li>
    					 #end
                    </ul>
    			<div class="clear"></div>
                </div>
    			<div class="qypage">
    				#if($!{search.parentId})
    					#set($sc="-c"+$!{search.parentId})
    				#end
    				#staticPageNavp($!{page} "#springUrl('/trade/zw')" "$!{sc}" "")
    			</div>
      </div>
    </div>
	<div class="qyright">
		<div class="qyleft_oneclass">
            <div class="qyleft_oneclass_tit">最新产品供应</div>
            <div class="qyleft_oneclass_list">
                    <ul>
                        #cacheFragment("$!{address.fragment}/fragment/trade/hotSupply.htm?limit=10&sub","","","600")
                        	#foreach($l in $!{list})
                                <li>·<a href="$!{address.esite}/detail$!{l.supply.id}.htm" target="_blank" title="$!{l.supply.title}">#printTitle($!{l.supply.title} 10 "")</a></li>
                            #end
                        #end
                    </ul>
                    <div class="clear"></div>                     
            </div>
         </div>
		 <div class="sonspaces"></div>
         <div class="qyleft_oneclass">
                <div class="qyleft_oneclass_tit">最新加入公司</div>
                <div class="qyleft_oneclass_list">
                        <ul>
                            #cacheFragment("$!{address.fragment}/fragment/company/newestCompany.htm?size=10&sub","","","600")
                        		#foreach($l in $!{list})
                                    <li>·<a href="$!{address.esite}/index$!{l.id}.htm" target="_blank" title="$!{l.name}">#printTitle($!{l.name} 10 "")</a></li>
                				#end
                			#end
                        </ul>
                        <div class="clear"></div>
                </div>
         </div>
	</div>
	<div class="clear"></div>
</div>
