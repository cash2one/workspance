</div>
<div class="pagesbar margintop8">
    #staticPageNav($!{page} "#springUrl('/trade/supply/index')" "--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--$!{search.time}--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--$!{page.sort}--$!{page.dir}" "")
</div>
</div>
<script language="javascript">
    jQuery(document).ready(function(){
        // 地区下拉框初始化
        var _selectAreaCode=["#provinceCode"];
        var selectAreaCode=new hb.util.Selector({
            selects:_selectAreaCode,
            url:"#springUrl('/trade/getAreaCode.htm')",
            field:{code:"code",label:"name"}
        });
        selectAreaCode.init({
            rootCode:"10011000",
            #if(!$!{search.region}) 
                initCode:"10011000"
            #else
                initCode:"$!{search.region}"
            #end
        });
    });
    function changePrice(){
    	if(jQuery("#searchPriceFrom").val() == ""){
        	jQuery("#searchPriceFrom").focus();
        	return false
        }
        if(jQuery("#searchPriceFrom").val() == "0"){
        	jQuery("#searchPriceFrom").val(1);
        }
        if(jQuery("#searchPriceTo").val() == "") {
        	jQuery("#searchPriceTo").focus();
        	return false
        } 
    	window.location="#springUrl('/trade/supply/index')"+"--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--$!{search.time}--"+jQuery("#searchPriceFrom").val()+"--"+jQuery("#searchPriceTo").val()+"--$!{search.showStyle}--$!{page.sort}--$!{page.dir}"+"--s0--l$!{page.limit}.htm";
    }
    function changeRegion(){
    	window.location="#springUrl('/trade/supply/index')"+"--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--"+jQuery("#provinceCode").val()+"--$!{search.time}--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--$!{page.sort}--$!{page.dir}"+"--s0--l$!{page.limit}.htm";
    }
    function changeSort(){
    	window.location="#springUrl('/trade/supply/index')"+"--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--$!{search.time}--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--"+jQuery("#searchSort").val()+"--$!{page.dir}"+"--s0--l$!{page.limit}.htm";
    }
    function changeDay(){
    	window.location="#springUrl('/trade/supply/index')"+"--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--"+jQuery("#searchTime").val()+"--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--$!{page.sort}--$!{page.dir}"+"--s0--l$!{page.limit}.htm";
    }
    function batchInquiry(){
    	var a=new Array();
        jQuery.each(jQuery("input[name=supplyIds]"),function(idx,e){
            if(e.checked){
            	a.push(e.value)
            }
        });
    	if (a.length < 2) {
            hb.util.Message.show({
                msg:"请选择要询价的供应信息！",
                autoClose:10000
            });
        }else if (a.length >5){
			hb.util.Message.show({
                msg:"最多同时询价5条供应信息！",
                autoClose:10000
            });
		} else {
        	var url = "?";
        	for(var i=0; i < a.length; i++) {
                if (i ==0 ) {
                	url+="ids="+a[i];
                } else {
                    url+="&ids="+a[i];
                }
            }
            window.location="#springUrl('/trade/supply/batchInquiry.htm')"+url;
        }
    }
    function infoContrast(){
        var a=new Array();
        jQuery.each(jQuery("input[name=supplyIds]"),function(idx,e){
            if(e.checked){
            	a.push(e.value)
            }
        });
		
        if (a.length < 2) {
            hb.util.Message.show({
                msg:"请选择要对比的供应信息！",
                autoClose:10000
            });
        }else if (a.length >5){
			hb.util.Message.show({
                msg:"最多同时对比5条供应信息！",
                autoClose:10000
            });
		} else {
            var url = "?";
            for(var i=0; i < a.length; i++) {
                if (i ==0 ) {
                	url+="ids="+a[i];
                } else {
                	url+="&ids="+a[i];
                }
            }
            window.location="#springUrl('/trade/supply/infoContrast.htm')"+url;
        }
    }
</script>
