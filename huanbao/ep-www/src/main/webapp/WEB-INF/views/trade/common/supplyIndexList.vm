<div class="tdl_lsbar_bottom margintop8">
    <div class="tdl_btn_lsbar_viewtype">
        <a class="viewtype_txt" href="#"></a>
        <a class="viewtype_pic" href="#"></a>
        <a class="viewtype_inquiry" href="#">批量询价</a>
        <a class="viewtype_contrast" href="#">对比产品</a>
    </div>
    <div class="tdl_btn_lsbar_pagesize">
        <span class="lsbar_pagesize_t">每页显示数量：</span>
        <a href="#springUrl('/trade/supply/index')--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--$!{search.time}--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--$!{page.sort}--$!{page.dir}--s0--l30.htm" class="pagesize_selected">30</a>
        <a href="#springUrl('/trade/supply/index')--$!{search.category}--$!{search.encodeKeywords}--$!{search.propertyValue}--$!{search.group}--$!{search.region}--$!{search.time}--$!{search.priceFrom}--$!{search.priceTo}--$!{search.showStyle}--$!{page.sort}--$!{page.dir}--s0--l40.htm" class="pagesize_selected">40</a>
