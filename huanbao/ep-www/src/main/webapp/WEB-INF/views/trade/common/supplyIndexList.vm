<div class="clear"></div>
<div class="tdl_lsbar margintop8 html5radius borderwhitegray">
    <div class="tdl_lsbar_form">
        <label class="tdlf_forprice" for="tdlf_price">单价</label>
        <input type="text" class="tdlf_price_s" id="searchPriceTo" value="$!{search.priceTo}" maxlength="10"/>
        <span class="tdlf_pric_to">至</span>
        <input type="text" class="tdlf_price_e" id="searchPriceFrom" value="$!{search.priceFrom}" maxlength="10"/>
        <span class="tdlf_unit">元</span>
        <select id="provinceCode" name="provinceCode" class="tdlf_area" onchange="changeRegion()">
            <option value="">所在地区</option>
        </select>
        <select class="tdlf_date" id="searchTime" onchange="changeDay()">
            <option value="" #if("" == "$!{search.time}") selected #end>发布时间</option>
            <option value="1" #if("1" == "$!{search.time}") selected #end>一天内</option>
			<option value="7" #if("7" == "$!{search.time}") selected #end>一周内</option>
			<option value="30" #if("30" == "$!{search.time}") selected #end>一个月内</option>
            <option value="90" #if("90" == "$!{search.time}") selected #end>三个月内</option>
            <option value="180" #if("180" == "$!{search.time}") selected #end>半年内</option>
        </select>
        <select class="tdlf_sorts" id="searchSort" onchange="changeSort()">
            <option value="" #if("" == "$!{page.sort}") selected #end>默认排序</option>
            <option value="1" #if("1" == "$!{page.sort}") selected #end>价格从高到低</option>
            <option value="2" #if("2" == "$!{page.sort}") selected #end>价格从低到高</option>
        </select>
        <input type="button" value="筛 选"class="tdlf_formsub" onclick="changePrice()"/>
    </div>
    <div class="clear"></div>
</div>
