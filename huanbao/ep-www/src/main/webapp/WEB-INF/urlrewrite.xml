<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/daohangList--(\d*)--([0-9,a-z,A-Z,%]*).htm$</from>
		<to>/daohangList.htm?parentCode=$1&amp;keywords=$2</to>
	</rule>

	<!-- <rule> <from>^/ws$</from> <to>/ws/index.htm</to> </rule> <rule> <from>^/ys$</from> 
		<to>/ys/index.htm</to> </rule> <rule> <from>^/kq$</from> <to>/kq/index.htm</to> 
		</rule> <rule> <from>^/yj$</from> <to>/yj/index.htm</to> </rule> <rule> <from>^/beng$</from> 
		<to>/beng/index.htm</to> </rule> <rule> <from>^/fa$</from> <to>/fa/index.htm</to> 
		</rule> <rule> <from>^/fj$</from> <to>/fj/index.htm</to> </rule> <rule> <from>^/hw$</from> 
		<to>/hw/index.htm</to> </rule> <rule> <from>^/yqyb$</from> <to>/yqyb/index.htm</to> 
		</rule> <rule> <from>^/zh$</from> <to>/zh/index.htm</to> </rule> -->

	<rule>
		<from>^/(ws|ys|kq|yj|beng|fa|fj|hw|yqyb|zh/?)$</from>
		<to>/trade/childtrade/index.htm?child=$1</to>
	</rule>

	<rule>
		<from>^/trade/?$</from>
		<to>/trade/index.htm</to>
	</rule>

	<!-- trade rule start -->
	<rule>
		<from>^/trade/supply/index--(\d*).htm$</from>
		<to>/trade/supply/index.htm?category=$1</to>
	</rule>
	<rule>
		<from>^/trade/supply/index--(\d*)--([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/supply/index.htm?category=$1&amp;keywords=$2</to>
	</rule>
	<rule>
		<from>^/trade/supply/index--(\d*)--([0-9,a-z,A-Z,%,+]*)--([0-9,a-z,A-Z,%,_,:]*)--(\d*)--(\d*)--(\d*)--(\d*)--(\d*)--([a-z]*)--(\d*)--([a-z]*)--s(\d*)--l(\d*).htm$</from>
		<to>/trade/supply/index.htm?category=$1&amp;keywords=$2&amp;propertyValue=$3&amp;group=$4&amp;region=$5&amp;time=$6&amp;priceFrom=$7&amp;priceTo=$8&amp;showStyle=$9&amp;sort=$10&amp;dir=$11&amp;start=$12&amp;limit=$13</to>
	</rule>

	<rule>
		<from>^/trade/buy/list--([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/buy/list.htm?keywords=$1</to>
	</rule>
	<rule>
		<from>^/trade/buy/list--([0-9,a-z,A-Z,%,+]*)--(\d*)--(\d*)--(\d*)--([a-z]*)--(\d*)--([a-z]*)--s(\d*)--l(\d*).htm$</from>
		<to>/trade/buy/list.htm?keywords=$1&amp;group=$2&amp;region=$3&amp;time=$4&amp;showStyle=$5&amp;sort=$6&amp;dir=$7&amp;start=$8&amp;limit=$9</to>
	</rule>
	<rule>
		<from>^/trade/buy/detail(\d+).htm$</from>
		<to>/trade/buy/detail.htm?id=$1</to>
	</rule>
	<!-- 标签子页 -->
	<rule>
		<from>^/trade/bq(\d+).htm$</from>
		<to>/trade/bq.htm?id=$1</to>
	</rule>
	<!-- 标签子页 分页 -->
	<rule>
		<from>^/trade/bq(\d+)-p(\d+).htm$</from>
		<to>/trade/bq.htm?id=$1&amp;pageNum=$2</to>
	</rule>
	<rule>
		<from>^/trade/zdbuy$</from>
		<to>/trade/zdbuy/list.htm</to>
	</rule>

	<rule>
		<from>^/trade/zdbuy/list(\d+).htm$</from>
		<to>/trade/zdbuy/list.htm?categoryCode=$1</to>
	</rule>

	<rule>
		<from>^/trade/zdbuy/list(\d*)--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/trade/zdbuy/list.htm?categoryCode=$1&amp;keywords=$2&amp;start=$3&amp;limit=$4</to>
	</rule>
	<rule>
		<from>^/trade/zdbuy/detail(\d+).htm$</from>
		<to>/trade/zdbuy/detail.htm?id=$1</to>
	</rule>
	<!-- 子网列表页 -->
	<rule>
		<from>^/trade/cp-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/cp.htm?code=$1</to>
	</rule>
	<rule>
		<from>^/trade/cp-([0-9,a-z,A-Z,%,+]*)-(\d*)-(\d*)-(\d*)-(\d*)-(\d*)-p(\d*).htm$</from>
		<to>/trade/cp.htm?code=$1&amp;region=$2&amp;time=$3&amp;priceFrom=$4&amp;priceTo=$5&amp;sort=$6&amp;pageNum=$7</to>
	</rule>
	<!-- 最新产品 -->
	<rule>
		<from>^/trade/newg-c([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/newg.htm?category=$1</to>
	</rule>
	<rule>
		<from>^/trade/newg-p(\d*).htm$</from>
		<to>/trade/newg.htm?pageNum=$1</to>
	</rule>
	<rule>
		<from>^/trade/newg-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
		<to>/trade/newg.htm?category=$1&amp;pageNum=$2</to>
	</rule>
	<rule>
		<from>^/trade/newb-c([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/newb.htm?category=$1</to>
	</rule>
	<rule>
		<from>^/trade/newb-p(\d*).htm$</from>
		<to>/trade/newb.htm?pageNum=$1</to>
	</rule>
	<rule>
		<from>^/trade/newb-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
		<to>/trade/newb.htm?category=$1&amp;pageNum=$2</to>
	</rule>
	<!-- 最新子网 -->
	<rule>
		<from>^/trade/zw-c([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/zw.htm?parentId=$1</to>
	</rule>
	<rule>
		<from>^/trade/zw-p(\d*).htm$</from>
		<to>/trade/zw.htm?pageNum=$1</to>
	</rule>
	<rule>
		<from>^/trade/zw-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
		<to>/trade/zw.htm?parentId=$1&amp;pageNum=$2</to>
	</rule>


	<!-- 最新标签 -->
	<rule>
		<from>^/trade/newbq-c([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/trade/newbq.htm?code=$1</to>
	</rule>
	<rule>
		<from>^/trade/newbq-p(\d*).htm$</from>
		<to>/trade/newbq.htm?pageNum=$1</to>
	</rule>
	<rule>
		<from>^/trade/newbq-c([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>
		<to>/trade/newbq.htm?code=$1&amp;pageNum=$2</to>
	</rule>
	<!-- trade rule end -->


	<!-- 技术文章 -->
	<rule>
		<from>^/compnews/technicalarticles/list--s(\d*)--l(\d*).htm$</from>
		<to>/compnews/technicalarticles/list.htm?start=$1&amp;limit=$2</to>
	</rule>
	<rule>
		<from>^/compnews/technicalarticles/newsList(\d*)--s(\d*)--l(\d*).htm$</from>
		<to>/compnews/technicalarticles/newsList.htm?categoryCode=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/compnews/technicalarticles/details(\d+).htm$</from>
		<to>/compnews/technicalarticles/details.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/compnews/technicalarticles/newsList(\d+).htm$</from>
		<to>/compnews/technicalarticles/newsList.htm?categoryCode=$1</to>
	</rule>
	<rule>
		<from>^/compnews/technicalarticles/tagsList--([0-9,a-z,A-Z,%,+]*)--s(\d*)--l(\d*).htm$</from>
		<to>/compnews/technicalarticles/tagsList.htm?keywords=$1&amp;start=$2&amp;limit=$3</to>
	</rule>
	<rule>
		<from>^/compnews/technicalarticles/newsDetails(\d+).htm$</from>
		<to>/compnews/technicalarticles/newsDetails.htm?id=$1</to>
	</rule>
</urlrewrite>