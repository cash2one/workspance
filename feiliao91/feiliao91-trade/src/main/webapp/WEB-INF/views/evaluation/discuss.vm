    $(".discuss-list-dl").delegate(".j_seepick span","click",function(){
        imgsrc_list =[];
        var this_obj = $(this);
        bigimg_index = $(this).index();
        bigimg_sum = $(this).parent().find("span").length;

        for(var i=0;i<bigimg_sum;i++){

          imgsrc_list.push($(this).parent().find("span").eq(i).attr("bigimgSrc"))  
        }

        if(this_obj.hasClass("thispic")){
            this_obj.removeClass("thispic");
            this_obj.parent().siblings(".big_picture").hide(500);
            return;
        }
        $(".thispic").removeClass("thispic");
        $(this).addClass("thispic");
        if(!this_obj.parent().hasClass("openBig")){
            $(".openBig").removeClass("openBig")
            $(".big_picture:visible").hide(500);
        }
        $(this).parent().addClass("openBig")
        this_obj.parent().siblings(".big_picture").find(".bigimg-pic").attr("src",imgsrc_list[bigimg_index])
        this_obj.parent().siblings(".big_picture").show(500)
    })
