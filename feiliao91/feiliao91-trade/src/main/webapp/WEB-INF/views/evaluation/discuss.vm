    var cm_All = $!{integral};//卖家信誉
    var cmName = ["djimg1","djimg2","djimg3","djimg4"]
    function getCm(obj,cmAll){   
        var first = start = 10;
        var cmIimg = ""
        for(var i=0;i<=3;i++){
            if(i>0){
               start = start*5; 
            }
            if(start<=cmAll&&cmAll<start*5){   //判断在什么阶段
                for(var j=0;i<5;j++){
                    cmAll = cmAll - start;
                    cmIimg += "<i  class="+ cmName[i] +"></i>";
                    if(cmAll<start){ 
                        if(!(cmAll<first)){   //如果还没高于最小的信誉等级重新回调
                            getCm(cmAll)
                        }
                        obj.prepend(cmIimg)
                        return true;
                    }
                }
                
                return;
            }else if(cmAll>=625*first){
                for(var j=0;i<5;j++){
                    start = 125*first;
                    cmAll = cmAll - start;
                    cmIimg += "<i  class="+ cmName[3] +"></i>";
                    if(cmAll<start){ 
                        if(!(cmAll<first)){   //如果还没高于最小的信誉等级重新回调
                            getCm(cmAll)
                        }
                        obj.prepend(cmIimg)
                        return true;
                    }
                }
            }
            // console.log(i);
            
        }
    }
    
