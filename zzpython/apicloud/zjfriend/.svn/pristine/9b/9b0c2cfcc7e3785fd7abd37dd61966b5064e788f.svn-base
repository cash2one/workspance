<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <link rel="stylesheet" type="text/css" href="../css/api.css"/>
    <link rel="stylesheet" type="text/css" href="../css/common.css"/>
    <style>
        body{
            height: auto;
        }
        .category {
            width: 100%;
        }

        .category li {

            display: inline-block;
            width: 24%;
            text-align: center;
            padding: 10px 0;

        }

        .category li a {
            width: 88%;
            box-sizing: border-box;
            border: 1px solid #DDDDDE;
            padding: 7px 0;
            border-radius: 20px;
            color: #424242;
        }
        .category li a:hover {
            border: 1px solid #DDDDDE;
            background-color: #EC971F;
            color: #fff;
        }
        .more-lable{
            padding: 10px;
            background-color: #E7E7E7;
        }
    </style>
<body>
<ul class="category">
    
</ul>
<script src="../script/zepto.min.js"></script>
<script>
	function getNavlist(){
		api.showProgress();
    	api.ajax({
			url : 'http://apptest.zz91.com/sex/navlist.html',
			method : 'get',
			timeout : 30,
			data : {
				t : (new Date()).getTime().toString()
			},
			dataType : 'text',
			returnAll : false
		}, function(ret, err) {
			if (ret) {
				ret=eval(ret)
				var navHtml = '';
				for (var i in ret) {
					navHtml +='<li onclick="openFrame('+ret[i].id+','+i+')"><a class="mui-navigate-right">' + ret[i].typename + '</a></li>'
				}
				$(".category").html(navHtml);
			} 
			api.hideProgress();
		});
    }
    apiready=function(){
        getNavlist();
    }

    function openFrame(tid,m) {
        try {
        	api.execScript({name:'root', script: 'openFrame('+tid.toString()+','+m+')'});
        	api.execScript({
        		name:'root',
	            script: '_closeNav();'
            });
            //api.execScript({frameName:'frame_'+m.toString(), script: 'changesexmain('+tid.toString()+')'});
        } catch (e) {
            //alert(e)
        }
    }
</script>
</body>
</html>