<!doctype html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<title></title>
		<link rel="stylesheet" type="text/css" href="../css/api.css"/>
		<link rel="stylesheet" type="text/css" href="../css/mui.min.css"/>
		<link href="../css/main.min.css" rel="stylesheet" type="text/css" />
		<link rel="stylesheet" href="../css/main.css" type="text/css">
		<link rel="stylesheet" href="../css/my_favorite.css" type="text/css">
	</head>
	<body>
		<div class="box_list">
			<header class="mui-bar mui-bar-nav">
				<a class="mui-action-back mui-icon mui-icon-left-nav mui-pull-left" onclick="api.closeWin();"></a>
				<h1 class="mui-title">我的收藏</h1>
			</header>
			<div class="main">
			</div>
		</div>
		<script type="text/javascript" src="../script/api.js"></script>
		<script type="text/javascript" src="../script/zepto.min.js"></script>
		<script type="text/javascript" src="../script/common.js"></script>
		<script type="text/javascript" src="../script/laytpl.js"></script>
		<script>
			apiready = function() {
				var oHeight = $(".main").height();
				var oWidth = $(".main").width();
				var topHeight = $(".mui-bar").height();
				api.openFrame({
					name: 'list',
				    url: './my_favorite_list.html',
				    rect:{
				        x:0,
				        y:topHeight,
				        w:oWidth,
				        h:oHeight
				    },
				    pageParam: {name: 'test'},
				    bounces: true,
				    bgColor: 'rgba(0,0,0,0)',
				    vScrollBarEnabled:true,
				    hScrollBarEnabled:true
				});
			}
		</script>
	</body>
</html>











