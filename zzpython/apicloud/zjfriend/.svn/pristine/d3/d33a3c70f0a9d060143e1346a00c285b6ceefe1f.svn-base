		<script type="text/javascript" src="../script/api.js"></script>
		<script type="text/javascript" src="../script/zepto.min.js"></script>
		<script type="text/javascript" src="../script/common.js"></script>
		<script type="text/javascript" src="../script/laytpl.js"></script>
		<script>
			function pinlun(obj){
				$("#div_contentbg").css("display","none");
				obj.style.height="60px";
				$(".plun_bt_tj_button").css("display","block");
				document.getElementById("txtContent0").focus();
			}

			var currPage = 1;
			var stoploading = false;
			function changesexmain() {
				api.showProgress();
				api.ajax({
					url : hosturl+'/sex/show_feedback.html?aid=25871',
					method : 'get',
					timeout : 30,
					data : {},
					dataType : "json",
					returnAll : false
				}, function(ret, err) {
					var pageHtml = "";
					if (ret) {
						$.each(ret.result,function(index,item){
							var getTpl = $api.html($api.byId("info-list"));
							laytpl(getTpl).render(item, function(html) {
								pageHtml = pageHtml + html;
							});
						})
						$("#MyComments").html(pageHtml);
					}
					else {
						$("#MyComments").html("<div style='width=100%;text-align:center;padding-top:20px;height:100%'>暂无信息</div>");
						$(".jzmore").css("display", "none");
						api.toast({
							msg : ('错误码：' + err.code + '；错误信息：' + err.msg + '网络状态码：' + err.statusCode),
							duration : 3000,
							location : 'bottom'
						});
					}
					api.hideProgress();
				})
			};
			apiready = function() {
				api.addEventListener({
					name : 'keyback'
				}, function(ret, err) {
					api.closeWidget();
				});
				api.addEventListener({
					name : 'scrolltobottom'
				}, function(ret, err) {
					loadmorelist(hosturl+'/sex/show_feedback.html?aid=25871', currPage + 1);
				});
				changesexmain();
				//下拉刷新
				api.setRefreshHeaderInfo({
					visible : true,
					// bgColor: '#F2F2F2',
					bgColor : '#E6E6E6',
					textColor : '#000000',
					textDown : '下拉刷新',
					textUp : '释放刷新',
				}, function(ret, err) {
					changesexmain()
					api.refreshHeaderLoadDone();
					api.hideProgress();
					api.toast({
						msg : '数据已是最新！',
						duration : 3000,
						location : 'bottom'
					});
				});
			}
			function loadmorelist(url, page) {
				if (stoploading == false) {
					stoploading = true;
					$(".jzmore").html("更多加载中...");
					api.ajax({
						url : hosturl+'/sex/show_feedback.html?aid=25871',
						method : 'get',
						timeout : 30,
						data : {},
						dataType : "json",
						returnAll : false
					}, function(ret, err) {
						if (ret && ret != "") {
							var pageHtml = "";
							$.each(ret.result,function(index,item){
								var getTpl = $api.html($api.byId("info-list"));
								laytpl(getTpl).render(item, function(html) {
									pageHtml = pageHtml + html;
								});
							})
							$("#MyComments").append(pageHtml);
							stoploading = false;
							currPage = page;
							api.hideProgress();
							$(".jzmore").html("点击加载更多");
							} else {
								stoploading = false;
								$(".jzmore").css("display", "none");
							};
					});
				}
			}
		</script>
	</body>

