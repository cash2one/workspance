	content: '';
}

.aui-input-row {
	overflow: hidden;
	clear: left;
}

.aui-input-group .aui-input-row {
	padding: 5px 0;
}
.aui-input-group .aui-input-row:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 0;
	height: 1px;
	content: '';
	background-color: #c8c7cc;
	-webkit-transform: scaleY(.5);
	      transform: scaleY(.5);
}

.aui-input-row label {
	float: left;
	width: 30%;
	padding: 10px 15px;
	font-family: sans-serif;
	line-height: 1.1;
}

.aui-input-row label ~ input,
.aui-input-row label ~ select,
.aui-input-row label ~ textarea {
	float: left;
	width: 70%;
	padding-left: 0;
	margin-bottom: 0;
	border: 0;
}
.aui-input-row .aui-btn {
	float: right;
	width: 15%;
	padding: 10px 15px;
	font-family: sans-serif;
	line-height: 1.1;
}

.aui-button-row {
	position: relative;
	padding-top: 5px;
	text-align: center;
}

.aui-input-group .aui-button-row {
	padding: 5px 0;
}

.aui-input-group .aui-button-row:before, .aui-input-group .aui-button-row:after, .aui-input-group:before, .aui-input-group:after {
	-webkit-transform: scaleY(0);
	      transform: scaleY(0);
}

.aui-input-row {
	position: relative;
}

/*HEADER*/
#aui-header {
	padding-top: 20px;
}
.aui-bar {
	top: 0;
	right: 0;
	left: 0;
	z-index: 10;
	width: 100%;
	height: 45px;
	line-height: 45px;
	background-color: #3498db;
	color: #ffffff;
}
.aui-bar a {
	color: #ffffff;
}

.aui-bar-dark {
	background-color: #34495e;
}
.aui-bar-primary {
	background-color: #1abc9c;
}
.aui-bar-info {
	background-color: #3498db;
}
.aui-bar-danger {
	background-color: #e74c3c;
}
.aui-bar-success {
	background-color: #2ecc71;
}
.aui-bar-warning {
	background-color: #f1c40f;
}
.aui-bar .aui-iconfont {
	position: relative;
	z-index: 20;
	font-size: 20px;
	color: #ffffff;
	font-size: 400;
	line-height: 45px;
}
.aui-bar .aui-pull-left {
	padding-left: 10px;
	font-size: 20px;
}
.aui-bar .aui-pull-right {
	padding-right: 10px;
	font-size: 20px;
}
.aui-bar .aui-btn {
	position: relative;
	z-index: 20;
	font-size: 16px;
	height: 45px;
	line-height: 45px;
	padding-top: 0;
	padding-bottom: 0;
	margin: 0;
	border-radius: 0;
}
.aui-bar .aui-btn .aui-iconfont {
	font-size: 22px;
	line-height: 45px;
	padding: 0;
	margin: 0;
	color: #ffffff;
}
.aui-title {
	position: absolute;
	width: 100%;
	margin: 0 -10px;
	font-size: 18px;
	line-height: 45px;
	color: #ffffff;
	text-align: center;
	white-space: nowrap;
}

.aui-title a {
	color: inherit;
}
.aui-bar .aui-title {
	right: 40px;
	left: 40px;
	width: auto;
	margin: 0;
	overflow: hidden;
	text-overflow: ellipsis;
}
.aui-bar-nav {
	top: 0;
}
/*底部切换栏*/
.aui-nav {
	position: fixed;
	right: 0;
	left: 0;
	bottom: 0;
	z-index: 10;
	height: 55px;
}
.aui-nav.aui-bar-tab {
	background: #ffffff;
	font-size: 12px;
}
.aui-nav.aui-bar-tab p {
	font-size: 12px;
}
.aui-nav.aui-bar-tab .aui-iconfont {
	padding-top: 8px;
	font-size: 22px;
	color: #8f8f94;
}
.aui-nav.aui-bar-tab:after {
	position: absolute;
	right: 0;
	top: 0;
	left: 0;
	height: 1px;
	content: '';
	background-color: #c8c7cc;
	-webkit-transform: scaleY(.3);
	      transform: scaleY(.3);
}
.aui-nav.aui-bar-tab .active .aui-iconfont, .aui-nav.aui-bar-tab .active p {
	color: #faa732;
}
/*loading*/
.aui-loading {
	width: 48px;
	height: 48px;
	position: fixed;
	top: 50%;
	left: 50%;
	margin-left: -24px;
	margin-top: -48px;
	z-index: 9999;
	background: url('../image/loading.gif') no-repeat #434343;
	opacity: 0.7;
	background-size: 32px;
	background-position: center;
	border-radius: 3px;
	padding: 5px;
}
