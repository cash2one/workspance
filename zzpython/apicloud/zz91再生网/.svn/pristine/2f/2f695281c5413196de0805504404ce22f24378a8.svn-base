.cantop-mation li{
	padding: 10px 8px;
	border-bottom: 1px solid #e9e8e9
}
.mation-title{
	font-size: 14px;
	color: #404040;
	line-height: 26px;
}
.summary{
	color: #959595;
	font-size: 12px;
}
.mation-pic{
	float: left;
	margin-right: 9px;
}
.go-top{
	color: #fff;
	width: 30px;
	height: 15px;
	text-align: center;
	line-height: 15px;
	background-color: #ff0000;
	float: right;
	font-size: 10px;
	margin-top: 2px;
}
.click-number{
	float: right;
	font-size: 10px;
	color: #b4b4b4;
	padding-left: 14px;
	position: relative;
}
.see-img{
	height: 13px;
	width: 13px;
	position: absolute;
	display: block;
	background-image: url(../image/imageslist.png);
    background-size:220px;
    background-position: -95px -74px;
    left: -3px;
    top: 7px;
}
#navmark { -webkit-transition: 300ms;}


#wrap {
      height: 100%;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-flex-flow: column;
      flex-flow: column;
    }
    #main {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
      flex: 1;
    }
    .footer span{
      width: 50%;
      float: left;
      text-align: center;
      height: 50px;
      border-top: 1px solid #e1e1e1;
      position: relative;
      line-height: 50px;
      padding-top: 10px;
      background-color: #f2f2f2;
      color: #959595
    }
    .footer span i{
      display: block;
      height: 15px;
      width: 15px;
      background-image: url(../image/imageslist.png);
      background-size: 250px;
      position: absolute;
      left: 50%;
      margin-left: -7px;
      top: 7px;

    }
    .gonew{
      border-right:1px solid #e1e1e1;
    }
    .footer .gonew i{
      background-position: -135px -0px;
    }
    .footer .notinto i{
      background-position: -135px -17px;
    }