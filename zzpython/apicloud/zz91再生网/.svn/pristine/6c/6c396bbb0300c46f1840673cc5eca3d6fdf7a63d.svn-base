html, body {
    height: 100%;
    width: 100%;
}

#wrap {
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-flow: column;
    flex-flow: column;
}

#main {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

#footer {
    width: 100%;
    text-align: center;
    height: 56px;
    line-height: 55px;
    background-color: #fcfcfc;
    border-top: 1px solid #ebebeb
}

ul {

    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}



#footer li {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    height: 55px;
}
.bottom_btn{
    line-height: 12px;
    font-size: 12px;
}
.bottom_btn span{
    display: block;
}
.bottom_btn i{
    background-position-y: 4px;
    padding-top: 14px;
    margin-top: 2px;
    font-size: 12px;
    color: #323237;
    width: 33px;
    height: 30px;
    background-repeat: no-repeat no-repeat;
    background-position-x: center;
    background-size: 320px;
    background-image: url(../image/imageslist.png);
    display: inline-block;
}
.cloud i{
    background-position: -81px -33px;
}

.client i{
    background-position: -84px -61px;
}

.dev i{
    background-position: -78px -89px;
}

.doc i{
    background-position: -80px -116px;
}
.active{
    color: #01ba2e
}
.active .cloud i{
    background-position: -81px -148px;
}

.active .client i{
    background-position: -84px -179px;
}

.active .dev i{
    background-position: -79px -207px;
}

.active .doc i{
    background-position: -79px -236px;
}

.active .bottom_btn i{
    color: #6ab494;
}

.topbar {
    background: #01ba2e;
    height: 48px;
/*    text-align: center;*/
    display: none;
    color: #323237;
}

.activebar {
    display: block;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-flex-flow: row;
           flex-flow: row;
}

.category,
.infonumber{
    width: 50px;
    padding-top: 9px;
    position: relative;
}
.category i,
.info-img,
.gout-img{
    display: block;
    background-image: url(../image/imageslist.png);
    background-size:220px 220px;
    width: 15px;
    height: 15px;
    margin: 0 auto
}
.infonumber s{
    display: block;
    width: 8px;
    height: 8px;
    background-color: #fff;
    position:absolute;
    top: 5px;
    right: 7px;
    border-radius: 5px;
    border: 1px solid #01ba2e
}
.category p,
.infonumber p{
    color:#fff;
    font-size: 12px;
    line-height: 16px;
    text-align: center;
}
.category i{
    background-position: -39px 0;
}
.info-img{
    background-position: -39px -15px;
}
.my-gout{
    display: none;
}
.gout-img{
    background-size:320px;
    background-position: -114px -219px;
}
