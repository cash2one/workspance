	<script type="text/javascript">
		apiready = function() {
			//供求置顶
			$(".buybutton").on("click", function() {
				var proid=$(this).attr("proid");
				topproducts(proid)
			})
		}
		function topproducts(proid) {
			var html = "";
			html += '<p style="color: #000000;">您想购买的置顶时间为：</p>'
			html += '<div class="topproductsmain">'
			html += '<ul>'
			html += '<li><input class="aui-radio" type="radio" name="topproducts" id="report1" value="5" /><label class="aui-radio-name" for=report1>5元/天</label></li>'
			html += '<li><input class="aui-radio" type="radio" name="topproducts" id="report2" value="25" checked /><label class="aui-radio-name" for=report2>25元/周</label></li>'
			html += '<li><input class="aui-radio" type="radio" name="topproducts" id="report3" value="120" /><label class="aui-radio-name" for=report3>120元/月</label></li>'
			html += '<li><input class="aui-radio" type="radio" name="topproducts" id="report4" value="1200" /><label class="aui-radio-name" for=report4>1200元/年</label></li>'
			html += '</li>'
			html += '</div>'
			$aui.alert({
				title : '',
				content : html,
				buttons : ['取消', '确定'],
				radius : 10,
				titleColor : '#333',
				contColor : '#333',
				btnColor : '',
				wintype : 'topproducts'
			}, function(ret) {
				if (ret == 1) {
					api.showProgress({
						title : '扣款中...',
						modal : false
					});
					var company_id = UserInfo.memberID();
					var usertoken = UserInfo.token();
					var chk_value = $("#topproductsvalue").val();
					
					var data = {
						paytype : '9',
						money : chk_value,
						company_id : company_id,
						usertoken : usertoken,
						appsystem : api.systemType,
						datatype : 'json',
						proid : proid,
						t : (new Date()).getTime().toString()
					}
					alert(JSON.stringify(data))
					api.ajax({
						url : hosturl + 'qianbao/qianbaopay.html',
						method : 'get',
						timeout : 30,
						dataType : 'json',
						returnAll : false,
						data : {
							values : data
						}
					}, function(ret, err) {
						//alert(JSON.stringify(ret))
						if (ret) {
							if (ret.err == 'false') {
								alert(ret.errtext);
								ajaxInfo();
							}
							if (ret.err == 'true' && ret.blanceflag == '0') {
								yuebuzuFun();
							}
							if (ret.err == 'true' && ret.blanceflag == '1') {
								alert(ret.errtext);
							}
							api.hideProgress();
						} else {
							api.hideProgress();
						};
					});
				}
			})
		}
		function topproductscheck(valu) {
			$("#topproductsvalue").val(valu);
		}
		function ajaxInfo() {
			var company_id = UserInfo.memberID();
			var usertoken = UserInfo.token();
			api.ajax({
				url : hosturl + 'qianbao/?company_id=' + company_id + '&usertoken=' + usertoken + '&appsystem=' + api.systemType + '&datatype=json&t=' + (new Date()).getTime().toString(),
				method : 'get',
				timeout : 30,
				dataType : 'json',
				returnAll : false
			}, function(ret, err) {
				if (ret) {
					// alert(JSON.stringify(ret))
					api.hideProgress();
				} else {
					api.hideProgress();
				};
			});
		}
