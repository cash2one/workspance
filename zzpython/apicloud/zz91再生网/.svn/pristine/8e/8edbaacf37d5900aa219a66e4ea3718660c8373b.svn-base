body{
	background-color: #ebebeb;
}
.box{
	padding-bottom: 51px;
	background-color: #ebebeb;
	text-align: center;
}
.firm-name{
	border-bottom: 1px solid #e1e1e1;
	text-align: center;
	font-size: 15px;
	padding: 20px 0 17px;
	line-height: 16px;
}
.member-type{
	padding: 8px 8px;
	font-size: 16px;
	line-height: 24px;
	border-bottom: 1px solid #e1e1e1;
	text-align: left;
}
.color-green{
	color: #01ba2e;
	padding-right: 15px;
	width:95px;
	float: left;
	display:block;
	text-align: left;
}
.small-name{
	padding: 19px 8px 9px;
	border-bottom: 1px solid #e1e1e1;
	font-size: 13px;
	line-height: 14px;
}
.firm-img{
	height:200px;
	overflow:hidden;
	text-align:center;
	position: relative;
}
.firm-imgtext{
	position: absolute;
	right: 20px;
	bottom: 10px;
	background-color: #000000;
	width: 40px;
	height: 40px;
	border-radius:20px;
	background: rgba(0, 0, 0, 0.4);
	z-index: 20000;
	color:#fff;
	line-height: 40px;
}
.firm-img img{
	vertical-align: top;
}
.color-red{
	color: #ff0000
}
.tell-num{
	position: relative;
	padding-left: 17px;
}
.tell-num i{
	position: absolute;
	display: block;
	width: 15px;
	height: 15px;
	background: url(../../image/imageslist.png);
	background-size: 250px;
	background-position: -114px -42px;
	left: 0px;
	top: 4px;
}
.firm-do{
	position: fixed;
	bottom: 0px;
	width: 100%;
	border-top: 1px solid #e1e1e1;
	background-color: #fff
}
.firm-do span{
	float: left;
	height: 41px;
	border-bottom: 1px solid #e1e1e1;
	width: 50%;
	line-height: 41px;
	text-align: center;
	border-right: 1px solid #e1e1e1;
	position: relative;
	padding-left: 20px;
}
.firm-do span:last-child{
	border-right: 0
}
.firm-do span i{
	position: absolute;;
	display: block;
	width: 15px;
	height: 15px;
	background: url(../../image/imageslist.png);
	background-size: 250px;
	left: 50%;
	top:14px;
	margin-left: -36px;
}
.firm-do .shouc i{
	background-position: -93px -84px;
}
.firm-do .liuy i{
	background-position: -108px -83px;
}
.firm-do .gongq i{
	background-position: -110px -64px;
}