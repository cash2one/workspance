.surplus{
	display: table;
	width: 100%;
	border-bottom: 1px solid #e1e1e1
}
.surplus-td{
	display: table-cell;
	vertical-align: middle;
	height: 150px;
	text-align: center;
	color: #ff7800
}
.surplus-title{
	font-size: 16px
}
.surplus-moery{
	font-size: 30px;
	margin-top: 23px;
}
.old-buy{
	margin-top: 10px
}
.old-buy div{
	border-bottom: 1px solid #e1e1e1;
}
.day-but{
	float: left;
	width: 50%;
	border-right: 1px solid #e1e1e1;
	padding-left: 8px;
	padding-bottom: 8px
}
.mouth-buy{
	float: left;
	width: 50%;
	padding-left: 8px;
	padding-bottom: 8px
}
.old-buy-tg{
	font-size: 14px;
	color: #959595
}
.old-buy-moery{
	font-size: 15px;
	color: #000
}
.buy-list span{
	float: left;
	width: 50%;
	padding-left: 8px;
	color: #01ba2e;
	font-size: 12px;
	margin: 10px 0
}
.btn-one{
	background:#ff7800;
	border:1px solid #ff7800;
}
.btn-one:active{
	background:#f1c40f;
	border:1px solid #f1c40f;
}
.btn-two{
	background-color: #01ba2e;
	border:1px solid #01ba2e;
}
.btn-two:active{
	background-color: #01BA60;
	border:1px solid #01BA60;
}