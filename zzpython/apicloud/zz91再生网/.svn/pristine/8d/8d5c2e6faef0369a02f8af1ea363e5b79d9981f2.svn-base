.nav-heart{
	position: relative;
}
.nav-heart-ul li{
	float: left;
	text-align: center;
	font-size: 14px;
	height: 30px;
	line-height: 30px;
	border-bottom: 1px solid #e9e8e9;
	color: #333333;
}
.nav-heart-ul .current-nav{
	color: #01ba2e
}
.current-nav-bottom{
	height: 2px;
/*	width: 25%;*/
	background-color: #01ba2e;
	position: absolute;
	bottom: 1px;
	left: 0;
}
.cantop-mation li{
	padding: 17px 8px;
	border-bottom: 1px solid #e9e8e9
}
.mation-title{
	font-size: 16px;
	color: #404040;
	line-height: 20px;
	font-weight:300;
}
.summary{
	color: #959595;
	font-size: 12px;
	line-height: 18px;
}
.mation-pic{
	float: left;
	margin-right: 9px;
}
.go-top{
	color: #fff;
	width: 30px;
	height: 15px;
	text-align: center;
	line-height: 15px;
	background-color: #ff0000;
	float: right;
	font-size: 10px;
	margin-top: 2px;
}
.click-number{
	float: right;
	font-size: 10px;
	color: #b4b4b4;
	padding-left: 14px;
	position: relative;
}
.see-img{
	height: 10px;
	width: 13px;
	position: absolute;
	display: block;
	background-image: url(../../image/imageslist.png);
    background-size:220px 220px;
    background-position: -68px -12px;
    left: 0;
    top: 4px;
}
#navmark { -webkit-transition: 300ms;}


#wrap {
      height: 100%;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-flex-flow: column;
      flex-flow: column;
    }
    #main {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
      flex: 1;
    }
    .footer span{
      width: 50%;
      float: left;
      text-align: center;
      height: 50px;
      border-top: 1px solid #e1e1e1;
      position: relative;
      line-height: 50px;
      padding-top: 10px;
      background-color: #f2f2f2;
      color: #959595
    }
    .footer span i{
      display: block;
      height: 15px;
      width: 15px;
      background-image: url(../../image/imageslist.png);
      background-size: 250px;
      position: absolute;
      left: 50%;
      margin-left: -7px;
      top: 7px;

    }
    .gonew{
      border-right:1px solid #e1e1e1;
    }
    .footer .gonew i{
      background-position: -135px -0px;
    }
    .footer .notinto i{
      background-position: -135px -17px;
    }
    


.aui-nav .aui-bar-tab {
        background: #34495E;
    }
    .aui-nav .aui-bar-tab li .aui-iconfont,
    .aui-nav .aui-bar-tab li p {
        color: #fff;
    }
    .aui-nav .aui-bar-tab li.active .aui-iconfont,
    .aui-nav .aui-bar-tab li.active p {
        color: #14bd7c;
    }
    .aui-nav {
        background: #01AA2A;
        font-size: 18px;
        color: #fff;
        text-align: center;
    }
    .aui-nav  div {
        height: 55px;
    }
    .aui-nav .buy {
        line-height: 26px;
        font-size:14px;
        text-align:left;
        padding-left:10px;
    }
    .aui-nav p {
        color: #fff;
    }
    .aui-nav .aui-iconfont {
        font-size: 20px;
        color: #ffffff;
    }
    .blue {
        background: #eee;
    }
    .blue .aui-iconfont,
    .blue p {
        color: #999;
    }
    .service {
        background: #eec363;
    }