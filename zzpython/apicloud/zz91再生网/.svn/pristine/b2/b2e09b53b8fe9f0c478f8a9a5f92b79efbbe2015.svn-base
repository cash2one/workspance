.top-tell{
	padding: 0px 8px;
	line-height: 25px;
	font-size: 10px;
}
.ftcolor{
	color: #959595
}
.active{
	float: right;
}
.phone{
	position: relative;
	padding-left: 17px;
	float: left;
}
.phone i{
	position: absolute;
	height: 15px;
	width: 15px;
	display: block;
	background-image: url(../image/imageslist.png);
    background-size:320px;
    background-position: -148px -56px;
    left: 0px;
    top: 5px;
}
.phone span{
	color: #ff0000
}
.nav{
	margin: 0px 8px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
}
.nav div:nth-child(2){
	margin:0 8px
}
.nav div{
	text-align: center;
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    background-color: #cccccc;
    font-size: 13px;
}
.nav .now-nav{
	color: #fff;
	background-color: #01ba2e
}
.out-info li{
	overflow: hidden;
	font-size: 12px;
}
.info-detail{
	padding: 17px 8px 8px;
	border-bottom: 1px solid #e1e1e1;
}
.info-div{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
}
.info-div div:nth-child(2){
	margin:0 8px
}
.info-div div{
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    white-space:nowrap; 
	overflow:hidden; 
	text-overflow:ellipsis;
}
.impt-info{
	font-size: 14px;
	color: #01ba2e
}
.handle{
	padding:0 8px;
	border-bottom: 1px solid #e1e1e1
}
.handle span{
	float: left;
	width: 33.33%;
	height: 30px;
	line-height: 30px;
	text-align: center;
	font-size: 14px;
	border-right: 1px solid #e1e1e1
}
.handle span:last-child{
	border-right: 0;
	background-color: #ff9600;
	color: #fff
}
.callphone{
	position: relative;
}
.callphone i{
	position: absolute;
	background-image: url(../image/imageslist.png);
    background-size:320px;
    background-position: -148px -56px;
    left: 50%;
    top: 50%;
    display: block;
    height: 15px;
	width: 15px;
	margin-top: -7px;
	margin-left: -25px;
}