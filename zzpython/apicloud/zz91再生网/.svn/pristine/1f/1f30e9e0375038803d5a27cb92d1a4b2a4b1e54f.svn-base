.box {
	padding: 20px 8px 0;
}
.into {
	display: block;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
	position: relative;
}
.xiala {
	display: block;
	position: absolute;
	height: 35px;
	width: 15px;
	background-color: #f3f3f3;
	top: 7px;
	right: 4px;
	border-radius: 3px;
}
.xiala i {
	display: block;
	position: absolute;
	height: 8px;
	width: 15px;
	background-image: url(../../image/imageslist.png);
	background-position: -123px -55px;
	background-size: 250px;
	top: 50%;
	margin-top: -6px;
}
.tle {
	line-height: 35px;
}
.into-set {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	margin-bottom: 15px;
	border: 1px solid #e1e1e1;
	border-radius: 3px;
	background-color: transparent;
	display: block;
	-webkit-appearance: listbox;
	padding-left: 5px;
}
.btn-xz {
	text-align: center;
	line-height: 30px;
	background-color: #01ba2e;
	color: #fff;
	width: 90px;
	border-radius: 3px;
	margin-left: 8px;
	margin-top: 10px;
	height: 30px;
}
.btn {
	text-align: center;
	line-height: 50px;
	background-color: #01ba2e;
	color: #fff;
	display: block;
	position: fixed;
	bottom: 0px;
	font-size:18px;
	width: 100%;
}
