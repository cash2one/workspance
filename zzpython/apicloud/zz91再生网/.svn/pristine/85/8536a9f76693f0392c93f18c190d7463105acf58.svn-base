select,input[type="text"]{
  padding: 0;
  font-size: 14px;
}
.aui-list-view-cell{
  padding-left: 0px;
  padding-right: 0px
}
.issue-type{
	color: #959595;
  
}
.issue-type:last-child{
  border:0;
}
.issue-type label{
	margin-right: 30px;
	height: 35px;
	line-height: 35px;
}
.issue-title{
	line-height: 35px;
	padding: 0 8px;
	line-height: 31px;
	
	position: relative;
}
.issue-img{
	display: block;
	width: 15px;
	height: 10px;
	float: right;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -160px -69px;
    margin-top: -5px;
    position: absolute;
    right: 0px;
    top: 50%;
    background-color: #fff;
}
.issue-img2{
	right: 8px;
}
.cp-name{
	
  padding: 5px 5px;
}
.cp-name{
  padding: 10px 5px;
}
.cp-name input{
  margin: 0;
  border:0;
}
.cp-name input{
	padding-left: 8px;
	width: 100%;
	line-height: normal;
	height: 35px;
}
.cp-number{
	display: block;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-flex-flow: row;
           flex-flow: row;
}
.cp-number{
	padding: 0px 8px;
	
}
.cp-number input{
  border:0;
  margin: 0
}
.celt-num{
	-webkit-box-flex: 1; 
    -webkit-flex: 1;
    flex: 1;
}
.num-str{
	width: 70px;
	border-left: 1px solid #c8c7cc;
	position: relative;
}
.slet{
	width: 100%;
	border: 0;
	background-color: transparent;
  display: block;
  margin: 0;
  padding: 10px 0
}
.see-price{
	padding: 0px 0px 10px;
	
}
.seedown{
	
}
.seedown label{
	margin-right: 30px;
	height: 35px;
	line-height: 35px;
}
.seedown label input{
	float: left;
	margin-top: 12px;
	margin-right: 5px;
}
.advise{
	font-size: 12px;
	color: #959595;
	line-height: 18px;
  padding: 0 10px;
}
.describe{
	padding: 5px 8px;
	position: relative;

}
.describe textarea{
	width: 100%;
	min-height:79px; 

}
.handle-add{
    border: 1px dashed #c8c7cc;
    width: 80px;
    height: 80px;
    text-align: center;
    line-height: 80px;
    background: #f2f2f2;
    float: left;
    margin: 0 15px 20px 0;
    
  }
  .img-kuang{
    position: relative;
    margin: 0 15px 20px 0;
    float: left;
    border: 1px dashed #c8c7cc;
    width: 80px;
    height: 80px;
  }
  .aui-icon-move{
    text-align: center;
    line-height: 20px;
    color: #fff
  }
  .delt-img{
    position: absolute;
    height: 20px;
    width: 20px;
    right: -10px;
    top: -10px;
    background: red;
    border-radius: 12px;
    box-shadow: 0px 0px 2px red;
  }
  .handle-add .aui-icon-add{
    font-size: 43px;
  }
  .handle-into{
    display: block;
    width: 100%;
    height: 40px;
    background-color: #01ba2e;
    color: #ffffff;
    position: relative;
    position: fixed;
    bottom: 0;
    text-align: center;
    padding-top: 8px;
  }
  .handle-into i{
    position: absolute;
    height: 20px;
    width: 20px;
    background: url(../../image/imageslist.png);
    background-size: 320px;
    background-position: -110px -214px;
    left: 50%;
    margin-left: -40px;
  }