.essay{
	margin: 0 8px;
	border-bottom: 1px solid #ccc;
	padding: 24px 0 15px;
}
.essay-info-list{
	color: #959595;
	line-height: 18px;
	margin-top: 10px;
	padding-right: 5px
}
.essay-title{
	font-size: 15px;
	text-align: center;
}
.essay-time{
	font-size: 10px;
}
.essay-fsize{
	float: right;
	margin-right: 12px;
}
.essay-fsize t{
	padding: 0 5px;
	display: inline-block;
}
.essay-fsize .small-size{
	float: left;
}
.essay-fsize .big-size{
	font-size: 18px;
	float: right;
}
.essay-click{
	float: right;
}
.interfix{
	font-size: 15px;
	line-height: 31px;
	border-bottom: 1px solid #cccccc;
	padding-left: 8px;
}

.info-content{
	padding: 8px 8px;
	font-size: 12px;
	border-bottom: 1px solid #cccccc
}
.info-content-all img{
	width: 100%
}
.info-content-all table{
	width: 100%
}
/*.info-content p{ text-indent: 2em }*/
.handle{
	padding: 10px 8px;
	border-bottom: 1px solid #cccccc;;
}
.share{
	width: 27px;
	height: 27px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    float: left;
    margin-right: 5px;
}
.weixin{ background-position: -115px 1px }
.weikon{ background-position: -115px -25px }
.xinlang{ background-position: -115px -51px }
.qq{ background-position: -115px -77px }
.house{
	float: right;
	height: 27px;
	position: relative;
}
.house-img{
	position: absolute;
	width: 20px;
	height: 20px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -120px -105px;
    left: -22px;
    color: #404040
}