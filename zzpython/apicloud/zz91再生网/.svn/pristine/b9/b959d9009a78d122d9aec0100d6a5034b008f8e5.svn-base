html,body{
	height: 100%;
	width:100%;
}
html, body, button, input, select, textarea {
    color: #404040;
    font: 14px/24px "Microsoft YaHei", "微软雅黑","Oxygen微软雅黑", Arial, "Hiragino Sans GB", "STHeiti", "WenQuanYi Micro Hei", SimSun, sans-serif;
    -webkit-font-smoothing: antialiased;
}
#wrap{
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-flow: column;
           flex-flow: column;
}
#header{
    text-align: center;color: #fff;
    width: 100%;
}
#header h1{
    font-size: 20px; height: 44px; line-height: 44px; margin: 0em; color: #fff;
}
#main{
    -webkit-box-flex: 1; 
    -webkit-flex: 1;
    flex: 1;
    padding: 2em;
}
#footer{
    height: 30px; line-height: 30px;
    background-color: #81a9c3; 
    width: 100%;
    text-align: center;
}
#footer h5{
    color: white;
}
.con{font-size: 28px; text-align: center;}
.clear:after {
    display:block;
    clear:both;
    content:"";
    visibility:hidden;
    height:0
} 
.clear {zoom:1} 

/*header*/
header{
    padding-top: 10px;
    background-color: #01ba2e
}
.mui-bar-nav {
    top: 22;
}
.mui-bar {
    /*position: fixed;*/
    z-index: 10;
    right: 0;
    left: 0;
    height: 48px;
    background-color: #01ba2e;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
a.mui-action-back.mui-icon.mui-icon-left-nav.mui-pull-left {
    background: url(../image/jiantou.png) no-repeat center center;
    background-size: 17px 15px;
    height: 15px;
    width: 17px;
}
.mui-bar-nav.mui-bar .mui-icon {
    padding: 24px 15px;
}
.mui-bar .mui-icon {
    position: relative;
    z-index: 20;
}
.mui-bar-nav a, .mui-title {
    color: #fff;
}
.mui-icon {
    font-family: Muiicons;
    font-size: 24px;
    font-weight: 400;
    font-style: normal;
    line-height: 1;
    display: inline-block;
    text-decoration: none;
    -webkit-font-smoothing: antialiased;
}
.mui-title {
    font-size: 17px;
    font-weight: 500;
    position: absolute;
    display: block;
    width: 100%;
    margin: 0 -10px;
    padding: 0;
    text-align: center;
    white-space: nowrap;
    color: #000;
}
.mui-bar .mui-title {
    right: 40px;
    left: 40px;
    display: inline-block;
    overflow: hidden;
    width: auto;
    margin: 0;
    line-height: 44px;
    text-overflow: ellipsis;
}
.mui-bar-nav a, .mui-title {
    color: #fff;
}

/*点击列表，背景颜色变化 灰色*/
.index-nav-li{
    background-color: #f2f2f2
}

/*按钮点击，背景颜色变化 暗绿*/
.hover-category{
    background-color: #01aa2a
}