<!doctype html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<title></title>
		<link rel="stylesheet" type="text/css" href="../css/api.css"/>
		<link rel="stylesheet" type="text/css" href="../css/common.css"/>
		<link rel="stylesheet" href="../css/win_blank.css" type="text/css">
	</head>
	<body>
		<header>
			<div id="cloud" class="topbar  activebar">
		        <div tapmode="hover-category" class="category" onclick="api.closeWin();"><i></i></div>
		        <div class="select">
		          <span class="select-ipt">点此输入搜索产品</span>
		          <i></i>
		        </div>
		        <div tapmode="hover-category" class="infonumber font-wen"></div>
		        <div tapmode="hover-category" class="infonumber my-gout">
          			<i class="gout-img"></i>
          			<p>发布</p>
        		</div>
        		<div tapmode="hover-category" class="infonumber shaxuan">
          			<i class="shaxuan-mig"></i>
          			<p>筛选</p>
        		</div>
		    </div>
		    <ul class="s_tab clear">
			    <li><i class="s_tab_img1"></i>供应</li>
			    <li><i class="s_tab_img1"></i>全部</li>
			    <li id="choice"><i class="s_tab_img1 s_tab_img2"></i>筛选</li>
			</ul>
		</header>
		<div class="main">
		</div>
		<script type="text/javascript" src="../script/api.js"></script>
		<script type="text/javascript" src="../script/zepto.min.js"></script>
		<script type="text/javascript" src="../script/common.js"></script>
		<script type="text/javascript" src="../script/laytpl.js"></script>
		<script>
			apiready = function() {
				var systemType = api.systemType;
		        if(systemType!="ios"){
		          $("header").css("padding-top","0px")
		        }
		        var type = api.pageParam.type;
    			if(type == "trade-list"){
    				$(".my-gout").show();
    				$(".s_tab").show();
    			}
				var url = api.pageParam.url;
				if(url){
					loadcommonurl(url,api.pageParam);
				}
				$("#choice").on("click",function(){
					var pageParam = {
						wintitle:"筛选",
      					type:"choice",
      					module:"确定"
    				};
    				openWin("choice","choice.html",pageParam);
				})	
				$(".shaxuan").on("click",function(){
					api.openFrame({
			            name: 'city-list',
			            opaque: false,
			            bounces: false,
			            bgColor: 'rgba(0,0,0,0.6)',
			            url: '../html/city-list.html',
			            rect: {
			                x: 0,
			                y: 0,
			                w: 'auto',
			                h: 'auto'
			            }
			        });
				})
				//互助社区发布信息
				$(".my-gout").on("click",function(){
					var pageParam = {
						wintitle:"互助社区发布",
      					type:"issue"
    				};
    				openWin("issue","issue.html",pageParam);
				})
			}
		</script>
	</body>
</html>
