.nav-heart{
	position: relative;
}
.nav-heart-ul li{
	float: left;
	width: 33.33%;
	text-align: center;
	font-size: 14px;
	height: 30px;
	line-height: 30px;
	color: #333333;
	border-bottom: 1px solid #e9e8e9;
}
.nav-heart-ul .current-nav{
	color: #01ba2e
}
.current-nav-bottom{
	height: 2px;
	width: 33.33%;
	background-color: #01ba2e;
	position: absolute;
	bottom: 1px;
	left: 0;
}
.look-list li{
	padding: 10px 8px;
	border-bottom: 1px solid #e1e1e1
}
.look-title{
	font-size: 15px;
}
.look-time{
	color: #959595;
	font-size: 12px;
}
.look-reason{
	color: #ff0000;
	font-size: 12px;
}
.alter{
	float: right;
}