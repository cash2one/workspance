.logo-pic{
  background: url(../image/logo.png) no-repeat;
  height: 126px;
  background-size: 140px 54px;
  background-position: center bottom;
}
.hint{
	text-align: center;
	font-size: 12px;
	color: #999999
}
.input-mo{
	border: 1px solid #cccccc;
	position: relative;
	border-radius: 3px;
	display: block;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-flex-flow: row;
           flex-flow: row;
}
.input-mo t{
	position: absolute;
	right: 5px;
	font-size: 12px;
	color: #999999;
	display: none;
	top: 3px;
}
.dlet{
	display: block;
	background-image: url(../image/imageslist.png);
    background-size:320px;
    background-position: -85px -266px;
    height: 16px;
    width: 16px;
    top: 3px
}
.import-ipt{
	-webkit-box-flex: 1; 
    -webkit-flex: 1;
    flex: 1;
    height: 25px;
    padding-left: 5px;
    font-size: 12px;
}
.login-form-ul{
	padding: 57px;
}
.login-form-ul li{
	margin-bottom: 18px;
}
.input-title{
	font-size: 12px;
	color: #666666;
	line-height: 15px;
	padding-left: 23px;
	padding-right: 8px;
	position: relative;
	border-right: 1px solid #cccccc;
	height: 15px;
	margin-top: 6px;
}
.input-title i{
	background-image: url(../image/imageslist.png);
    background-size:320px;
    display: inline-block;
    width: 15px;
    height: 15px;
    position: absolute;
    left: 3px;
    top: -1px
}
.title-code{
	background-position: -85px -305px;
}
.title-password{
	background-position: -99px -0px;
}
.login-btn{
	height: 25px;
	border-radius: 3px;
	background-color: #01ba2e;
	width: 100%;
	text-align: center;
	color: #fff;
	font-family: "宋体";
	font-size: 14px;
	line-height: 25px;
}
.cannot-login{
	line-height: 12px;
	margin-top: -10px;
}
.forget{
	color: #666666;
	font-size: 12px;
}
.enroll{
	float: right;
	color: #01ba2e;
	padding-right: 18px;
	position: relative;
	font-size: 12px
}
.enroll i{
	position: absolute;;
	height: 13px;
	width: 18px;
	display: block;
	background-image: url(../image/imageslist.png);
    background-size:220px 220px;
    background-position: -54px -195px;
    right: 0;
    top: 0
}
.login-btn-click{
	background-color: #01aa2a
}