.type-small span{
	width: 33.333%;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	float: left;
	border-bottom: 1px solid #e1e1e1;
	border-right: 1px solid #e1e1e1;
	border-collapse: collapse;
}
.type-small .two-span{
	width: 66.666%;
}
.type-small{
	border-top: 1px solid #e1e1e1;
}
.type-small span:last-child{
	border-right: 0;
}
.type-small .take-now-span{
	border: 1px solid #01ba2e;
	color: #01ba2e;
}
.type-small li{
	overflow: hidden;
	text-align: center;
}
.price-type {
	position: relative;
	height: 38px;
	line-height: 38px;
	background-color: #f2f2f2;
	padding-left: 39px;
}
.price-type i{
	display: block;
	height: 29px;
	width: 29px;
	position: absolute;
	background: url(../image/imageslist.png);
	background-size: 220px;
	left: 8px;
	top: 5px;
}
.price-type .type-img1{
	background-position: -1px -4px;
}
.price-type .type-img2{
	background-position: -1px -34px;
}
.price-type .type-img3{
	background-size: 320px;
	background-position: -115px -188px;
}
.btn-td{
	padding: 20px 8px;
}
.btn{
	display: block;	
	line-height: 30px;
	background-color: #01ba2e;
	font-size: 15px;
	text-align: center;
	color: #fff;
	border-radius: 5px;
}