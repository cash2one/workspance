.my-info-table{
	display: table;
	width: 100%
}
.my-info{
	height: 85px;
	background-color: #01ba2e;
	display: table-cell;
	width: 100%;
	vertical-align: middle;
	text-align: center;
}
.login{
	background-color: #fff;
	width: 100px;
	text-align: center;
	font-size: 13px;
	color: #01ba2e;
	padding: 5px 0
}
.my-fit li{
	border-bottom: 1px solid #e1e1e1;
	height: 46px;
	line-height: 36px;
	padding:5px 0px 5px 45px;
	font-size: 16px;
}
.my-fit li span{
	position: relative;
}
.my-fit li i{
	position: absolute;
	height: 20px;
	width: 20px;
	background-image: url(../../image/imageslist.png);
  	background-size: 320px;
  	display: block;
  	left: -30px;
  	top: 9px;
}
.my-shouc i{
	background-position: -155px -202px;
}
.my-infoout i{
	background-position: -155px -221px;
}
.my-buyin i{
	background-position: -155px -242px;
}
.my-block i{
	background-position: -155px -265px;
}
.my-tell i{
	background-position: -155px -286px;
}
.my-yaoqi i{
	background-position: -134px -237px;
}
.my-revise i{
	background-position: -134px -255px;
}
.my-password i{
	background-position: -134px -274px;
}