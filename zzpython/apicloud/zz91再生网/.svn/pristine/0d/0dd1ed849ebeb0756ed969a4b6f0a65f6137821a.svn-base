<script type="text/javascript">
	function closeselect(){
		api.execScript({
			name : 'company-list',
			script : "keybackFun('1')"
		});
		api.closeFrame();
	}
	apiready = function(){
		var $body = $api.dom('body');
		$api.addEvt($body, 'click', function(e){
	        if(e.target == $body){
	            closeselect();
	        }else{
	            e.stopPropagation();
	        }
	    });
	    $(".small-area span").on("click",function(){
	    	closeselect();
			api.execScript({
				name : 'company-list',
				frameName : 'company-list_',
				script : "searcharea('"+$(this).text()+"')"
			});
		})
	};
