html, body {
    height: 100%;
    width: 100%;
}

#wrap {
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-flow: column;
    flex-flow: column;
}

#main {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

#footer {
    width: 100%;
    text-align: center;
    height: 56px;
    line-height: 55px;
    background-color: #fcfcfc;
    border-top: 1px solid #ebebeb
}

ul {

    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}



#footer li {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    height: 55px;
}
.bottom_btn{
    line-height: 12px;
    font-size: 12px;
}
.bottom_btn span{
    display: block;
}
.bottom_btn i{
    background-position-y: 4px;
    padding-top: 14px;
    margin-top: 2px;
    margin-bottom: 2px;
    font-size: 12px;
    color: #323237;
    width: 33px;
    height: 29px;
    background-repeat: no-repeat no-repeat;
    background-position-x: center;
    background-size: 320px;
    background-image: url(../image/imageslist.png);
    display: inline-block;
}
.cloud i{
    background-position: -81px -33px;
}

.client i{
    background-position: -214px -195px;
}

.dev i{
    background-position: -178px -195px;
}

.doc i{
    background-position: -80px -116px;
}
.active{
    color: #01ba2e
}
.active .cloud i{
    background-position: -81px -148px;
}

.active .client i{
    background-position: -214px -224px;
}

.active .dev i{
    background-position: -178px -221px;
}

.active .doc i{
    background-position: -79px -236px;
}

.active .bottom_btn i{
    color: #6ab494;
}


footer{
	border-top: 1px solid #ebebeb;
	background-color: #fcfcfc;
}
.aui-bar-tab{
	background-color: #fcfcfc;
}
.aui-nav .aui-bar-tab .active-warning .aui-iconfont, .aui-nav .aui-bar-tab .active-warning p{
	color: #01ba2e
}
.aui-nav .aui-bar-tab .aui-iconfont{
	font-size:28px;
}
.aui-bar-tab li{
	line-height:20px;
}
.aui-nav .aui-bar-tab p{
	font-size:12px;
}

