<script>
  apiready = function () {
      //资讯中心页面
      $(".infomation-heart").on("click",function(){
        var pageParam = {
          wintitle:"资讯中心",
          type:"infomation-heart",
          nav_list:["行业资讯", "评论预测", "技术文库", "政策法规", "2行业资讯", "2评论预测", "2技术文库", "2政策法规"],
          frame_url:["../html/infolist.html"],
          topnumber:4
        };
        openWin("infomation-heart","infomation-heart.html",pageParam);
      })
      //互动社区
      $(".community").on("click",function(){
        var pageParam = {
          type:"community",
          module:"发布",      //头部右边文字
          nav_list:["问答", "社区", "学院", "热门"],
          frame_url:["../html/hudong-list.html"],
          topnumber:4    //滚动Nav栏数码
        };
        openWin("community","infomation-heart.html",pageParam);
      })
      //行情报价
      $(".offer-list").on("click",function(){
        var pageParam = {
          type:"offer-list"
        };
        openWin("offer-list","offer-list.html",pageParam);
      })
  }
</script>
