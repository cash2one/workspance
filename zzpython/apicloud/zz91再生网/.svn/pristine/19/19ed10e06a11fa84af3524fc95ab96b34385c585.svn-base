/*index-nav clear*/
.index-nav-div{
	padding-top: 12px;
}
.index-nav{
	border-top:1px solid #f6f6f6;
}
.index-nav li:nth-child(4),
.index-nav li:last-child{
	border-right:0;
}
.index-nav li{
	height: 75px;
	width: 25%;
	float: left;
	padding-top: 8px;
	text-align: center;
	font-size: 14px;
	color: #4a4a4a;
	border-bottom:1px solid #f6f6f6;
	border-right:1px solid #f6f6f6;
}
.index-nav li i{
	margin: 0 auto;
	display: block;
	width: 34px;
	height: 34px;
	background-image: url(../image/imageslist.png);
	margin-bottom: 5px;
}

.nav_image1{
	background-position: -90px -72px;
}
.nav_image2{
	background-position: -90px -106px;
}
.nav_image3{
	background-position: -90px -151px;
}
.nav_image4{
	background-position: -90px -197px;
}
.nav_image5{
	background-position: -90px -236px;
}
.nav_image6{
	background-position: -90px -275px;
}
.nav_image7{
	background-position: -90px -314px;
}
.nav_image8{
	background-position: -90px -357px;
}

/*hot-products*/
.hot-title{
