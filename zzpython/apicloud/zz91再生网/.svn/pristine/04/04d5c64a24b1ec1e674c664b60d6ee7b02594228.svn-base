.border-bottom{
	border-bottom: 1px solid #e1e1e1
}
.hot-title{
	padding: 22px 8px 13px;
}
.hot-title h4{
	float: left;
	padding-left: 5px;
	border-left: 3px solid #ea5f64;
	line-height: 14px;
	font-size: 14px;
}
.headle{
	float: right;
	color: #959595;
	font-size:11px;
	line-height: 12px;
}
.headle span{
	margin-left: 10px;
}
.old-metal{
	padding: 0px 0px;
}
.old-metal .on{
	background-color: #ebebeb;
}
.old-metal li{
	border-bottom: 1px dotted #e1e1e1;
	font-size: 14px;
	line-height: 40px;
	overflow:hidden;
	white-space:nowrap;text-overflow:ellipsis;
	padding-left: 8px;
	padding-right: 8px;
}
.old-type{
	color: #959595
}
.hot-title .no-right{
	border-left: 0;
	padding-left: 0;
}
.hot-title-sck{
	padding: 0px 8px;
	margin: 10px 0;
}
.qh-table{
	margin: 0px 8px;
	border-top: 1px solid #e1e1e1;
	border-left: 1px solid #e1e1e1;
}
.qh-table span{
	width: 33.33%;
	border-bottom: 1px solid #e1e1e1;
	border-right: 1px solid #e1e1e1;
	line-height: 30px;
	text-align: center;
}
.go-down{
	color: #01ba2e
}
.go-up{
	color: #ff0000
}