<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <title>发布需求</title>
    <link rel="stylesheet" type="text/css" href="../../css/api.css"/>
    <link rel="stylesheet" type="text/css" href="../../css/common.css"/>
    <link rel="stylesheet" type="text/css" href="../../css/trade/need-out.css"/>
</head>
<body>
  <div class="box">
    <div class="into">
      <span class="tle">供 求：</span>
      <select class="into-set"><option>全部</option></select>
      <span class="xiala"><i></i></span>
    </div>
    <div class="into">
      <span class="tle">类 别：</span>
      <div class="get-xinx">
        <span>废铜</span>
      </div>
      <span class="btn-xz j_type">选择</span>
    </div>
    <div class="into">
      <span class="tle">地 区：</span>
      <div class="get-xinx">
          <span>杭州</span>
          <span>上海</span>
      </div>
      <span class="btn-xz j_city">选择</span>
    </div>
    <div class="into">
      <span class="tle">时 间：</span>
      <select class="into-set"><option>三天内</option></select>
      <span class="xiala"><i></i></span>
    </div>
    <span class="btn">确定</span>
  </div>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/zepto.min.js"></script>
<script type="text/javascript" src="../../script/common.js"></script>
<script type="text/javascript" src="../../script/laytpl.js"></script>
<script>
apiready = function(){
  //选择类型
  $(".j_type").on("click",function(){
    var pageParam = {
          wintitle:"发布需求",
          type:"need-type",
      };
      openWin("need-type","../trade/need-type.html",pageParam);
  })
  //选择城市
  $(".j_city").on("click",function(){
     var pageParam = {
        wintitle:"地区列表",
        type:"ulcitylist",
      };
      openWin("ulcitylist","../trade/ulcitylist.html",pageParam);
  })
}
</script>
</html>