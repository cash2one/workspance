.div-wt{
	position: fixed;
	height: 50px;
	width: 50px;
	background: #01ba2e;
	border-radius: 50px;
	bottom:30px;
	right: 20px;
	text-align: center;
	line-height: 50px;
	box-shadow: 0px 0px 5px #888888;
}
.div-wt .aui-icon-writefill{
	font-size: 24px;
	color: #fff	
}
.essay{
	border-bottom: 1px solid #ccc;
	padding: 24px 8px 15px;
}
.essay-info-list{
	color: #959595;
	line-height: 18px;
	margin-top: 10px;
	padding-right: 5px
}
.essay-title{
	font-size: 15px;
	text-align: center;
}
.essay-time{
	font-size: 10px;
}
.essay-time span:first-child{
	padding-right:10px;
}
.essay-time span:last-child{
	padding-left:10px;
}
.essay-fsize{
	float: right;
	margin-right: 20px;
}
.essay-fsize t{
	padding: 0 5px;
	display: inline-block;
}
.essay-fsize .small-size{
	float: left;
}
.essay-fsize .big-size{
	font-size: 18px;
	float: right;
}
.click-number{
	float: right;
	position: relative;
}
.click-number .see-img{
	display: block;
	width: 15px;
	height: 18px;
	position: absolute;
	left: -18px;
	background: url(../../image/imageslist.png) -130px -154px;
	background-size: 320px;
}
.essay-click{
	float: right;
}
.interfix{
	
	line-height: 31px;
	border-bottom: 1px solid #cccccc;
	padding-left: 8px;
}
.interfix h3{
	font-size: 15px;
	float: left;
}
.sort{
	float: right;
	font-size: 11px;
	color: #959595;
	margin-right: 8px;
	position: relative;
}
.sort-img{
	position: absolute;
	width: 15px;
	height: 15px;
	display: block;
	background: url(../../image/imageslist.png) -115px -154px;
	background-size: 320px;
	left: -17px;
	top: 5px;
}
.info-content{
	padding: 8px 8px;
	font-size: 14px;
	background-color: #f2f2f2
}
.info-content p{ text-indent: 2em }
.handle{
	padding: 10px 8px;
	border-bottom: 1px solid #cccccc;
	background-color: #f2f2f2
}
.share{
	width: 27px;
	height: 27px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    float: left;
    margin-right: 5px;
}
.weixin{ background-position: -115px 1px }
.weikon{ background-position: -115px -25px }
.xinlang{ background-position: -115px -51px }
.qq{ background-position: -115px -77px }
.house{
	float: right;
	height: 27px;
	position: relative;
}
.house-img{
	position: absolute;
	width: 20px;
	height: 20px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -120px -105px;
    left: -22px;
    color: #404040
}
.assess-name{
	font-size: 14px;
	color: #00a2ff;
}
.assess-tip{
	float: right;
	font-size: 12px;
	color: #ff0000
}
.assess-li{
	padding: 8px 8px;
	border-bottom: 1px solid #e1e1e1
}
.assess-mos{
	font-size: 14px;
	color: #404040
}
.assess-time{
	font-size: 10px;
	color:#959595;
}
.assess-answer{
	float: right;
	color: #959595;
	position: relative;
	font-size: 10px;
}
.assess-answer i{
	position: absolute;
	width: 20px;
	height: 15px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -131px -157px;
    left: -17px;
    top: 6px;
    color: #404040
}
.answer-list{
	padding: 4px 8px;
	background-size: #f2f2f2;
	position: relative;
}
.answer-list li:first-child{
	border-top: 1px solid #e1e1e1
}
.answer-list li{
	padding: 0px 8px;
	border-color: #e1e1e1;
	border-width:0 1px 1px 1px;
	border-style: solid;
	background-color: #f6f6f6;
}
.hf-more{
	text-align: center;
	line-height: 30px;
	color: #00a2ff
}
.tape-ask{
	padding: 0 10px;
	color: #959595
}
.sanjiao{
	height: 19px;
	width: 25px;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -114px -174px;
    position: absolute;
    top: -10px;
}