.box{
	padding: 15px 0
}
.into-inpt{
	padding: 0 8px;
}
.into-inpt{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
    line-height: 30px;
    margin-top: 10px;
}
.into-inpt span{
	width: 73px;
	margin-right: 8px;
}
.into-inpt input{
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    border:1px solid #e1e1e1;
    border-radius: 3px;
    padding-left: 5px;
}
.btn{
	display: block;
	margin: 0 8px;
	height: 30px;
	text-align: center;
	line-height: 30px;
	color: #fff;
	background-color: #01ba2e;
	border-radius: 5px;
}
.tishi{
	padding: 10px 8px 0px;
}
.into-tare{
	height: 70px;
	border-radius: 5px;
	border: 1px solid #e1e1e1;
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    padding-left: 5px;
}
.redfont{
	color: #ff0000
}
.flow{
	padding: 10px 10px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
}
.flow-span{
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    font-size: 12px;
    text-align: center;
    color: #01ba2e;
    border:1px solid #e1e1e1;
    height: 20px;
    line-height: 20px;
}
.flow span:nth-child(1){
	border-right:0;
}
.flow span:nth-child(3){
	border-right:0;
	border-left:0;
}
.flow span:nth-child(5){
	border-left:0;
}
.jitu{
	height: 20px;
	width: 16px;
	background-image: url(../image/imageslist.png);
    background-size:250px;
    background-position: -143px -92px;
}