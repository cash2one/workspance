.my-info-table{
	display: table;
	width: 100%
}
.my-info{
	height: 85px;
	background-color: #01ba2e;
	display: table-cell;
	width: 100%;
	vertical-align: middle;
	text-align: center;
}
.login{
	background-color: #fff;
	width: 80px;
	text-align: center;
	font-size: 13px;
	color: #01ba2e
}
.my-fit li{
	border-bottom: 1px solid #e1e1e1;
	height: 41px;
	line-height: 31px;
	padding:5px 0px 5px 33px;
}
.my-fit li span{
	position: relative;
}
.my-fit li i{
	position: absolute;
	height: 15px;
	width: 15px;
	background-image: url(../image/imageslist.png);
  	background-size: 250px;
  	display: block;
  	left: -22px;
  	top: 9px;
}
.my-shouc i{
	background-position: -122px -158px;
}
.my-infoout i{
	background-position: -122px -174px;
}
.my-buyin i{
	background-position: -122px -190px;
}
.my-block i{
