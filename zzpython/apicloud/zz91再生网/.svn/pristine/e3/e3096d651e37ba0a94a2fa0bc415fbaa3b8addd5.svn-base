.logo-pic{
  background: url(../../image/logo.png) no-repeat;
  height: 100px;
  background-size: 140px 54px;
  background-position: center bottom;
}
.hint-ts{
	text-align: center;
	font-size: 12px;
	color: #999999
}
.input-mo{
	border: 1px solid #cccccc;
	position: relative;
	border-radius: 3px;
	display: block;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-flex-flow: row;
           flex-flow: row;
}
.input-mo t{
	position: absolute;
	right: 5px;
	font-size: 14px;
	color: #999999;
	display: none;
	top: 50%;
	margin-top:-10px;
	line-height: 20px;
}
.dlet{
	display: block;
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    background-position: -86px -265px;
    height: 19px;
    width: 20px;
    
}
.import-ipt{
	-webkit-box-flex: 1; 
    -webkit-flex: 1;
    flex: 1;
    height: 34px;
    padding-left: 5px;
    font-size: 16px;
}
.login-form-ul{
	padding: 45px 25px;
}
.login-form-ul li{
	margin-bottom: 18px;
}
.input-title{
	font-size: 16px;
	color: #666666;
	line-height: 22px;
	padding-left: 23px;
	padding-right: 8px;
	position: relative;
	border-right: 1px solid #cccccc;
	height: 22px;
	margin-top: 6px;
}
.input-title i{
	background-image: url(../../image/imageslist.png);
    background-size:320px;
    display: inline-block;
    width: 15px;
    height: 15px;
    position: absolute;
    left: 3px;
    top: 4px
}
.title-code{
	background-position: -85px -305px;
}
.title-password{
	background-position: -99px -0px;
}
.btn{
	height: 36px;
	border-radius: 3px;
	background-color: #ccc;
	width: 100%;
	text-align: center;
	color: #fff;
	font-family: "宋体";
	font-size: 14px;
	line-height: 36px;
}
.login-btn{
	background-color: #01ba2e;
}
.cannot-login{
	line-height: 20px;
	margin-top: -10px;
}
.forget{
	color: #666666;
	font-size: 16px;
}
.enroll{
	float: right;
	color: #01ba2e;
	padding-right: 18px;
	position: relative;
	font-size: 16px
}
.enroll i{
	position: absolute;;
	height: 13px;
	width: 18px;
	display: block;
	background-image: url(../../image/imageslist.png);
    background-size:220px 220px;
    background-position: -54px -195px;
    right: 0;
    top: 4px;
}
.login-btn-click{
	background-color: #01aa2a
}

/* 底部样式 */
.bottom {/*position: absolute;bottom: 0;*/width: 100%; text-align: center;/*padding-bottom: 20px;*/font-size: 12px;}
.bottom img {height: 50px;}
.bottom .arrow {text-align: center;margin: 10px 0;}
.bottom .arrow img {width: 40px;height: 20px;}
#qqpic {margin-right: 10px; margin-left: 10px;}
.thirdinfo {color: #666;margin-top: 10px;}


.thirdcompy .left {margin-right: 20px;}
.thirdcompy .left , .thirdcompy .right {width: 40%; display: inline-block;background: #fff;border: 1px solid #e0e0e0;border-radius: 4px;vertical-align: top;text-align: left; font-size: 18px;}
.thirdcompy .left img , .thirdcompy .right img {width: 30px;height: 30px;float: left;margin: 5px 10px;}
.thirdcompy span {line-height: 40px;color: #666;}