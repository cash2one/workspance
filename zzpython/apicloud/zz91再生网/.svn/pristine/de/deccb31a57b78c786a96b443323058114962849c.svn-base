<!doctype html>
<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
	<title>供求详情-图片</title>
	<link rel="stylesheet" type="text/css" href="../../css/api.css"/>
	<link rel="stylesheet" type="text/css" href="../../css/common.css"/>
	<style>
	header{
		position: fixed;
		top: 0;
		width: 100%
	}
	.glv{
		height: 12px;
		background-color: #01ba2e
	}
	.imglist{
		margin-top: 22px;
	}
	.imglist img{
		vertical-align: top;
		width: 100%;
		border-bottom: 2px solid #fff
	}
	.top-black{
		position: fixed;
		height: 20px;
		width: 20px;
		display: block;
		background: url(../../image/imageslist.png);
		background-size: 250px;
		background-position: -114px -101px;
		top: 30px;
		left: 8px;
	}
	.bottom-black{
		position: fixed;
		height: 33px;
		width: 33px;
		display: block;
		background: url(../../image/imageslist.png);
		background-size: 250px;
		background-position: -115px -123px;
		bottom: 20px;
		left: 50%;
		margin-left: -16px;
	}
	</style>
</head>
<body>
	<header><div class="glv"></div></header>
	<i class="top-black"></i>
	<i class="bottom-black"></i>
    <div class="imglist">
    	<img src="../../image/infoimage.png">
    	<img src="../../image/infoimage.png">
    	<img src="../../image/infoimage.png">
    	<img src="../../image/infoimage.png">
    </div>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/jquery.js"></script>
<script type="text/javascript" src="../../script/zepto.min.js"></script>
<script type="text/javascript" src="../../script/common.js"></script>
<script type="text/javascript" src="../../script/laytpl.js"></script>
<script type="text/javascript">
	apiready = function() {
		$(".bottom-black").on("click",function(){
			api.closeWin({
			    name: 'imglist',
			    animation: {
			        type: 'reveal',
			        subType: 'from_bottom'
			    }
			});
		})
		$(".top-black").on("click",function(){
			api.closeWin({
			    name: 'imglist',
			    animation: {
			        type: 'reveal',
			        subType: 'from_left'
			    }
			});
		})
	}
</script>
</html>