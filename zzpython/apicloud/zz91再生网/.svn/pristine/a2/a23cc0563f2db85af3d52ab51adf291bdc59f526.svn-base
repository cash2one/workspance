.type-small div{
	width: 50%;
	height: 41px;
	line-height: 41px;
	font-size: 14px;
	float: left;
	border-bottom: 1px solid #e1e1e1;
	border-right: 1px solid #e1e1e1;
	border-collapse: collapse;
	
}
.type-small div span{
	position: relative;
}
.type-small .two-span{
	width: 100%;
}
.type-small{
	border-top: 1px solid #e1e1e1;
}
.type-small div i{
	display: block;
	position: absolute;
	width: 13px;
	height: 13px;
	background:url(../../image/imageslist.png);
	background-size: 250px;
	background-position: -125px -64px;
	top: 50%;
	margin-top: -6px;
	left: -15px;
	display: none;
}
.type-small div:last-child{
	border-right: 0;
}
.type-small .take-now-span{
	background-color:#fff;
	color: #01ba2e;
}
.type-small .take-now-span  i{
	display: block;
}
.type-small li{
	overflow: hidden;
	text-align: center;
}
.price-type {
	position: relative;
	height: 38px;
	line-height: 38px;
	background-color: #f2f2f2;
	padding-left: 39px;
}
.price-type i{
	display: block;
	height: 29px;
	width: 29px;
	position: absolute;
	background: url(../../image/imageslist.png);
	background-size: 220px;
	left: 8px;
	top: 5px;
}
.price-type .type-img1{
	background-position: -1px -4px;
}
.price-type .type-img2{
	background-position: -1px -34px;
}
.price-type .type-img3{
	background-size: 320px;
	background-position: -115px -188px;
}
.btn-td{
	padding: 20px 8px;
}
.btn{
	display: block;	
	line-height: 30px;
	background-color: #01ba2e;
	font-size: 15px;
	text-align: center;
	color: #fff;
	border-radius: 5px;
}