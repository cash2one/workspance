    <script type='text/javascript' src='../script/api.js'></script>
    <script type='text/javascript'>
    function openFileBrowser()
    {
    var iaf = api.require('fileBrowser');
    iaf.open({},
             function(ret,err)
             {
             iaf.close();
             document.getElementById("title").value = ret.url;
             api.alert(
                      {
                      title: '本文件url',
                      msg: ret.url,
                      buttons: ['确定']
                      });
             });
    };
    </script>
