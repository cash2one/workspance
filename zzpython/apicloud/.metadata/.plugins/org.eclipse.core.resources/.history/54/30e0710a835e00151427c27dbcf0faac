    <script type='text/javascript'>
    function openimageAry()
    {
        var array = new Array();
		for ( var i = 0; i < 22; i++) {
			array[i] = document.getElementById('imageUrl_' + 18).value;
		}
        var iaf = api.require('imageBrowser');
		iaf.openImages({imageUrls: array,showList:true,activeIndex:"2"});

    };
    </script>
