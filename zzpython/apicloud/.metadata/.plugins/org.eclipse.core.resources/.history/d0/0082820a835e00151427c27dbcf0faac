            <div id="main">
                <div class="list">
                    <ul>
                        <li><a tapmode="active" onclick="openHTML(this)" name="baiduLocation" data-url="./baiduLocation.html">baiduLocation</a></li>
                    </ul>
                </div>
            </div>
            <div id="footer">
            </div>
