    <script type="text/javascript" src="../script/api.js"></script>
    <script type="text/javascript">
        function openHTML(t){
            var name = t.name;
            var url = $api.attr(t, 'data-url');

            if(name && url){
                api.openWin({
                    name: name,
                    url: url
                });
            }
        }

    </script>
