    <script type='text/javascript' src='../script/api.js'></script>
    <script type='text/javascript'>
    apiready = function() {
        fullScreen = api.fullScreen;
    }
    function change(){
        fullScreen = !fullScreen;
    	api.setFullScreen({
    	    fullScreen:fullScreen
        });
    };
    function changeLight(){
        api.setStatusBarStyle({
            style: 'light'
        });
    };
    function changeDark(){
        api.setStatusBarStyle({
            style: 'dark'
        });
    };
    </script>
