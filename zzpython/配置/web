		expires 3d;
	}
	
	access_log  /mnt/var/log/nginx/feitong.zz91.access.log;
	include /etc/nginx/err.conf;
}

server {
	listen   80; ## listen for ipv4; this line is default and implied
	index index.html index.htm;
	server_name caigou.zz91.com;
	location / {
		proxy_next_upstream http_502 http_504;
		proxy_pass http://tradeServer/trust/;
		include /etc/nginx/proxy.conf;
		expires -1;
	}

	
	access_log  /mnt/var/log/nginx/caigou.zz91.access.log;
	include /etc/nginx/err.conf;
}


server {
	listen   80; ## listen for ipv4; this line is default and implied
	index index.html index.htm;
	server_name jinshu.zz91.com;
	include /etc/nginx/deny.conf;
	location / {	
		rewrite ^/(.*)$ http://www.zz91.com/jinshu/$1 permanent;
		expires 30d;
   	}
	
	access_log  /mnt/var/log/nginx/jinshu.zz91.access.log;
	include /etc/nginx/err.conf;
}


