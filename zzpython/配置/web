		set $domain $1;
	}
	#rewrite /products([0-9]+)(.*).htm http://trade.zz91.com/productdetails$1.htm permanent;
	location / {
		if ($http_user_agent ~* (UCWEB|iPhone|Opera\sMini|MIDP-[0-9]|MIDP[0-9]|SymbianOS|Symbian\sOS|Windows\sCE|BlackBerry|^SonyEricsson|^Nokia|^NOKIA|UP\.Link|^SAMSUNG|^MOT-|^DoCoMo|UP\.Browser|^Mitsu|MAUI|^Palm|^Blazer|^BIRD\.E868)) {
			
			rewrite ^/(.*)$ http://$domain.m.zz91.com/$1 permanent;
		}
        proxy_pass http://esiteServer/;
        include /etc/nginx/proxy.conf;
		expires 1h;
    }
	
	
