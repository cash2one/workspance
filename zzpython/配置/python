upstream searchServer {
    #server 7006.zz91server.com:8000;
	#server 7009.zz91server.com:8000;
	#server 7012.zz91server.com:8000;
	server 100.98.112.169:816;
}
server {
	listen 80;
	index index.htm;
	server_name s.zz91.com ss.zz91.com sss.zz91.com;
	include /etc/nginx/deny.conf;
	location / {
		rewrite ^/trade/(.*)$ http://trade.zz91.com/trade/$1 permanent;
        proxy_pass http://searchServer/;
		include /etc/nginx/proxy.conf;
		expires 24h;
    }
	include /etc/nginx/err.conf;
}

