upstream mobileServer {
	ip_hash;
	server 100.98.112.169:805;
	#server 7006.zz91server.com:805;
	#server 7012.zz91server.com:805;
}
server {
    listen 80;
    server_name m.zz91.com;
    access_log  /mnt/var/log/nginx/m.zz91.access.log;
	include /etc/nginx/deny.conf;
	if ($http_user_agent ~* "qihoobot|Baiduspider|Googlebot|Googlebot-Mobile|Googlebot-Image|Mediapartners-Google|Adsbot-Google|Feedfetcher-Google|Yahoo! Slurp|Yahoo! Slurp China|YoudaoBot|Sosospider|Sogou spider|Sogou web spider|MSNBot|ia_archiver|Tomato Bot") {  
		#return 403;  
	}
    location / {
		rewrite /caigou/ http://m.zz91.com/trust/ permanent;
		proxy_pass http://mobileServer/;
		include /etc/nginx/proxy.conf;
		expires -1;
    }
	location /jiage/ {
		proxy_pass http://mobileServer/jiage/;
		include /etc/nginx/proxy.conf;
		expires 30m;
    }
	location /company/ {
		proxy_pass http://mobileServer/company/;
		include /etc/nginx/proxy.conf;
		expires 30m;
    }
	location /huzhu/ {
		proxy_pass http://mobileServer/huzhu/;
		include /etc/nginx/proxy.conf;
		expires 30m;
    }
	location /trust/ {
		proxy_pass http://mobileServer/trust/;
		include /etc/nginx/proxy.conf;
		expires 30m;
    }
	location /trade/ {
		proxy_pass http://mobileServer/trade/;
		include /etc/nginx/proxy.conf;
		expires 30m;
    }
	location /images/ {
		proxy_pass http://mobileServer/images/;
		include /etc/nginx/proxy.conf;
		expires 1d;
    }
	location /js/ {
		proxy_pass http://mobileServer/js/;
		include /etc/nginx/proxy.conf;
		expires 1d;
    }
	location /css/ {
		proxy_pass http://mobileServer/css/;
		include /etc/nginx/proxy.conf;
		expires 1d;
    }
	location /choujiang/ {
		proxy_pass http://mobileServer/choujiang/;
		include /etc/nginx/proxy.conf;
		expires 1d;
    }
	location /registerSave/ {
                proxy_pass http://mobileServer/registerSave/;
                include /etc/nginx/proxy.conf;
                expires -1;
    }
	
	include /etc/nginx/proxy.conf;
	error_page   500 502 503 504 404 /mobile.html;
	location = /mobile.html {
		root   /var/www/error/;
	}
	location /2016zadanplay/ {
                proxy_pass http://mobileServer/2016zadanplay/;
                include /etc/nginx/proxy.conf;
                expires 1d;
    }
}

