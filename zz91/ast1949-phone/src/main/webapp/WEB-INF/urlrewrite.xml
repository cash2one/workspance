<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

	<rule>
		<from>^/index([0-9]+).htm$</from>
		<to>/index.htm?companyId=$1</to>
	</rule>
	
	<rule>
		<from>^/gsdt([0-9]+).htm$</from>
		<to>/gsdt.htm?companyId=$1</to>
	</rule>
	
	<rule>
		<from>^/gsjj([0-9]+).htm$</from>
		<to>/gsjj.htm?companyId=$1</to>
	</rule>
	
	<rule>
		<from>^/newsdetail([0-9]+).htm$</from>
		<to>/newsdetail.htm?id=$1</to>
	</rule>
	
	<rule>
		<from>^/zxgq([0-9]+).htm$</from>
		<to>/zxgq.htm?companyId=$1</to>
	</rule>
	
	<rule>
		<from>^/productdetail([0-9]+).htm$</from>
		<to>/productdetail.htm?id=$1</to>
	</rule>

</urlrewrite>