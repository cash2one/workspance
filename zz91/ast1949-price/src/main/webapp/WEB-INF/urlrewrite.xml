<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!--频道-->
	<rule>
		<from>^/channel/wasteMetal.htm$</from>
		<to>/channel/index.htm?id=1</to>
	</rule>
	<rule>
		<from>^/channel/wastePlastic.htm$</from>
		<to>/channel/index.htm?id=2</to>
	</rule>
	<rule>
		<from>^/channel/otherWaste.htm$</from>
		<to>/channel/index.htm?id=3</to>
	</rule>
	<!--报价-->
	<rule>
		<from>^/priceList_a(\d+)_([a-z]+).htm$</from>
		<to>/priceList.htm?assistTypeId=$1&amp;code=$2</to>
	</rule>
	<rule>
		<from>^/priceList_p(\d+)_([a-z]+).htm$</from>
		<to>/priceList.htm?parentId=$1&amp;code=$2</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_a(\d+)_([a-z]+).htm$</from>
		<to>/priceList.htm?id=$1&amp;assistTypeId=$2&amp;code=$3</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_a(\d+)_([a-z]+)_p(\d+).htm$</from>
		<to>/priceList.htm?id=$1&amp;assistTypeId=$2&amp;code=$3&amp;page=$4</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_c(\d+)_([a-z]+).htm$</from>
		<to>/priceList.htm?id=$1&amp;categoryId=$2&amp;code=$3</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_c(\d+)_([a-z]+)_p(\d+).htm$</from>
		<to>/priceList.htm?id=$1&amp;categoryId=$2&amp;code=$3&amp;page=$4</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_([a-z]+).htm$</from>
		<to>/priceList.htm?id=$1&amp;code=$2</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d+)_([a-z]+)_p(\d+).htm$</from>
		<to>/priceList.htm?id=$1&amp;code=$2&amp;page=$3</to>
	</rule>
	<rule>
		<from>^/priceList_t(\d*)_p(\d*)_a(\d*)_c(\d*)_([a-z]*)--s(\d*)--l(\d*).htm$</from>
		<to>/priceList.htm?id=$1&amp;parentId=$2&amp;assistTypeId=$3&amp;categoryId=$4&amp;code=$5&amp;startIndex=$6&amp;pageSize=$7</to>
	</rule>

	<rule>
		<from>^/moreList_p(\d+)_t(\d+)_([a-z]+).htm$</from>
		<to>/moreList.htm?parentId=$1&amp;id=$2&amp;code=$3</to>
	</rule>
	<rule>
		<from>^/moreList_p(\d+)_a(\d+)_([a-z]+).htm$</from>
		<to>/moreList.htm?parentId=$1&amp;assistTypeId=$2&amp;code=$3</to>
	</rule>
	<rule>
		<from>^/moreList_p(\d+)_a(\d+)_t(\d+)_([a-z]+).htm$</from>
		<to>/moreList.htm?parentId=$1&amp;assistTypeId=$2&amp;id=$3&amp;code=$4</to>
	</rule>
	<rule>
		<from>^/moreList_p(\d+)_([a-z]+).htm$</from>
		<to>/moreList.htm?parentId=$1&amp;code=$2</to>
	</rule>
	<rule>
		<from>^/priceDetails_(\d+).htm$</from>
		<to>/priceDetails\.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/priceDetails_(\d+)_([a-z]+)\.htm$</from>
		<to>/priceDetails\.htm?id=$1&amp;code=$2</to>
	</rule>
	<rule>
		<from>^/list_([a-z]+).htm$</from>
		<to>/list.htm?code=$1</to>
	</rule>

	<rule>
		<from>^/companyprice/list--pc(\d*)\.htm$</from>
		<to>/companyprice/list.htm?categoryCompanyPriceCode=$1</to>
	</rule>
	<rule>
		<from>^/companyprice/list--([0-9,a-z,A-Z,%]*)--pc(\d*)--area(\d*)--int([\-,\d]*)--pr([0-9,\-]*)\.htm$</from>
		<to>/companyprice/list.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;postInDays=$4&amp;priceRange=$5</to>
	</rule>
	<rule>
		<from>^/companyprice/list--([0-9,a-z,A-Z,%]*)--pc(\d*)--area(\d*)--int([\-,\d]*)--pr([0-9,\-]*)--s(\d*)--l(\d*)\.htm$</from>
		<to>/companyprice/list.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;postInDays=$4&amp;priceRange=$5&amp;startIndex=$6&amp;pageSize=$7</to>
	</rule>
	<rule>
		<from>^/companyprice/list--([0-9,a-z,A-Z,%]*)--pc(\d*)--area(\d*)--t([\-,\d]*)--pr([0-9,\-]*)--s(\d*)--l(\d*)\.htm$</from>
		<to>/companyprice/list.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;clickTime=$4&amp;priceRange=$5&amp;startIndex=$6&amp;pageSize=$7</to>
	</rule>
	<rule>
		<from>^/companyprice/list--([0-9,a-z,A-Z,%]*)--pc(\d*)--area(\d*)--t([\-,\d]*)--pr([0-9,\-]*)\.htm$</from>
		<to>/companyprice/list.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;clickTime=$4&amp;priceRange=$5</to>
	</rule>
	
	<!--企业报价-->
	<rule>
		<from>^/companyprice/priceDetails(\d+).htm$</from>
		<to>/companyprice/priceDetails.htm?id=$1</to>
	</rule>

</urlrewrite>