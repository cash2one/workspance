<script>
	jQuery(document).ready(function(){
	
		jQuery("#searchform").submit(function(){
			
			var keyWord=jQuery("#searchKeyword").val();
    		if(keyWord==""){
    			return false;
    		}
			var searchtype = jQuery("#searchType").val();
			
			if(searchtype==1){
				zz91Search.products({
					contextpath:"${address.trade}",
                    target:"_blank",
                    searchOptions:{
                        keywords:keyWord,
						ptype:"1"
                    }
				});
				
				return false;
			}
			
			if(searchtype==2){
				zz91Search.products({
					contextpath:"${address.trade}",
                    target:"_blank",
                    searchOptions:{
                        keywords:keyWord,
						ptype:"2"
                    }
				});
				
				return false;
			}
			
			if(searchtype==3){
				zz91Search.price({
					contextpath:"${address.jage}",
                    target:"_blank",
                    searchOptions:{
                        keywords:keyWord
                    }
				});
				
				return false;
			}
			
			if(searchtype==4){
				window.open("$!{address.huzhu}/search.htm?keywords=" + encodeURIComponent(keyWord));
    			return false;
			}
			
			if(searchtype==5){
				window.open("$!{address.company}/index.htm?keywords=" + encodeURIComponent(keyWord));
				return false;
			}
			
			return false;
		});
		
	});
	
</script>
