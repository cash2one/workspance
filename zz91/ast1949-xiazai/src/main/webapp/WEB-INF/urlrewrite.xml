<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	
	
	<rule>
		<from>^/list-c([0-9]*).htm$</from>
		<to>/list.htm?code=$1</to>
	</rule>
	
	<rule>
		<from>^/list-c([0-9]*)-s(\d+).htm$</from>
		<to>/list.htm?code=$1&amp;startIndex=$2</to>
	</rule>
	
	<rule>
		<from>^/list--([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?title=$1</to>
	</rule>
	
	<rule>
		<from>^/list--([0-9,a-z,A-Z,%,+]*)-s(\d+).htm$</from>
		<to>/list.htm?title=$1&amp;startIndex=$2</to>
	</rule>
	
	<rule>
		<from>^/detail(\d+).htm$</from>
		<to>/detail.htm?id=$1</to>
	</rule>

</urlrewrite>