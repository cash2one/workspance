<script>
	$(".succbox").mouseover(function(){
		$(this).find(".floatbox").show();
		$(this).css("position","relative");
	})
	$(".succbox").mouseout(function(){
		$(this).find(".floatbox").hide();
		$(this).css("position","static");
	})
</script>