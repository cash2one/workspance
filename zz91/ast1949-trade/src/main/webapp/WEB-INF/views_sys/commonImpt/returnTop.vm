<div class="rTop" id="rTop" style="display:none;"> ## 
	<a href="#pageTop">
		<img src="$!{address.img}/zz91/trade/images/zz91te_top.jpg" width="24" height="75" />
	</a>
</div>
<script type="text/javascript" >
	jQuery(document).ready(function(){
		jQuery("body").prepend("<a name='pageTop'></a>");
		
		$(window).bind('scroll', function() {
			var margin_top = $(document).scrollTop(); 
			if(margin_top>768){
				jQuery("#rTop").show();
			}else{
				jQuery("#rTop").hide();
			}
		});
	});
</script>