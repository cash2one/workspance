jQuery(document).ready(function() {
    if ((jQuery('#pics-indiv').find('li').length) > 6) {
        var autoFun;
        jQuery('#pics-indiv li:first').before(jQuery('#pics-indiv li:last'));
        jQuery('#pics-rgtbtn img').click(function() {
            var item_width = jQuery('#pics-indiv li').outerWidth() + 26;
            var left_indent = parseInt(jQuery('#pics-indiv').css('left')) - item_width;
            jQuery('#pics-indiv:not(:animated)').animate({
                'left': left_indent
            },
            1000,
            function() {
                jQuery('#pics-indiv li:last').after(jQuery('#pics-indiv li:first'));
                jQuery('#pics-indiv').css({
                    'left': '-126px'
                });
            });
        });
        //
        jQuery('#pics-leftbtn img').click(function() {
            var item_width = jQuery('#pics-indiv li').outerWidth() + 26;
            var left_indent = parseInt(jQuery('#pics-indiv').css('left')) + item_width;
            jQuery('#pics-indiv:not(:animated)').animate({
                'left': left_indent
            },
            1000,
            function() {
                jQuery('#pics-indiv li:first').before(jQuery('#pics-indiv li:last'));
                jQuery('#pics-indiv').css({
                    'left': '-126px'
                });
            });
        });
        //***事件划入时停止自动滚动
        function clearFun(elem) {
            elem.hover(function() {
                clearAuto();
            },
            function() {
                autoSlide();
            });
        };
        //***自动滚动
        function autoSlide() {
            jQuery('#pics-rgtbtn img').trigger('click');
            autoFun = setTimeout(autoSlide, 3000); //此处不可使用setInterval,setInterval是重复执行传入函数,这会引起第二次划入时停止失效
        };
        //***清除自动滚动
        function clearAuto() {
            clearTimeout(autoFun);
        };
        //***调用自动滚动
        autoSlide();
        //***停止滚动
        clearFun(jQuery('#pics-indiv'));
        clearFun(jQuery('#pics-rgtbtn img'));
        clearFun(jQuery('#pics-leftbtn img'));
    } else {
        jQuery('#pics-indiv').css({
            'left': '0px'
        });
        jQuery('#pics-rgtbtn img').hide();
        jQuery('#pics-leftbtn img').hide();
    }
});

