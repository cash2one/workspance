});
jQuery("#searchCode").click(function(){
	$("#conBtn").find("a").removeClass("btn02-primary").addClass("btn01-primary");
	var key = jQuery("#kwd").val().replace(/\s/g,'');
	if(key==null||key==""||key==undefined){
				return false;
	}else{
		jQuery("#choosebox").find("ul").empty();
		jQuery("#selectbox").parent().css("display","none");
		jQuery("#chooseboxx").parent().css("display","none");
		jQuery("#choosebox").parent().css("display","block");
	}
	jQuery.ajax({
    		url : "#springUrl('/myyuanliao/queryCategoryByKeyword.htm?keyword=')"+key,
    		type : "post",
    		cache : false,
    		dataType : "json", 
    		success : function(data, textStatus){
    			for(var i=0;i<data.length;i++){
					jQuery("#choosebox").find("ul").append("<li><input type='radio' name='options' value='"+data[i].code+"'/><span>&nbsp"+data[i].first+"&nbsp;>&nbsp;"+data[i].second+"&nbsp;>&nbsp;"+data[i].third+"</span></li>");
				}
    		}
    });
});
	jQuery("#choosebox").click(function(){
		var v = $("input[name=options]:checked + span").text();
		jQuery("#choosetext").find("span").html(v);
		var vv = $(":radio:checked").val();
		jQuery("#categorycode").val(vv);
		jQuery("input[name=categoryYuanliaoCode]").val(vv);
		$("#conBtn").find("a").removeClass("btn01-primary").addClass("btn02-primary");
