		jQuery("#categorycode").val("");
		jQuery("#c2").empty();
		jQuery("#c2").append("<option disabled='disabled'>---二级分类---</option>");
		jQuery.ajax({
    		url : "#springUrl('/myyuanliao/queryCategoryYuanliaoByParentCode.htm?parentCode=')"+"20091000",
    		type : "post",
    		cache : false,
    		dataType : "json", 
