function subm(){
    //有效期
	var limittime = jQuery("input[name=time]:checked").val();
	if(!limittime){
		jQuery(".limittime").css("display","inline");
	}
	//描述信息
	var info = editor.text();//jQuery("#textarea1").text().replace(/\s/g,''); 
	if(!info){
		jQuery("#textarea1").parent().find(".red").css("display","inline");
	}
    //期货现货
	var goods = jQuery("input[name=goodsTypeCode]:checked").val();
	if(!goods){
		jQuery("#goods").find(".red").css("display","inline");
	}
    //价格
	var price = jQuery("input[name=minPrice]").val().replace(/\s/g,'');
	if(jQuery("#price_chk").val()){
		price = jQuery("input[name=maxPrice]").val().replace(/\s/g,'')
	}
	if(!price){
		jQuery("#pricelit").css("display","inline");
	}
    //供应数量
	var quantity = jQuery("input[name=quantity]").val().replace(/\s/g,'');
	if(!quantity){
		jQuery("input[name=quantity]").parent().parent().find("span").css("display","inline");
	}
    //标签
	var tagArray = jQuery("input[name=tags]");
	var tag = 0;
	for(var i = 0;i<tagArray.length;i++){
		if(tagArray[i].value.replace(/\s/g,'')){
			tag = tag + 1;
		}
	}
	if(tag==0){
		jQuery("input[name=tags]").parent().parent().find(".red").css("display","inline");
	}
 	//标题	
    var title=jQuery("input[name=title]").val().replace(/\s/g,'');
	if(!title){
		jQuery("input[name=title]").parent().find("span").html("标题不能为空").css("display","inline");
	}else if(title.length>30){
		jQuery("input[name=title]").parent().find("span").html("标题的字数只能在30字内").css("display","inline");
	}
	//牌号
	var mark = jQuery("input[name=tradeMark]").val().replace(/\s/g,'');   
	if(!mark){
		jQuery("input[name=tradeMark]").parent().find("span").css("display","inline");
	}
	//厂家
	var trade = jQuery("input[name=categoryMainDesc]:checked").val()||jQuery("input[name=categoryAssistDesc]").val().replace(/\s/g,'');
	if(!trade){
		jQuery("#brand").parent().parent().find("span").css("display","inline");
	}
	if(!trade){
		$("html,body").animate({scrollTop:jQuery("#brand").offset().top-100});
		return false;
	}
	if(!mark){
		$("html,body").animate({scrollTop:jQuery("input[name=tradeMark]").offset().top-100});
		return false;
	}
	if(!title||title.length>30){
		$("html,body").animate({scrollTop:jQuery("input[name=title]").offset().top});
		return false;
	}
	if(tag==0){
		$("html,body").animate({scrollTop:jQuery("input[name=tags]").offset().top-100});
		return false;
	}
	if(!quantity){
		$("html,body").animate({scrollTop:jQuery("input[name=quantity]").offset().top-100});
		return false;
	}
	if(!price){
		$("html,body").animate({scrollTop:jQuery("input[name=price]").offset().top-200});
		return false;
	}
	if(!goods){
		$("html,body").animate({scrollTop:jQuery("input[name=goodsTypeCode]").offset().top-100});
		return false;
	}
	if(!limittime){
		$("html,body").animate({scrollTop:jQuery("input[name=time]").offset().top-100});
		return false;
	} 
	if(!info||info.length<15){
		$("html,body").animate({scrollTop:jQuery("#textarea1").offset().top-100});
		return false;
	}
	jQuery("#subForm").submit();
}
