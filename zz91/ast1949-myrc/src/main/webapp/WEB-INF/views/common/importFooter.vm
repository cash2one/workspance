	        if ( keyWord == ""){
	            return false;
	        } else {
	        	switch(option) {
	        		case "0":
	        			zz91Search.products({
		                    contextpath:"${address.trade}",
		                    target:"_blank",
		                    searchOptions:{
		                        keywords:keyWord
		                    }
		                });
	        			break;
	        		case "1":
	        			zz91Search.price({
		                    contextpath:"$!{address.price}",
		                    target:"_blank",
		                    searchOptions:{
		                        keywords:keyWord
		                    }
		                });
	        			break;
	        		case "2":
	        			window.open("$!{address.huzhu}/search.htm?keywords=" + encodeURIComponent(keyWord));
	        			break;
	        		case "3":
	        			window.open("$!{address.company}/index-p-" + keyWord + ".htm");
	        			break;
	        		default:
	        			break;
	        	}
				return false;
	        }
	    })
