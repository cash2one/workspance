<div class="container mt clearfix">
    <div class="grid_12">
        <ul class="breadcrumb">
            <li class="active">
				<a href="#springUrl('/myphone/index.htm')">来电宝</a>- 查看通话扣费记录
				<a href="#springUrl('/myphone/index.htm')" class="btn">返回来电宝</a>
			</li>
        </ul>
        <div class="well mt">
            <table class="table table-bordered table-condensed mt" id="myTable">
                <thead>
                    <tr>
						<th width="25%">开始时间</th>
                        <th width="25%" class="tac" >结束时间</th>
                        <th width="25%" class="tac" >来电号码</th>
                        <th width="25%" class="tac" >通话费用</th>
                    </tr>
                </thead>
                <tbody>
                    #foreach($obj in $!{page.records})
                        <tr>
                            <td>$!date.format('yyyy-MM-dd HH:mm:ss', $!{obj.startTime})</td>
                            <td class="tac" id="refreshTimeFlag$!{products.id}">$!date.format('yyyy-MM-dd HH:mm:ss', $!{obj.endTime})</td>
                            <td class="tac">$!{obj.callerId}</td>
                            <td class="tac">$!{obj.callFee} 元 </td>
                        </tr>
					#end
						<tr>
							<td colspan="4" style="text-align:right">余额：<b class="red">$!{phone.balance}</b>元</td>
						</tr>
                </tbody>
            </table>
        </div>
    </div>
	#myrcPageNav($!{page} "#springUrl('/myphone/phoneLog.htm')" "" "")
</div>
<script>
jQuery(function(){
	myrc.menu(1, 7);
})
</script>
