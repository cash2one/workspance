<script>
    jQuery(function(){
    	myrc.menu(5, 5);
    	tabs();
    	selecter();
	
		jQuery(".function_add").click(function(){
			if (jQuery(this).attr("checked")) {
    			var value= jQuery(this).attr("value");
    			jQuery.ajax({
    				url:"#springUrl('/subscribe/addSubscribeSms.htm')",
    				type:"POST",
        			cache:false,
        			dataType:"json",
    				data:{"categoryCode":value},
    				success:function(response){
        				if(response.success){
        					//提示订制成功
        					//跳转到报价订制管理页面
        					alert("您刚刚订阅了此类别报价信息!");
        					//window.location.href="#springUrl('/mysubscribe/subscribesms.htm')";
        				}else{
							if(response.data=="outofSubscribeLimit"){
            					alert("对不起，您已经成功订阅了20个类别,不能再订阅了，谢谢！")
            				}
        				}
        			}
    			});
			}else{
				var value= jQuery(this).attr("value");
				if(confirm('确定要取消订阅吗?')==false){
					return false;
				}
    			jQuery.ajax({
    				url:"#springUrl('/mysubscribe/deleteSubscribeSms.htm')",
    				type:"POST",
        			cache:false,
        			dataType:"json",
    				data:{"categoryCode":value},
    				success:function(response){
        				if(response.success){
        					alert("您刚刚取消订阅此类别信息!");
							//window.location.href="#springUrl('/mysubscribe/subscribesms.htm')";
        				}
        			}
    			});
			}
		});
	});
	
    function tabs(){
    	var tabs = jQuery("#Js_tabs").find("li"),
    		tab_content = jQuery("#Js_tab_content").find(".tab-pane");
    		index = 0;
    	tabs.click(function(){
    		jQuery(this).addClass("active");
    		tabs.eq(index).removeClass("active");
    		tab_content[index].style.display = "none";
    		index = tabs.index(this);
    		tab_content[index].style.display = "block";
    	});
    }
    function selecter(){
    	var group = jQuery("#Js_tab_content").find("table"),
    		input = [],
    		value = false;
    	group.each(function(i){
    		input[i] = jQuery(this).find("input[type='checkbox']");
    	});
    	jQuery(input).each(function(i){
    		jQuery(this[0]).on("click", function(){
    			value = this.checked;
    			jQuery(input[i]).each(function(j){
    				if (j !== 0) {
    					this.checked = value;
    				}
    			})
    		});
    	})
    }
</script>
