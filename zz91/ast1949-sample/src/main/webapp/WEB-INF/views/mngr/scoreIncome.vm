        <div class="gContainer">
		<div class="gwTopic clearfix">
            <h2>积分收入</h2>
        </div>
        <div class="basicForm recordLog">
			
            <div class="ct" >当前可兑换积分：#if($!{totalScore})$!{totalScore}#else 0 #end  分</div>
            
			<table class="gtb recordprint">
                <tbody>
                <tr class="gtbTh">
                    <th width="25%"><div>获得积分</div></th>
					<th width="25%"><div>积分来源</div></th>
                    <th width="25%"><div>获得时间</div></th>
					<!-- 
                    <th width="25%"><div>有效时间</div></th>
					-->
                </tr>
				
				#foreach($obj in $page.records)
				<tr>
                    <td width="25%"><div> $!{obj.score}  </div></td>
                    <td width="25%"><div>#if($!{obj.rulesCode}=='yingorder') 成交一笔样品订单 #else  手机站签到  #end </div></td>
                    <td width="25%"><div>$!date.format('yyyy-MM-dd HH:mm:ss',$!{obj.gmtCreated})</div></td>
					<!-- 
                    <td width="25%"><div>$!date.format('yyyy-MM-dd HH:mm:ss',$!{obj.validity})</div></td>
					-->
                </tr>
				#end
				
				
				#if($!{page.records.size()}==0)
                <tr>
                    <td colspan="6">暂无收入记录！</td>
                </tr>
				#end	
				
              </tbody></table>
              
			 <div class="gPageBar clearfix">
					<div class="gPage">
						#myrcPageNav($!{page} "#springUrl('/mngr/scoreIncome.htm')" "" "")
				</div> 
