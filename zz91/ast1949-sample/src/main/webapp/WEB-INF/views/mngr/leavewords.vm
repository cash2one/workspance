#set($layout="/layout/blank.vm")
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>订单留言</title>
<link rel="stylesheet" type="text/css" href="http://img0.zz91.com/zz91/sample/admin/css/common.css">
<link rel="stylesheet" type="text/css" href="http://img0.zz91.com/zz91/sample/admin/css/index.css">
<style>
body td{padding:10px}
.line{
	border-top-width: 1px;
	border-top-style: solid;
	border-top-color: #CCC;
}
.brue{color:#03C}
</style>
</head>
<body>
<table width="100%" border="0" cellspacing="0">
  <tr>
    <td width="70px" align="left" valign="top" class="line brue">样品级别：</td>
    <td valign="top" class="line" align="left">$!{custominfo.level}</td>
  </tr>
  <tr>
    <td align="left" valign="top" class="line brue">样品颜色：</td>
    <td valign="top" class="line" align="left">$!{custominfo.color}</td>
  </tr>
  <tr>
    <td align="left" valign="top" class="line brue">用途：</td>
    <td valign="top" class="line" align="left">$!{custominfo.useDes}</td>
  </tr>
  <tr>
    <td align="left" valign="top" class="line brue">详细描述：</td>
    <td valign="top" class="line" align="left">$!{custominfo.memo}</td>
  </tr>
</table>
</body>
</html>
