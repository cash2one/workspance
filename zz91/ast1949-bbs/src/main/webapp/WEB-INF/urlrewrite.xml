<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/bbsTags(\d+).htm$</from>
		<to>/bbsTags.htm?tagsId=$1</to>
	</rule>
	<rule>
        <from>^/bbsTags_t(\d+)_p(\d+)\.htm$</from>
        <to>/bbsTags.htm?tagsId=$1&amp;p=$2</to>
    </rule>
	<rule>
        <from>^/bbsTags_t(\d+)_([a-z]+)_p(\d+)\.htm$</from>
        <to>/bbsTags.htm?tagsId=$1&amp;title=$2&amp;p=$3</to>
    </rule>
	<rule>
		<from>^/bbsCategory(\d+).htm$</from>
		<to>/bbsCategory.htm?categoryId=$1</to>
	</rule>
	<rule>
		<from>^/bbsCategory--a(\d+)--b([a-z,_]*)--s(\d*)--l(\d*).htm$</from>
		<to>/bbsCategory.htm?categoryId=$1&amp;sort=$2&amp;startIndex=$3&amp;pageSize=$4</to>
	</rule>

	<rule>
		<from>^/viewReply(\d+).htm$</from>
		<to>/viewReply.htm?postId=$1</to>
	</rule>
	 <rule>
        <from>^/viewReply_p(\d+)_(\d+)\.htm$</from>
        <to>/viewReply.htm?postId=$1&amp;p=$2</to>
    </rule>
	<rule>
		<from>^/othersreply(\d+).htm$</from>
		<to>/othersreply.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/othersposted(\d+).htm$</from>
		<to>/othersposted.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/postReply_c(\d+)\.htm$</from>
		<to>/postReply.htm?categoryId=$1</to>
	</rule>	
	<rule>
		<from>^/bbsTags_c(\d+)\.htm$</from>
		<to>/bbsTags.htm?sign=$1</to>
	</rule>
	<rule>
		<from>^/postDel_p(\d+)\.htm$</from>
		<to>/postDel.htm?postId=$1</to>
	</rule>
</urlrewrite>