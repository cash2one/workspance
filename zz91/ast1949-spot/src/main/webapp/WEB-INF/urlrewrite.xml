<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	
	<!-- 详细页面 -->
	<rule>
		<from>^/detail(\d+).htm$</from>
		<to>/detail.htm?spotId=$1</to>
	</rule>

	<rule>
		<from>^/cart.htm$</from>
		<to>/transaction/cart.htm</to>
	</rule>

	<rule>
		<from>^/confirm.htm$</from>
		<to>/transaction/confirm.htm</to>
	</rule>
	
	<rule>
		<from>^/baogang/details(\d+).htm$</from>
		<to>/baogang/detail.htm?id=$1</to>
	</rule>

</urlrewrite>