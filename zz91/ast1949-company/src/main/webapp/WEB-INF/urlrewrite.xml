<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/company_list_g(\d+)_i(\d+).htm$</from>
		<to>/list.htm?categoryGardenId=$1&amp;industryCode=$2</to>
	</rule>
	<rule>
		<from>^/compinfo(\d+).htm$</from>
		<to>/compinfo.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/compinfo(\d+)-f(\d*)-p(\d*).htm$</from>
		<to>/compinfo.htm?id=$1&amp;flag=$2&amp;startIndex=$3</to>
	</rule>
	<rule>
		<from>^/compinfo(\d+)-m(\d*)-f(\d*)-p(\d*).htm$</from>
		<to>/compinfo.htm?id=$1&amp;mark=$2&amp;flag=$3&amp;startIndex=$4</to>
	</rule>	
	<rule>
		<from>^/index-p(\d*)-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?startIndex=$1&amp;keywords=$2</to>
	</rule>

	<rule>
		<from>^/index-p(\d*).htm$</from>
		<to>/list.htm?startIndex=$1</to>
	</rule>
	
<!--	<rule>-->
<!--		<from>^/index-b(\d*)-c(\d*)-([0-9,a-z,A-Z,%,+]*)-p(\d*).htm$</from>-->
<!--		<to>/index.htm?industryCode=$1&amp;areaCode=$2&amp;startIndex=$3&amp;keywords=$4</to>-->
<!--	</rule>-->
	
	<rule>
		<from>^/index-b(\d*)-c(\d*)-p(\d*)-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?industryCode=$1&amp;areaCode=$2&amp;startIndex=$3&amp;keywords=$4</to>
	</rule>
	
	
	
	<rule>
		<from>^/index-b(\d*)-c(\d*)-p(\d*).htm$</from>
		<to>/list.htm?industryCode=$1&amp;areaCode=$2&amp;startIndex=$3</to>
	</rule>
	
	<rule>
		<from>^/index-b(\d*)-c(\d*)-([0-9,a-z,A-Z,%,+]*).htm$</from>
		<to>/list.htm?industryCode=$1&amp;areaCode=$2&amp;keywords=$3</to>
	</rule>
	
	<rule>
		<from>^/index-b(\d*)-c(\d*).htm$</from>
		<to>/list.htm?industryCode=$1&amp;areaCode=$2</to>
	</rule>

</urlrewrite>