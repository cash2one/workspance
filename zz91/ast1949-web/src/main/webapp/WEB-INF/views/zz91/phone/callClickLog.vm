	<script type="text/javascript" src="$!{address.js}/phone/callClickLog.js" ></script>
	
<script type="text/javascript">
	Ext.onReady(function() {
		var grid = new ast.ast1949.phonecallclick.callClickLogGrid({
			id:"callClickLogGrid",
			region:"center",
			layout:"fit"
		});
		grid.loadCallClickLogRecord($!{companyId});
    	var viewport = new Ext.Viewport({
    		layout : "border",
    		border : true,
    		items : [grid]
    	});
    	grid.initDate("$!{from}", "$!{to}");
	});
</script>
