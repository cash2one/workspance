<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title></title>
	#parse("/zz91/common/import.vm")
	#parse("/zz91/common/context.vm")
	
	<script type="text/javascript" src="$!{address.js}/admin/lib/CategoryCombo.js" ></script>
	<script type="text/javascript" src="$!{address.js}/admin/credit/creditfile/list.js" ></script>
	<script type="text/javascript">
		//定义变量
		var CreditFileContext = new function(){
			this.RESOURCE_URL="$!{resourceUrl}";
			this.UPLOAD_MODEL="$!{uploadModel}";
		}
		
		Ext.onReady(function() {
        	//加载列表
        	var grid = new ast.ast1949.admin.credit.creditfile.ResultGrid({
        		title:"证书荣誉列表",
        		id:_C.RESULT_GRID,
        		listUrl:Context.ROOT+Context.PATH+"/admin/credit/creditfile/list.htm",
        		region:'center',
        		autoScroll:true
        	});
        	
        	var B=grid.getStore().baseParams||{};
        	B["checkStatus"] = "0,2";
        	grid.getStore().baseParams = B;
        	grid.getStore().reload({params:{"startIndex":0, "pageSize":Context.PAGE_SIZE}});
        	
        	var viewport = new Ext.Viewport({
        		layout:'border',
        		items:[grid]
        	});
        	
        });
	</script>
