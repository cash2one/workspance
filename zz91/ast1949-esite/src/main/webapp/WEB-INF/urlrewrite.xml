<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/index(\d+).htm$</from>
		<to>/index.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/gsjs(\d+).htm$</from>
		<to>/gsjs.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/zxgq(\d+).htm$</from>
		<to>/zxgq.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/gsdt(\d+).htm$</from>
		<to>/gsdt.htm?cid=$1</to>
	</rule>
	
	<rule>
		<from>^/gsdtdetail(\d+).htm$</from>
		<to>/gsdtdetail.htm?cid=$1</to>
	</rule>
	
	<rule>
		<from>^/news(\d+).htm$</from>
		<to>/gsdtdetail.htm?id=$1</to>
	</rule>
	
	<rule>
		<from>^/products(\d+).htm$</from>
		<to>/static/products.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/yuanliao(\d+).htm$</from>
		<to>/static/yuanliao.htm?id=$1</to>
	</rule>	
	<rule>
		<from>^/cxda(\d+).htm$</from>
		<to>/cxda.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/cxdavote(\d+).htm$</from>
		<to>/cxdavote.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/zxly(\d+).htm$</from>
		<to>/zxly.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/lxfs(\d+).htm$</from>
		<to>/lxfs.htm?cid=$1</to>
	</rule>
	
	<!-- from old esite -->
	<rule>
		<from>^/mysite_about(\d+)_(\d*).html$</from>
		<to>/gsjs.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_news(\d+).html$</from>
		<to>/gsdt.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_question(\d+)_(\d*).html$</from>
		<to>/zxly.htm.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_contact(\d+).html$</from>
		<to>/lxfs.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_offer_detail(\d+)_(\d*).html$</from>
		<to>/static/products.htm?cid=$1&amp;id=$2</to>
	</rule>
	<rule>
		<from>^/mysite_offer(\d+)_(\d+).html$</from>
		<to>/zxgq.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_offer(\d+)_(\d+)_(\d+).html$</from>
		<to>/zxgq.htm?cid=$1&amp;sid=$3</to>
	</rule>
	<rule>
		<from>^/mysite_remark(\d+)_(\d+).html$</from>
		<to>/cxda.htm?cid=$1</to>
	</rule>
	<rule>
		<from>^/mysite_news_details(\d+).html$</from>
		<to>/gsdt/detail.htm?id=$1</to>
	</rule>
	<rule>
		<from>^/zxgq-sid(\d+).htm$</from>
		<to>/zxgq.htm?sid=$1</to>
	</rule>
	<rule>
		<from>^/sitemap.htm$</from>
		<to>/errorPage404.htm</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z]+).xml$</from>
		<to>/silianXML.htm?fileName=$1</to>
	</rule>
</urlrewrite>