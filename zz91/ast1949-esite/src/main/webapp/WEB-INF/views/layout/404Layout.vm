		}
		
	function banner(el) {
		var el = jQuery(el);
		if (!el.attr("data-banner")) {
			return;
		}
		var nav = jQuery("<ul class='naver'></ul>"),
			img = el.find("img"),
			num = img.length,
			lis = [],
			cur = 0,
			timer;
		for (var i = 0; i < num; i ++) {
			lis[i] = jQuery("<li>" + (i + 1) + "</li>");
			nav.append(lis[i]);
		}
		el.append(nav);
		img.eq(0).show();
		lis[0].addClass("cur");
		timer = setInterval(anim, 3000);
		
		jQuery(lis).each(function(index){
			jQuery(this).click(function(){
				if (index !== cur) {
					img.eq(cur).fadeOut();
        			lis[cur].removeClass("cur");
					cur = index;
        			img.eq(cur).fadeIn();
        			lis[cur].addClass("cur");
				}
			});
		});
		
		nav.hover(function(){
			clearInterval(timer);
		}, function(){
			timer = setInterval(anim, 3000);
		});
		
		function anim(){
			img.eq(cur).fadeOut();
			lis[cur].removeClass("cur");
			cur = cur === num - 1 ? 0: cur + 1;
			img.eq(cur).fadeIn();
			lis[cur].addClass("cur");
		}
	}
	function banner1(el) {
		var el = jQuery(el);
		if (!el.attr("data-banner")) {
			return;
		}
		var nav = jQuery("<ul class='naver'></ul>"),
			img = el.find("img"),
			num = img.length,
			lis = [],
			cur = 0,
			timer;
		el.append(nav);
		img.eq(0).show();
		
		
	}

