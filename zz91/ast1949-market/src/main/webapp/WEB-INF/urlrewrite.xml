<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite >
   <!--市场链接-->	
	<rule>
		<from>^/market/v(\d*)i(\d*)c(\d*).htm$</from>
		<to>/market.htm?provice=$1&amp;industry=$2&amp;category=$3</to>
	</rule>
    <rule>
		<from>^/market/v(\d*)i(\d*)c(\d*)f(\d*)d(\d*).htm$</from>
		<to>/market.htm?provice=$1&amp;industry=$2&amp;category=$3&amp;flag=$4&amp;dir=$5</to>
	</rule>	
	<rule>
		<from>^/market/v(\d*)i(\d*)c(\d*)f(\d*)d(\d*)-p(\d*).htm$</from>
		<to>/market.htm?provice=$1&amp;industry=$2&amp;category=$3&amp;flag=$4&amp;dir=$5&amp;currentPage=$6</to>
	</rule>	
	<rule>
		<from>^/marketIndex/(\d*).htm$</from>
		<to>/marketIndex.htm?id=$1</to>
	</rule>
    <rule>
		<from>^/marketTrader/(\d*)-p(\d*).htm$</from>
		<to>/marketTrader.htm?id=$1&amp;currentPage=$2</to>
	</rule>	
	<rule>
		<from>^/marketTrade/(\d*)t(\d*)i(\d*).htm$</from>
		<to>/marketTrade.htm?id=$1&amp;type=$2&amp;industry=$3</to>
	</rule>	
	<rule>
		<from>^/marketTrade/(\d*)t(\d*)i(\d*)-p(\d*).htm$</from>
		<to>/marketTrade.htm?id=$1&amp;type=$2&amp;industry=$3&amp;currentPage=$4</to>
	</rule>	
	<rule>
		<from>^/marketIndustry/(\d*).htm$</from>
		<to>/marketIndustry.htm?id=$1</to>
	</rule>		
	<rule>
		<from>^/marketIndustry/(\d*)-p(\d*).htm$</from>
		<to>/marketIndustry.htm?id=$1&amp;currentPage=$2</to>
	</rule>	
	<rule>
		<from>^/search-([0-9,a-z,A-Z,%,+]*)-s(\d*)-i(\d*).htm$</from>
		<to>/market.htm?keywords=$1&amp;provice=$2&amp;industry=$3</to>
	</rule>	
	<rule>
		<from>^/search-([0-9,a-z,A-Z,%,+]*)-s(\d*)-i(\d*)f(\d*)d(\d*).htm$</from>
		<to>/market.htm?keywords=$1&amp;provice=$2&amp;industry=$3&amp;flag=$4&amp;dir=$5</to>
	</rule>		
	<rule>
		<from>^/search-([0-9,a-z,A-Z,%,+]*)-s(\d*)-i(\d*)f(\d*)d(\d*)-p(\d*).htm$</from>
		<to>/market.htm?keywords=$1&amp;provice=$2&amp;industry=$3&amp;flag=$4&amp;dir=$5&amp;currentPage=$6</to>
	</rule>	
   	<rule>
		<from>^/zsmapDetail/p(\d*).htm$</from>
		<to>/map/zsmapDetail.htm?province=$1</to>
	</rule>	
	<rule>
		<from>^/zsmapDetail/c(\d*).htm$</from>
		<to>/map/zsmapDetail.htm?category=$1</to>
	</rule>	
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/$</from>
		<to>/marketIndex.htm?words=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/info.html$</from>
		<to>/marketInfo.htm?words=$1</to>
	</rule>	
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/scsh.html$</from>
		<to>/marketTrader.htm?words=$1</to>
	</rule>
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/scsh-p(\d*).html$</from>
		<to>/marketTrader.htm?words=$1&amp;currentPage=$2</to>
	</rule>		
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/scgq([a-z,A-Z,%,+]*)(\d*).html$</from>
		<to>/marketTrade.htm?words=$1&amp;type=$2&amp;industry=$3</to>
	</rule>	
    <rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/scgq([a-z,A-Z,%,+]*)(\d*)-p(\d*).html$</from>
		<to>/marketTrade.htm?words=$1&amp;type=$2&amp;industry=$3&amp;currentPage=$4</to>
	</rule>					
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/thysc.html$</from>
		<to>/marketIndustry.htm?words=$1</to>
	</rule>	
	<rule>
		<from>^/([0-9,a-z,A-Z,%,+]*)/thysc-p(\d*).html$</from>
		<to>/marketIndustry.htm?words=$1&amp;currentPage=$2</to>
	</rule>								
</urlrewrite>