<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
    	<from>^/details(\d+).htm$</from>
    	<to>/details.htm?id=$1</to>
    </rule>
    <rule>
    	<from>^/list(\d+).htm$</from>
    	<to>/list.htm?plateCategoryCode=$1</to>
    </rule>
    
    <rule>
    	<from>^/list(\d+)--s(\d+)--l(\d+).htm$</from>
    	<to>/list.htm?plateCategoryCode=$1&amp;startIndex=$2&amp;pageSize=$3</to>
    </rule>
    <rule>
    	<from>^/list(\d*)--b(\d*)--s(\d+)--l(\d+).htm$</from>
    	<to>/list.htm?plateCategoryCode=$1&amp;category=$2&amp;startIndex=$3&amp;pageSize=$4</to>
    </rule>
    
    <rule>
    	<from>^/searchExhibit--a(\d*)--b(\d*)--c([0-9,-]*)--d([0-9,-]*)--k([0-9,a-z,A-Z,%]*)--s(\d+)--l(\d+).htm$</from>
    	<to>/searchExhibit.htm?exhibitCategoryCode=$1&amp;areaCode=$2&amp;mystartTime=$3&amp;myendTime=$4&amp;name=$5&amp;startIndex=$6&amp;pageSize=$7</to>
    </rule>
    <rule>
    	<from>^/exhibitchannel/taizhou/newsDetail(\d+).htm$</from>
    	<to>/exhibitchannel/taizhou/newsDetail.htm?id=$1</to>
    </rule>
   	<rule>
    	<from>^/exhibitchannel/taizhou/newsList/p(\d+).htm$</from>
    	<to>/exhibitchannel/taizhou/newsList.htm?currentPage=$1</to>
    </rule> 
</urlrewrite>