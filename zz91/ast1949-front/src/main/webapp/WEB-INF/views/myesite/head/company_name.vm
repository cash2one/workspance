<div id="company_name"  class="moveChild topbaner"  style="height:90px;">
	<div class="clr"></div>
	<div class="pointer"  onclick="window.location.href='#'" onmousedown="return aliclick(this,'?tracelog=wp_home_slogan')" >
		<div id="log">
			<table id="logTable" width="120" height="90">
				<tr>
					<td align="center" valign="middle">
						#if($!{logoUrl})
							#if($!{logoUrl.url})
								<img id="log_img" src="$!{logoUrl.url}" height="$!{logoUrl.height}"/>
							#else
								<img id="log_img" src="$!{address.images}/esite/logo.gif" height="58"/>
							#end
						#else
                            <img id="log_img" src="$!{address.images}/esite/logo.gif" height="58"/>
						#end
					</td>
                </tr>
            </table>
		</div>
		<div id="companyname">
			<ul>
				<li class="china">
					<span id="chinaname" class="chinaname">$!{company.name}</span>
                    <span id="enname" class="enname">http://example.zz91.com</span>
				</li>
			</ul>
		</div>
		<div class="clr"></div>
	</div>
</div>