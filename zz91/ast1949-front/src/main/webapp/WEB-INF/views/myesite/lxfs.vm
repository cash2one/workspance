#set( $layout = "/layout/myesiteLayout.vm" )
<div id="content2" class="bodyRight filter filterDom">
    <div id="company_index" class="bodyCont moveChild" style="margin-bottom:7px;">
        <div class="clr"></div>
        <div class="bodyContTitle"> 
            <span class="fl b titleLinkColor titleName">联系方式</span>
        </div>
        <div class="bodyContContent rel rightConteWidth">
            <div class="bodyContContentRightCont fl tal mainTextColor break" style="line-height:22px">
                <ul class="mainTextColor">
                    <li>公司名：&nbsp;&nbsp;$!{company.name}</li>
                    <li >联系人：&nbsp;&nbsp;$!{contactInfo.contact}</li>
                    <li >电&nbsp;&nbsp;&nbsp;&nbsp;话： $!{contactInfo.telCountryCode} $!{contactInfo.telAreaCode}  $!{contactInfo.tel}</li>
                    <li >移动电话： $!{contactInfo.mobile}</li>
                    <li >传&nbsp;&nbsp;&nbsp;&nbsp;真： $!{contactInfo.faxCountryCode} $!{contactInfo.faxAreaCode} $!{contactInfo.fax}</li>
                    <li >地&nbsp;&nbsp;&nbsp;&nbsp;址： $!{contactInfo.address}</li>
                    <li>公司主页：<a class="draft_no_link" href="#">$!{contactInfo.website}</a></li>
                </ul>
            </div>
            <div class="clr"></div>
        </div>
    </div>
</div>
