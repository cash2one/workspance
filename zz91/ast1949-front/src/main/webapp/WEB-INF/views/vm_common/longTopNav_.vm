    function getRelace(stringdou) {
        var strdou;
        strdou = stringdou.toLowerCase();
        strdou = strdou.replace(/'/g, "''");
        strdou = strdou.replace(/\"/g, "”");
        return strdou;
    }

    function getSearchRepalceTO(searname) {
        var s;
        s = searname.replace(/%/g, "asto");
        s = s.replace(/\\/g, "xie");
        s = s.replace(/\//g, "fang")
        s = s.replace(/\+/g, "xiao")
        s = s.replace(/\-/g, "ming")
        return s;
    }

