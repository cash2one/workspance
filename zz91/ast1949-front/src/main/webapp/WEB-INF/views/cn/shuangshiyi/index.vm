			## 点击确认付费
			var serviceId = 0;
            var fkHtml = '<div class="login"><div class="login-du"><span class="login-into fl">确认支付</span> <span class="close fr j_close">关闭</span></div><div class="fk-qut">确定支付双十一定金<br>金额：<span>100</span>元</div><div class="lg-tishi">双十一当天支付尾款可抵用500元</div><div class="btn-div"><span class="fk-bt">确认支付</div></div></div>';
            $(".dj-ul span").on("click",function(){
				if(jQuery("#loginFlag").val()==0){
    				Asto.util.Callback.setSubmitCallback(function(success, data){
    					closeoverlay();
    					jQuery("#loginFlag").val(1)
        			});
    				openoverlay('$!{address.front}/mini/login.htm?destUrl=$!{address.front}/submitCallback.htm&regfrom=10031041','注册/登录',750,430,0);
    			}else{
					buy_type=true;
					serviceId = this.id;
					serviceId = serviceId.replace("dj","");
					tancHtml(fkHtml)
    			}
            });
            ## 领优惠券成功
            var yhHtml ='<div class="buy-success"><div class="success-titel2">恭喜您领券成功，请到生意管家查看领券编码。</div><div class="success-lq"><span class="gb-btn j_close">确定</span></div></div>' 
            $("body").on("click",".lg-bt",function(){
				tancHtml(yhHtml)
            });
            ## 支付成功
            var assHtml = '<div class="buy-success"><div class="success-titel">如支付完成，请至<a href="$!{address.myrc}" target="_blank">生意管家</a>查看预付编码。  如支付异常，请致电0571-56633091！</div><div class="success-do"><span class="bm"><a href="$!{address.myrc}" target="_blank">查看支付编码</a></span><span class="gb-btn j_close fr">关闭</span></div></div>'
            $("body").on("click",".fk-bt",function(){
				online(serviceId);
                tancHtml(assHtml)
            })
			
            //领取优惠券
			$(".lqu-a").on("click",function(){
				if(jQuery("#loginFlag").val()==0){
    				Asto.util.Callback.setSubmitCallback(function(success, data){
    					closeoverlay();
    					jQuery("#loginFlag").val(1)
        			});
    				openoverlay('$!{address.front}/mini/login.htm?destUrl=$!{address.front}/submitCallback.htm&regfrom=10031041','注册/登录',750,430,0);
    			}else{
					var serviceId = this.id;
					serviceId = serviceId.replace("yh","");
					console.log(this.title);
    				## 领取优惠券ajax
    				jQuery.ajax({
    					url:"#springUrl('/cn/shuangshiyi/getCoupon.htm')",
						data:{serviceId:serviceId},
						dataType:"json",
						async:false,
						success:function(data){
							if(data.success){
								buy_type = false;
			    				tancHtml(yhHtml);
							}else{
								alert("领取优惠券失败或您已经领取过优惠券。有任何疑问欢迎致电0571-56611111")
							}
						}
    				});
    			}
			})
            //关闭登录框
            $("body").on("click",".j_close",function(){
                $(".tanc").remove()
            });
        });
        // 弹出层
        function tancHtml(oHtml){
            $("body").append('<div class="tanc"></div>')
            $(".tanc").html(oHtml);
            $(".tanc").width($(window).width());
            var bottomHeight =($(window).height()-$(".tanc").height())/2
            $(".tanc").css("margin-bottom",bottomHeight+"px")
		}
		
		function online(codeId){
			parent.overtitle("确认支付")
			var code=jQuery("#mid"+codeId).val();
			jQuery("#mtd"+codeId).trigger('close');
			openoverlay('$!{address.front}/cn/shuangshiyi/egoldPay.htm?codeId='+codeId+'&code='+code,'确认支付',580,350,0);
		}

	
		
