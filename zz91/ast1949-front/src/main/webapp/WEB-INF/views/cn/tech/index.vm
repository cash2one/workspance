<script>
jQuery(function(){
	fader("#J_fader");
});
function fader(selector){
	var el = jQuery(selector),
		pics = el.find("a"),
		length = pics.length,
		num = el.find("span"),
		cur = 0,
		timer, 
		index;
		
	function anim(){
		pics.eq(cur).fadeOut();
		num.eq(cur).removeClass("cur");
		cur = cur === length - 1 ? 0: cur + 1;
		num.eq(cur).addClass("cur");
		pics.eq(cur).fadeIn();
	}
	
	num.on("click", function(){
		index = num.index(this);
		if ( cur !== index) {
			pics.eq(cur).fadeOut();
			num.eq(cur).removeClass("cur");
			cur = index;
			pics.eq(cur).fadeIn();
			num.eq(cur).addClass("cur");
		}
	});
		
	timer = setInterval(anim, 3000);
	
	el.hover(function(){
		clearInterval(timer);
	}, function(){
		timer = setInterval(anim, 3000);
	});
	
}
</script>	