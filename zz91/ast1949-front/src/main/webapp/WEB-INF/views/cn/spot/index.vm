<script type="text/javascript">
jQuery(function(){
	fader("Js_banner");
	jQuery(".join").facebox();
	myScroll("Js_scrolls", true);
	myScroll("Js_add");

	(function(){
		var tab = jQuery("#Js_tab").find("span"),
			table = jQuery(".price_table").find("table"),
			index = 0,
			num = table.length,
			auto;
		tab.click(function(){
			if (tab.index(jQuery(this)) === 1) {
				index = index === num - 1 ? 0 : index + 1;
			} else {
				index = index === 0 ? num - 1 : index - 1;
			}
			show(index);
		});
		auto = setInterval(function(){
			index = index === num - 1 ? 0 : index + 1; 
			show(index);
		}, 3000);
		jQuery(".price_table").hover(function(){
			clearInterval(auto);
		}, function(){
			auto = setInterval(function(){
				index = index === num - 1 ? 0 : index + 1; 
				show(index);
			}, 3000);
		});
		function show(index) {
			table.eq(index).show().
				siblings("table").hide();
		}
	})();
});

function fader(id){
	var target = jQuery("#" + id),
		li = target.find("ul>li"),
		thumb = target.find(".thumb>span"),
		cur = 0,
		index,
		anim;
	li.eq(1).hide();
	li.eq(2).hide();
	thumb.eq(0).addClass("cur");
	thumb.on("click", function(){
		index = thumb.index(jQuery(this));
		if (index !== cur) {
			li.eq(cur).fadeOut();
			li.eq(index).fadeIn();
			thumb.eq(cur).removeClass("cur");
			thumb.eq(index).addClass("cur");
			cur = index;
		}
	});
	function fade() {
		li.eq(cur).fadeOut();
		thumb.eq(cur).removeClass("cur");
		cur = cur === 2 ? 0 : cur + 1;
		li.eq(cur).fadeIn();
		thumb.eq(cur).addClass("cur");	
	}
	target.hover(function(){
		clearInterval(anim);
	}, function(){	
		anim = setInterval(fade, 2000);
	});
	anim = setInterval(fade, 2000);
}

function myScroll(id, copy){
    var doc = document,
    el = doc.getElementById(id),
    ul = el.getElementsByTagName("ul")[0],
    li = ul.getElementsByTagName("li"),
    liHeight = li[0].offsetHeight,
    top;
    el.style.cssText = "overflow:hidden;height:" + liHeight * 5 + "px;";
    if (copy) {
	    el.appendChild(ul.cloneNode(true));
    }
    var move = function(){
    top = el.scrollTop;
    if (top > ul.offsetHeight) {
    el.scrollTop = 0;
    } else {
    el.scrollTop = top + 1;
    }
    }
    var loop = setInterval(move, 50);
    el.onmouseover = function(){
    clearInterval(loop);
    };
    el.onmouseout = function(){
    loop = setInterval(move, 50);
    }
}
</script>