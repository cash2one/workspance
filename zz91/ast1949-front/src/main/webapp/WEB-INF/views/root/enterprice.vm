<SCRIPT language=javascript type=text/javascript> 
	var my_glider = new Glider('my-glider', {duration:0.5});
	var stopscroll=1
	setTimeout('autoprov()',6000);
	function autoprov()
	{
		my_glider.previous();
		if (stopscroll==1)
		{
			setTimeout('autoprov()',6000);
		}
		return false;
	}
	function prov()
	{
		my_glider.previous();
		stopscroll=0;
		setTimeout('getprov()',30000);
		return false;
	}
	function nextp()
	{
		my_glider.next();
		stopscroll=0;
		setTimeout('getprov()',30000);
		return false;
	}
	function getprov()
	{
		stopscroll=1;
		autoprov()
	}
</SCRIPT> 