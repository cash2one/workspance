	<!--交易中心-->
	<rule>
		<from>^/trade/offerindex(\d+).htm$</from>
		<to>/trade/offerindex.htm?mainCode=$1</to>
	</rule>
	<rule>
		<from>^/trade/productdetails(\d+).htm$</from>
		<to>/trade/productdetails.htm?productsId=$1</to>
	</rule>
	<rule>
		<!-- 分类查询 -->
		<from>^/trade/offerlist--mc(\d*)\.htm$</from>
		<to>/trade/offerlist\.htm?mainCode=$1</to>
	</rule>
	<rule>
		<from>^/trade/offerlist--a(\d*)--b(\d*)--c(\d*)--d(\d*)--e(\d*)--f(\d*)--p(\d*)--([0-9,a-z,A-Z,%]*).htm$</from>
		<to>/trade/offerlist\.htm?ptype=$1&amp;province=$2&amp;posttime=$3&amp;priceflag=$4&amp;nopiclist=$5&amp;havepic=$6&amp;page=$7&amp;keywords=$8</to>
	</rule>
	<!--企业报价-->
	<rule>
		<from>^/companyprice/index--pc(\d*)\.htm$</from>
		<to>/companyprice/index.htm?categoryCompanyPriceCode=$1</to>
	</rule>
	<rule>
		<from>^/companyprice/index--([0-9,a-z,A-Z,%]*)--pc(\d*)--area(\d*)--int(\d*)--pr([0-9,\-]*)\.htm$</from>
		<to>/companyprice/index.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;postInDays=$4&amp;priceRange=$5</to>
