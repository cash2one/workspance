		<to>/companyprice/index.htm?keywords=$1&amp;categoryCompanyPriceCode=$2&amp;areaCode=$3&amp;postInDays=$4&amp;priceRange=$5&amp;startIndex=$6&amp;pageSize=$7</to>
	</rule>
	<!--公司黄页-->
