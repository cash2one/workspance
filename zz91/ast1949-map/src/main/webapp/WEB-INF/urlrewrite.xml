<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<!--首页 province-->
	<!--<rule>
		<from>^/(\w+).htm$</from>
		<to>/province.htm?areaCode=$1</to>
	</rule>
	
	
	-->
	<rule>
		<from>^/zhejiang.htm$</from>
		<to>/province.htm?areaCode=zhejiang</to>
	</rule>
	
	<rule>
		<from>^/guangdong.htm$</from>
		<to>/province.htm?areaCode=guangdong</to>
	</rule>
	<rule>
		<from>^/shandong.htm$</from>
		<to>/province.htm?areaCode=shandong</to>
	</rule>
	<rule>
		<from>^/jiangsu.htm$</from>
		<to>/province.htm?areaCode=jiangsu</to>
	</rule>
	<rule>
		<from>^/beijing.htm$</from>
		<to>/province.htm?areaCode=beijing</to>
	</rule>
	<rule>
		<from>^/shanghai.htm$</from>
		<to>/province.htm?areaCode=shanghai</to>
	</rule>
	<rule>
		<from>^/tianjin.htm$</from>
		<to>/province.htm?areaCode=tianjin</to>
	</rule>
	<rule>
		<from>^/hebei.htm$</from>
		<to>/province.htm?areaCode=hebei</to>
	</rule>
	<rule>
		<from>^/henan.htm$</from>
		<to>/province.htm?areaCode=henan</to>
	</rule>
	<rule>
		<from>^/hunan.htm$</from>
		<to>/province.htm?areaCode=hunan</to>
	</rule>
	<rule>
		<from>^/([a-z]*)-feijinshu.htm$</from>
		<to>/province.htm?areaCode=$1&amp;productCode=feijinshu</to>
	</rule>
	
	<rule>
		<from>^/([a-z]*)-feisuliao.htm$</from>
		<to>/province.htm?areaCode=$1&amp;productCode=feisuliao</to>
	</rule>
	
	<rule>
		<from>^/([a-z]*)-zhfeiliao.htm$</from>
		<to>/province.htm?areaCode=$1&amp;productCode=zhfeiliao</to>
	</rule>
	
	<!--关键字搜索页 info-->
	<rule>
		<from>^/info-([a-z]*)-([0-9,a-z,A-Z,%]*).htm$</from>
		<to>/info.htm?areaName=$1&amp;keywords=$2</to>
	</rule>
	
	<!-- 列表页 -->
	<!-- 默认 地区 -->
	<rule>
		<from>^/list-([a-z]*).htm$</from>
		<to>/list.htm?areaName=$1</to>
	</rule>
	<!-- 默认 地区 翻页 -->
	<rule>
		<from>^/list-([a-z]*)-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;startIndex=$2</to>
	</rule>
	<!-- 默认 地区 最新供求 -->
	<rule>
		<from>^/list-([a-z]*)-p.htm$</from>
		<to>/list.htm?areaName=$1&amp;listType=p</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-p-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;listType=p&amp;startIndex=$2</to>
	</rule>
	
	<rule>
		<from>^/list-([a-z]*)-feijinshu.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feijinshu</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feijinshu-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feijinshu&amp;startIndex=$2</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feisuliao.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feisuliao</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feisuliao-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feisuliao&amp;startIndex=$2</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-zhfeiliao.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=zhfeiliao</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-zhfeiliao-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=zhfeiliao&amp;startIndex=$2</to>
	</rule>
	
	<rule>
		<from>^/list-([a-z]*)-feijinshu-p.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feijinshu&amp;listType=p</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feijinshu-p-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feijinshu&amp;listType=p&amp;startIndex=$2</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feisuliao-p.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feisuliao&amp;listType=p</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-feisuliao-p-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=feisuliao&amp;listType=p&amp;startIndex=$2</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-zhfeiliao-p.htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=zhfeiliao&amp;listType=p</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-zhfeiliao-p-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;productType=zhfeiliao&amp;listType=p&amp;startIndex=$2</to>
	</rule>
	
	<rule>
		<from>^/list-([a-z]*)-([0-9,a-z,A-Z,%]*).htm$</from>
		<to>/list.htm?areaName=$1&amp;keywords=$2</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-([0-9,a-z,A-Z,%]*)-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;keywords=$2&amp;startIndex=$3</to>
	</rule>
	
	<rule>
		<from>^/list-([a-z]*)-([0-9,a-z,A-Z,%]*)-p.htm$</from>
		<to>/list.htm?areaName=$1&amp;keywords=$2&amp;listType=p</to>
	</rule>
	<rule>
		<from>^/list-([a-z]*)-([0-9,a-z,A-Z,%]*)-p-s(\d+).htm$</from>
		<to>/list.htm?areaName=$1&amp;keywords=$2&amp;startIndex=$3&amp;listType=p</to>
	</rule>

</urlrewrite>