function tab(id, prev, next, tabs, numId) {
	var tab = jQuery("#" + id),
		prev = jQuery("#" + prev),
		next = jQuery("#" + next),
		item = tab.find("." + tabs),
		number = jQuery("#" +　numId),
		length = item.length - 1,
		pos = 0, temp;
	item.eq(pos).show();
	number.text(pos + 1);
	if (length > 0) { 
	prev.on("click", function(){
		temp = pos;
		pos = pos === 0 ? length : pos - 1;
		item.eq(pos).show();
		item.eq(temp).hide();
		number.text(pos + 1);
	});
  	next.on("click", function(){
    	temp = pos;
    	pos = pos === length ? 0 : pos + 1;
    	item.eq(pos).show();
    	item.eq(temp).hide();
		number.text(pos + 1);
  	});
  	var loop = setInterval(function(){
  		next.click();
  	}, 3000);
  	tab.hover(function(){
  		clearInterval(loop);
  	}, function(){
  		loop = setInterval(function(){
  			next.click();
  		}, 3000);
  	})
	}
}
tab("Js_tabs", "prev", "next", "item", "num");
