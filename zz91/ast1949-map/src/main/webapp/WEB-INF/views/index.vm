<script>
function fader(id){
    var target = document.getElementById(id),
        item = target.children,
        num = item.length,
        cur = 0;
    jQuery(item).each(function(i){
        if (i !== cur) {
            item[i].style.display = "none";
        }
    })
    function anim(){
        jQuery(item[cur]).fadeOut("slow");
        cur = cur === num - 1 ? 0 : cur + 1;
        jQuery(item[cur]).fadeIn("slow");
    }
    setInterval(anim, 3000);
}
jQuery(function(){
    fader("Js_banner");
})   
</script>